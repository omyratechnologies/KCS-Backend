            total_duration_minutes: estimatedTotalTime,
            watched_duration_minutes: totalWatchTime,
            estimated_remaining_minutes: Math.max(0, estimatedTotalTime - totalWatchTime),
            time_to_completion: this.calculateTimeToCompletion(totalWatchTime, estimatedTotalTime)
          },
          sections: sectionsWithProgress,
          next_lecture: this.findNextLecture(sectionsWithProgress),
          bookmarked_lectures: sectionsWithProgress.flatMap(s => {
            /* istanbul ignore next */
            cov_14b4odhq0v().f[63]++;
            cov_14b4odhq0v().s[320]++;
            return s.lectures;
          }).filter(l => {
            /* istanbul ignore next */
            cov_14b4odhq0v().f[64]++;
            cov_14b4odhq0v().s[321]++;
            return l.is_bookmarked;
          }).map(l => {
            /* istanbul ignore next */
            cov_14b4odhq0v().f[65]++;
            var _a;
            /* istanbul ignore next */
            cov_14b4odhq0v().s[322]++;
            return {
              id: l.id,
              title: l.title,
              section_title:
              /* istanbul ignore next */
              (cov_14b4odhq0v().b[130][0]++, (_a = sectionsWithProgress.find(s => {
                /* istanbul ignore next */
                cov_14b4odhq0v().f[66]++;
                cov_14b4odhq0v().s[323]++;
                return s.lectures.includes(l);
              })) === null) ||
              /* istanbul ignore next */
              (cov_14b4odhq0v().b[130][1]++, _a === void 0) ?
              /* istanbul ignore next */
              (cov_14b4odhq0v().b[129][0]++, void 0) :
              /* istanbul ignore next */
              (cov_14b4odhq0v().b[129][1]++, _a.title)
            };
          })
        },
        message: "Course progress details retrieved successfully"
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_14b4odhq0v().s[324]++;
      throw new Error(`Failed to get course progress details: ${error}`);
    }
  }
  /**
   * Helper method to calculate estimated time to completion
   */
  static calculateTimeToCompletion(watchedMinutes, totalMinutes) {
    /* istanbul ignore next */
    cov_14b4odhq0v().f[67]++;
    cov_14b4odhq0v().s[325]++;
    if (totalMinutes === 0) {
      /* istanbul ignore next */
      cov_14b4odhq0v().b[131][0]++;
      cov_14b4odhq0v().s[326]++;
      return "N/A";
    } else
    /* istanbul ignore next */
    {
      cov_14b4odhq0v().b[131][1]++;
    }
    const remainingMinutes =
    /* istanbul ignore next */
    (cov_14b4odhq0v().s[327]++, Math.max(0, totalMinutes - watchedMinutes));
    const hours =
    /* istanbul ignore next */
    (cov_14b4odhq0v().s[328]++, Math.floor(remainingMinutes / 60));
    const minutes =
    /* istanbul ignore next */
    (cov_14b4odhq0v().s[329]++, remainingMinutes % 60);
    /* istanbul ignore next */
    cov_14b4odhq0v().s[330]++;
    if (hours === 0) {
      /* istanbul ignore next */
      cov_14b4odhq0v().b[132][0]++;
      cov_14b4odhq0v().s[331]++;
      return `${minutes}min`;
    } else
    /* istanbul ignore next */
    {
      cov_14b4odhq0v().b[132][1]++;
    }
    cov_14b4odhq0v().s[332]++;
    if (minutes === 0) {
      /* istanbul ignore next */
      cov_14b4odhq0v().b[133][0]++;
      cov_14b4odhq0v().s[333]++;
      return `${hours}hr`;
    } else
    /* istanbul ignore next */
    {
      cov_14b4odhq0v().b[133][1]++;
    }
    cov_14b4odhq0v().s[334]++;
    return `${hours}hr ${minutes}min`;
  }
  /**
   * Helper method to find next lecture to watch
   */
  static findNextLecture(sections) {
    /* istanbul ignore next */
    cov_14b4odhq0v().f[68]++;
    cov_14b4odhq0v().s[335]++;
    for (const section of sections) {
      /* istanbul ignore next */
      cov_14b4odhq0v().s[336]++;
      for (const lecture of section.lectures) {
        /* istanbul ignore next */
        cov_14b4odhq0v().s[337]++;
        if (
        /* istanbul ignore next */
        (cov_14b4odhq0v().b[135][0]++, lecture.status === "not_started") ||
        /* istanbul ignore next */
        (cov_14b4odhq0v().b[135][1]++, lecture.status === "in_progress")) {
          /* istanbul ignore next */
          cov_14b4odhq0v().b[134][0]++;
          cov_14b4odhq0v().s[338]++;
          return {
            lecture_id: lecture.id,
            lecture_title: lecture.title,
            section_title: section.title,
            resume_position_seconds: lecture.resume_position_seconds,
            estimated_duration_minutes: lecture.estimated_duration_minutes
          };
        } else
        /* istanbul ignore next */
        {
          cov_14b4odhq0v().b[134][1]++;
        }
      }
    }
    /* istanbul ignore next */
    cov_14b4odhq0v().s[339]++;
    return null; // All lectures completed
  }
  /**
   * Set learning schedule and reminders (like Udemy's schedule feature)
   */
  static async setLearningSchedule(user_id, course_id, scheduleData) {
    /* istanbul ignore next */
    cov_14b4odhq0v().f[69]++;
    var _a;
    /* istanbul ignore next */
    cov_14b4odhq0v().s[340]++;
    try {
      // Verify enrollment
      const enrollmentResult =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[341]++, await CourseEnrollment.find({
        course_id,
        user_id
      }));
      /* istanbul ignore next */
      cov_14b4odhq0v().s[342]++;
      if (enrollmentResult.rows.length === 0) {
        /* istanbul ignore next */
        cov_14b4odhq0v().b[136][0]++;
        cov_14b4odhq0v().s[343]++;
        throw new Error("User is not enrolled in this course");
      } else
      /* istanbul ignore next */
      {
        cov_14b4odhq0v().b[136][1]++;
      }
      const enrollment =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[344]++, enrollmentResult.rows[0]);
      const course =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[345]++, await this.findCourseById(course_id));
      // Calculate recommended schedule based on course duration
      let recommendedDailyMinutes =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[346]++, scheduleData.daily_study_minutes);
      /* istanbul ignore next */
      cov_14b4odhq0v().s[347]++;
      if (
      /* istanbul ignore next */
      (cov_14b4odhq0v().b[138][0]++, !recommendedDailyMinutes) &&
      /* istanbul ignore next */
      (cov_14b4odhq0v().b[138][1]++, scheduleData.target_completion_date) &&
      /* istanbul ignore next */
      (cov_14b4odhq0v().b[138][2]++, course)) {
        /* istanbul ignore next */
        cov_14b4odhq0v().b[137][0]++;
        const daysUntilTarget =
        /* istanbul ignore next */
        (cov_14b4odhq0v().s[348]++, Math.ceil((new Date(scheduleData.target_completion_date).getTime() - Date.now()) / (1000 * 60 * 60 * 24)));
        const totalMinutesRemaining =
        /* istanbul ignore next */
        (cov_14b4odhq0v().s[349]++, (
        /* istanbul ignore next */
        (cov_14b4odhq0v().b[139][0]++, course.estimated_duration_hours) ||
        /* istanbul ignore next */
        (cov_14b4odhq0v().b[139][1]++, 0)) * 60);
        const studyDaysPerWeek =
        /* istanbul ignore next */
        (cov_14b4odhq0v().s[350]++,
        /* istanbul ignore next */
        (cov_14b4odhq0v().b[140][0]++,
        /* istanbul ignore next */
        (cov_14b4odhq0v().b[142][0]++, (_a = scheduleData.study_days) === null) ||
        /* istanbul ignore next */
        (cov_14b4odhq0v().b[142][1]++, _a === void 0) ?
        /* istanbul ignore next */
        (cov_14b4odhq0v().b[141][0]++, void 0) :
        /* istanbul ignore next */
        (cov_14b4odhq0v().b[141][1]++, _a.length)) ||
        /* istanbul ignore next */
        (cov_14b4odhq0v().b[140][1]++, 5));
        const totalStudyDays =
        /* istanbul ignore next */
        (cov_14b4odhq0v().s[351]++, Math.floor(daysUntilTarget / 7 * studyDaysPerWeek));
        /* istanbul ignore next */
        cov_14b4odhq0v().s[352]++;
        recommendedDailyMinutes = totalStudyDays > 0 ?
        /* istanbul ignore next */
        (cov_14b4odhq0v().b[143][0]++, Math.ceil(totalMinutesRemaining / totalStudyDays)) :
        /* istanbul ignore next */
        (cov_14b4odhq0v().b[143][1]++, 30); // Default 30 minutes
      } else
      /* istanbul ignore next */
      {
        cov_14b4odhq0v().b[137][1]++;
      }
      // Update enrollment with learning schedule
      const updatedEnrollment =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[353]++, await CourseEnrollment.updateById(enrollment.id, {
        learning_schedule: {
          target_completion_date: scheduleData.target_completion_date,
          daily_study_minutes:
          /* istanbul ignore next */
          (cov_14b4odhq0v().b[144][0]++, recommendedDailyMinutes) ||
          /* istanbul ignore next */
          (cov_14b4odhq0v().b[144][1]++, 30),
          study_days:
          /* istanbul ignore next */
          (cov_14b4odhq0v().b[145][0]++, scheduleData.study_days) ||
          /* istanbul ignore next */
          (cov_14b4odhq0v().b[145][1]++, ["monday", "tuesday", "wednesday", "thursday", "friday"]),
          reminder_time:
          /* istanbul ignore next */
          (cov_14b4odhq0v().b[146][0]++, scheduleData.reminder_time) ||
          /* istanbul ignore next */
          (cov_14b4odhq0v().b[146][1]++, "19:00"),
          timezone:
          /* istanbul ignore next */
          (cov_14b4odhq0v().b[147][0]++, scheduleData.timezone) ||
          /* istanbul ignore next */
          (cov_14b4odhq0v().b[147][1]++, "UTC"),
          send_reminders: scheduleData.send_reminders !== false,
          created_at: new Date(),
          last_reminder_sent: null
        },
        updated_at: new Date()
      }));
      /* istanbul ignore next */
      cov_14b4odhq0v().s[354]++;
      return {
        success: true,
        data: {
          learning_schedule: updatedEnrollment.learning_schedule,
          recommendations: {
            daily_study_minutes: recommendedDailyMinutes,
            estimated_completion_date: scheduleData.target_completion_date,
            total_study_sessions: scheduleData.target_completion_date ?
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[148][0]++, Math.ceil((
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[149][0]++,
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[151][0]++, course === null) ||
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[151][1]++, course === void 0) ?
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[150][0]++, void 0) :
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[150][1]++, course.estimated_duration_hours)) ||
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[149][1]++, 0)) * 60 / (
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[152][0]++, recommendedDailyMinutes) ||
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[152][1]++, 30)))) :
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[148][1]++, null)
          }
        },
        message: "Learning schedule set successfully"
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_14b4odhq0v().s[355]++;
      throw new Error(`Failed to set learning schedule: ${error}`);
    }
  }
  /**
   * Get learning statistics and achievements (like Udemy insights)
   */
  static async getLearningStatistics(user_id, campus_id, timeframe =
  /* istanbul ignore next */
  (cov_14b4odhq0v().b[153][0]++, "month")) {
    /* istanbul ignore next */
    cov_14b4odhq0v().f[70]++;
    cov_14b4odhq0v().s[356]++;
    try {
      const endDate =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[357]++, new Date());
      const startDate =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[358]++, new Date());
      /* istanbul ignore next */
      cov_14b4odhq0v().s[359]++;
      switch (timeframe) {
        case "week":
          /* istanbul ignore next */
          cov_14b4odhq0v().b[154][0]++;
          {
            /* istanbul ignore next */
            cov_14b4odhq0v().s[360]++;
            startDate.setDate(endDate.getDate() - 7);
            /* istanbul ignore next */
            cov_14b4odhq0v().s[361]++;
            break;
          }
        case "month":
          /* istanbul ignore next */
          cov_14b4odhq0v().b[154][1]++;
          {
            /* istanbul ignore next */
            cov_14b4odhq0v().s[362]++;
            startDate.setMonth(endDate.getMonth() - 1);
            /* istanbul ignore next */
            cov_14b4odhq0v().s[363]++;
            break;
          }
        case "year":
          /* istanbul ignore next */
          cov_14b4odhq0v().b[154][2]++;
          {
            /* istanbul ignore next */
            cov_14b4odhq0v().s[364]++;
            startDate.setFullYear(endDate.getFullYear() - 1);
            /* istanbul ignore next */
            cov_14b4odhq0v().s[365]++;
            break;
          }
      }
      // Get user's progress records
      const progressResult =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[366]++, await CourseProgress.find({
        user_id,
        campus_id,
        updated_at: {
          $gte: startDate,
          $lte: endDate
        }
      }));
      const progressRecords =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[367]++, progressResult.rows);
      // Get user's enrollments
      const enrollmentsResult =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[368]++, await CourseEnrollment.find({
        user_id,
        campus_id
      }));
      const enrollments =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[369]++, enrollmentsResult.rows);
      // Calculate statistics
      const totalWatchTime =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[370]++, progressRecords.reduce((sum, p) => {
        /* istanbul ignore next */
        cov_14b4odhq0v().f[71]++;
        cov_14b4odhq0v().s[371]++;
        return sum + p.watch_time_seconds;
      }, 0));
      const uniqueLectures =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[372]++, new Set(progressRecords.map(p => {
        /* istanbul ignore next */
        cov_14b4odhq0v().f[72]++;
        cov_14b4odhq0v().s[373]++;
        return p.lecture_id;
      })).size);
      const completedLectures =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[374]++, progressRecords.filter(p => {
        /* istanbul ignore next */
        cov_14b4odhq0v().f[73]++;
        cov_14b4odhq0v().s[375]++;
        return p.progress_status === "completed";
      }).length);
      const coursesInProgress =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[376]++, enrollments.filter(e => {
        /* istanbul ignore next */
        cov_14b4odhq0v().f[74]++;
        cov_14b4odhq0v().s[377]++;
        return /* istanbul ignore next */(cov_14b4odhq0v().b[155][0]++, e.enrollment_status === "active") &&
        /* istanbul ignore next */
        (cov_14b4odhq0v().b[155][1]++, e.progress_percentage > 0) &&
        /* istanbul ignore next */
        (cov_14b4odhq0v().b[155][2]++, e.progress_percentage < 100);
      }).length);
      const coursesCompleted =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[378]++, enrollments.filter(e => {
        /* istanbul ignore next */
        cov_14b4odhq0v().f[75]++;
        cov_14b4odhq0v().s[379]++;
        return e.enrollment_status === "completed";
      }).length);
      // Calculate streaks and consistency
      const dailyActivity =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[380]++, this.calculateDailyActivity(progressRecords, startDate, endDate));
      const currentStreak =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[381]++, this.calculateCurrentStreak(dailyActivity));
      const longestStreak =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[382]++, this.calculateLongestStreak(dailyActivity));
      /* istanbul ignore next */
      cov_14b4odhq0v().s[383]++;
      return {
        success: true,
        data: {
          timeframe,
          period: {
            start_date: startDate,
            end_date: endDate
          },
          statistics: {
            total_watch_time_hours: Math.round(totalWatchTime / 3600 * 10) / 10,
            total_watch_time_minutes: Math.round(totalWatchTime / 60),
            unique_lectures_watched: uniqueLectures,
            lectures_completed: completedLectures,
            courses_in_progress: coursesInProgress,
            courses_completed: coursesCompleted,
            average_daily_minutes: Math.round(totalWatchTime / 60 / Math.max(1, Math.ceil((endDate.getTime() - startDate.getTime()) / (1000 * 60 * 60 * 24))))
          },
          engagement: {
            current_streak_days: currentStreak,
            longest_streak_days: longestStreak,
            active_days: dailyActivity.filter(d => {
              /* istanbul ignore next */
              cov_14b4odhq0v().f[76]++;
              cov_14b4odhq0v().s[384]++;
              return d.minutes > 0;
            }).length,
            consistency_percentage: Math.round(dailyActivity.filter(d => {
              /* istanbul ignore next */
              cov_14b4odhq0v().f[77]++;
              cov_14b4odhq0v().s[385]++;
              return d.minutes > 0;
            }).length / dailyActivity.length * 100)
          },
          daily_activity: dailyActivity,
          achievements: this.calculateAchievements(enrollments, progressRecords, totalWatchTime)
        },
        message: "Learning statistics retrieved successfully"
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_14b4odhq0v().s[386]++;
      throw new Error(`Failed to get learning statistics: ${error}`);
    }
  }
  /**
   * Helper methods for learning statistics
   */
  static calculateDailyActivity(progressRecords, startDate, endDate) {
    /* istanbul ignore next */
    cov_14b4odhq0v().f[78]++;
    const dailyActivity =
    /* istanbul ignore next */
    (cov_14b4odhq0v().s[387]++, []);
    const currentDate =
    /* istanbul ignore next */
    (cov_14b4odhq0v().s[388]++, new Date(startDate));
    /* istanbul ignore next */
    cov_14b4odhq0v().s[389]++;
    while (currentDate <= endDate) {
      const dayStart =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[390]++, new Date(currentDate));
      /* istanbul ignore next */
      cov_14b4odhq0v().s[391]++;
      dayStart.setHours(0, 0, 0, 0);
      const dayEnd =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[392]++, new Date(currentDate));
      /* istanbul ignore next */
      cov_14b4odhq0v().s[393]++;
      dayEnd.setHours(23, 59, 59, 999);
      const dayProgress =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[394]++, progressRecords.filter(p => {
        /* istanbul ignore next */
        cov_14b4odhq0v().f[79]++;
        const updateDate =
        /* istanbul ignore next */
        (cov_14b4odhq0v().s[395]++, new Date(p.updated_at));
        /* istanbul ignore next */
        cov_14b4odhq0v().s[396]++;
        return /* istanbul ignore next */(cov_14b4odhq0v().b[156][0]++, updateDate >= dayStart) &&
        /* istanbul ignore next */
        (cov_14b4odhq0v().b[156][1]++, updateDate <= dayEnd);
      }));
      const watchTime =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[397]++, dayProgress.reduce((sum, p) => {
        /* istanbul ignore next */
        cov_14b4odhq0v().f[80]++;
        cov_14b4odhq0v().s[398]++;
        return sum + p.watch_time_seconds;
      }, 0));
      /* istanbul ignore next */
      cov_14b4odhq0v().s[399]++;
      dailyActivity.push({
        date: new Date(currentDate),
        minutes: Math.round(watchTime / 60),
        lectures_watched: dayProgress.length,
        lectures_completed: dayProgress.filter(p => {
          /* istanbul ignore next */
          cov_14b4odhq0v().f[81]++;
          cov_14b4odhq0v().s[400]++;
          return p.progress_status === "completed";
        }).length
      });
      /* istanbul ignore next */
      cov_14b4odhq0v().s[401]++;
      currentDate.setDate(currentDate.getDate() + 1);
    }
    /* istanbul ignore next */
    cov_14b4odhq0v().s[402]++;
    return dailyActivity;
  }
  static calculateCurrentStreak(dailyActivity) {
    /* istanbul ignore next */
    cov_14b4odhq0v().f[82]++;
    let streak =
    /* istanbul ignore next */
    (cov_14b4odhq0v().s[403]++, 0);
    /* istanbul ignore next */
    cov_14b4odhq0v().s[404]++;
    for (let i =
    /* istanbul ignore next */
    (cov_14b4odhq0v().s[405]++, dailyActivity.length - 1); i >= 0; i--) {
      /* istanbul ignore next */
      cov_14b4odhq0v().s[406]++;
      if (dailyActivity[i].minutes > 0) {
        /* istanbul ignore next */
        cov_14b4odhq0v().b[157][0]++;
        cov_14b4odhq0v().s[407]++;
        streak++;
      } else {
        /* istanbul ignore next */
        cov_14b4odhq0v().b[157][1]++;
        cov_14b4odhq0v().s[408]++;
        break;
      }
    }
    /* istanbul ignore next */
    cov_14b4odhq0v().s[409]++;
    return streak;
  }
  static calculateLongestStreak(dailyActivity) {
    /* istanbul ignore next */
    cov_14b4odhq0v().f[83]++;
    let longestStreak =
    /* istanbul ignore next */
    (cov_14b4odhq0v().s[410]++, 0);
    let currentStreak =
    /* istanbul ignore next */
    (cov_14b4odhq0v().s[411]++, 0);
    /* istanbul ignore next */
    cov_14b4odhq0v().s[412]++;
    for (const day of dailyActivity) {
      /* istanbul ignore next */
      cov_14b4odhq0v().s[413]++;
      if (day.minutes > 0) {
        /* istanbul ignore next */
        cov_14b4odhq0v().b[158][0]++;
        cov_14b4odhq0v().s[414]++;
        currentStreak++;
        /* istanbul ignore next */
        cov_14b4odhq0v().s[415]++;
        longestStreak = Math.max(longestStreak, currentStreak);
      } else {
        /* istanbul ignore next */
        cov_14b4odhq0v().b[158][1]++;
        cov_14b4odhq0v().s[416]++;
        currentStreak = 0;
      }
    }
    /* istanbul ignore next */
    cov_14b4odhq0v().s[417]++;
    return longestStreak;
  }
  static calculateAchievements(enrollments, progressRecords, totalWatchTime) {
    /* istanbul ignore next */
    cov_14b4odhq0v().f[84]++;
