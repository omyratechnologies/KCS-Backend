            /* istanbul ignore next */
            (cov_14b4odhq0v().b[122][1]++, _c === void 0) ?
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[121][0]++, void 0) :
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[121][1]++, _c.length)) ||
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[120][1]++, 0),
            // Status indicators
            status: isCompleted ?
            /* istanbul ignore next */
