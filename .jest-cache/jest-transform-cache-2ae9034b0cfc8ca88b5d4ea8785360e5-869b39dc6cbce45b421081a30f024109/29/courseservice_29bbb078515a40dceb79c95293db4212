        campus_id
      }));
      /* istanbul ignore next */
      cov_14b4odhq0v().s[89]++;
      if (
      /* istanbul ignore next */
      (cov_14b4odhq0v().b[32][0]++, !courseResults.rows) ||
      /* istanbul ignore next */
      (cov_14b4odhq0v().b[32][1]++, courseResults.rows.length === 0)) {
        /* istanbul ignore next */
        cov_14b4odhq0v().b[31][0]++;
        cov_14b4odhq0v().s[90]++;
        throw new Error("Course not found");
      } else
      /* istanbul ignore next */
      {
        cov_14b4odhq0v().b[31][1]++;
      }
      const courseResult =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[91]++, courseResults.rows[0]);
      // Get course sections and lectures
      const sectionsResult =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[92]++, await CourseSection.find({
        course_id,
        campus_id,
        is_published: true
      }));
      const sections =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[93]++, sectionsResult.rows.sort((a, b) => {
        /* istanbul ignore next */
        cov_14b4odhq0v().f[23]++;
        cov_14b4odhq0v().s[94]++;
        return a.section_order - b.section_order;
      }));
      // Get lectures for each section
      const sectionsWithLectures =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[95]++, await Promise.all(sections.map(async section => {
        /* istanbul ignore next */
        cov_14b4odhq0v().f[24]++;
        const lecturesResult =
        /* istanbul ignore next */
        (cov_14b4odhq0v().s[96]++, await CourseLecture.find({
          section_id: section.id,
          is_published: true
        }));
        const lectures =
        /* istanbul ignore next */
        (cov_14b4odhq0v().s[97]++, lecturesResult.rows.sort((a, b) => {
          /* istanbul ignore next */
          cov_14b4odhq0v().f[25]++;
          cov_14b4odhq0v().s[98]++;
          return a.lecture_order - b.lecture_order;
        }));
        // If user is provided, get their progress for each lecture
        let lecturesWithProgress =
        /* istanbul ignore next */
        (cov_14b4odhq0v().s[99]++, lectures);
        /* istanbul ignore next */
        cov_14b4odhq0v().s[100]++;
        if (user_id) {
          /* istanbul ignore next */
          cov_14b4odhq0v().b[33][0]++;
          cov_14b4odhq0v().s[101]++;
          lecturesWithProgress = await Promise.all(lectures.map(async lecture => {
            /* istanbul ignore next */
            cov_14b4odhq0v().f[26]++;
            const progressResult =
            /* istanbul ignore next */
            (cov_14b4odhq0v().s[102]++, await CourseProgress.find({
              user_id,
              lecture_id: lecture.id
            }));
            const progress =
            /* istanbul ignore next */
            (cov_14b4odhq0v().s[103]++, progressResult.rows[0]);
            /* istanbul ignore next */
            cov_14b4odhq0v().s[104]++;
            return Object.assign(Object.assign({}, lecture), {
              user_progress: progress ?
              /* istanbul ignore next */
              (cov_14b4odhq0v().b[34][0]++, {
                progress_status: progress.progress_status,
                completion_percentage: progress.completion_percentage,
                last_accessed_at: progress.last_accessed_at,
                resume_position_seconds: progress.resume_position_seconds
              }) :
              /* istanbul ignore next */
              (cov_14b4odhq0v().b[34][1]++, null)
            });
          }));
        } else
        /* istanbul ignore next */
        {
          cov_14b4odhq0v().b[33][1]++;
        }
        cov_14b4odhq0v().s[105]++;
        return Object.assign(Object.assign({}, section), {
          lectures: lecturesWithProgress,
          lecture_count: lectures.length,
          total_duration_minutes: lectures.reduce((sum, lecture) => {
            /* istanbul ignore next */
            cov_14b4odhq0v().f[27]++;
            cov_14b4odhq0v().s[106]++;
            return sum + lecture.estimated_duration_minutes;
          }, 0)
        });
      })));
      // Get enrollment info if user is provided
      let enrollmentInfo =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[107]++, null);
      /* istanbul ignore next */
      cov_14b4odhq0v().s[108]++;
      if (user_id) {
        /* istanbul ignore next */
        cov_14b4odhq0v().b[35][0]++;
        const enrollmentResult =
        /* istanbul ignore next */
        (cov_14b4odhq0v().s[109]++, await CourseEnrollment.find({
          user_id,
          course_id
        }));
        /* istanbul ignore next */
        cov_14b4odhq0v().s[110]++;
        enrollmentInfo =
        /* istanbul ignore next */
        (cov_14b4odhq0v().b[36][0]++, enrollmentResult.rows[0]) ||
        /* istanbul ignore next */
        (cov_14b4odhq0v().b[36][1]++, null);
      } else
      /* istanbul ignore next */
      {
        cov_14b4odhq0v().b[35][1]++;
      }
      // Get instructor details
      const instructorDetails =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[111]++, await Promise.all(courseResult.instructor_ids.map(async instructor_id => {
        /* istanbul ignore next */
        cov_14b4odhq0v().f[28]++;
        var _a;
        /* istanbul ignore next */
        cov_14b4odhq0v().s[112]++;
        try {
          const userResults =
          /* istanbul ignore next */
          (cov_14b4odhq0v().s[113]++, await User.find({
            id: instructor_id
          }));
          const userResult =
          /* istanbul ignore next */
          (cov_14b4odhq0v().s[114]++,
          /* istanbul ignore next */
          (cov_14b4odhq0v().b[38][0]++, userResults.rows) &&
          /* istanbul ignore next */
          (cov_14b4odhq0v().b[38][1]++, userResults.rows.length > 0) ?
          /* istanbul ignore next */
          (cov_14b4odhq0v().b[37][0]++, userResults.rows[0]) :
          /* istanbul ignore next */
          (cov_14b4odhq0v().b[37][1]++, null));
          /* istanbul ignore next */
          cov_14b4odhq0v().s[115]++;
          return userResult ?
          /* istanbul ignore next */
          (cov_14b4odhq0v().b[39][0]++, {
            id: userResult.id,
            name: `${userResult.first_name} ${userResult.last_name}`,
            email: userResult.email,
            profile_image:
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[41][0]++, (_a = userResult.meta_data) === null) ||
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[41][1]++, _a === void 0) ?
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[40][0]++, void 0) :
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[40][1]++, _a.profile_image)
          }) :
          /* istanbul ignore next */
          (cov_14b4odhq0v().b[39][1]++, null);
        } catch (error) {
          /* istanbul ignore next */
          cov_14b4odhq0v().s[116]++;
          console.error(`Error finding instructor ${instructor_id}:`, error);
          /* istanbul ignore next */
          cov_14b4odhq0v().s[117]++;
          return null;
        }
      })));
      /* istanbul ignore next */
      cov_14b4odhq0v().s[118]++;
      return {
        success: true,
        data: Object.assign(Object.assign({}, courseResult), {
          sections: sectionsWithLectures,
          total_sections: sections.length,
          total_lectures: sectionsWithLectures.reduce((sum, section) => {
            /* istanbul ignore next */
            cov_14b4odhq0v().f[29]++;
            cov_14b4odhq0v().s[119]++;
            return sum + section.lectures.length;
          }, 0),
          total_duration_minutes: sectionsWithLectures.reduce((sum, section) => {
            /* istanbul ignore next */
            cov_14b4odhq0v().f[30]++;
            cov_14b4odhq0v().s[120]++;
            return sum + section.total_duration_minutes;
          }, 0),
          instructors: instructorDetails.filter(Boolean),
          enrollment_info: enrollmentInfo
        }),
        message: "Course retrieved successfully"
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_14b4odhq0v().s[121]++;
      throw new Error(`Failed to get course: ${error}`);
    }
  }
  /**
   * Update course
   */
  static async updateCourse(course_id, campus_id, user_id, updateData) {
    /* istanbul ignore next */
    cov_14b4odhq0v().f[31]++;
    cov_14b4odhq0v().s[122]++;
    try {
      const existingCourse =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[123]++, await this.findCourseById(course_id, campus_id));
      /* istanbul ignore next */
      cov_14b4odhq0v().s[124]++;
      if (!existingCourse) {
        /* istanbul ignore next */
        cov_14b4odhq0v().b[42][0]++;
        cov_14b4odhq0v().s[125]++;
        throw new Error("Course not found");
      } else
      /* istanbul ignore next */
      {
        cov_14b4odhq0v().b[42][1]++;
      }
      const updatedCourse =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[126]++, await this.updateCourseById(course_id, Object.assign(Object.assign({}, updateData), {
        last_updated_by: user_id,
        version: existingCourse.version + 1,
        updated_at: new Date()
      }), campus_id));
      /* istanbul ignore next */
      cov_14b4odhq0v().s[127]++;
      return {
        success: true,
        data: updatedCourse,
        message: "Course updated successfully"
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_14b4odhq0v().s[128]++;
      throw new Error(`Failed to update course: ${error}`);
    }
  }
  /**
   * Publish course
   */
  static async publishCourse(course_id, campus_id, user_id) {
    /* istanbul ignore next */
    cov_14b4odhq0v().f[32]++;
    cov_14b4odhq0v().s[129]++;
    try {
      const course =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[130]++, await this.findCourseById(course_id, campus_id));
      /* istanbul ignore next */
      cov_14b4odhq0v().s[131]++;
      if (!course) {
        /* istanbul ignore next */
        cov_14b4odhq0v().b[43][0]++;
        cov_14b4odhq0v().s[132]++;
        throw new Error("Course not found");
      } else
      /* istanbul ignore next */
      {
        cov_14b4odhq0v().b[43][1]++;
      }
      // Validate course has required content before publishing
      const sectionsResult =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[133]++, await CourseSection.find({
        course_id,
        is_published: true
      }));
      /* istanbul ignore next */
      cov_14b4odhq0v().s[134]++;
      if (sectionsResult.rows.length === 0) {
        /* istanbul ignore next */
        cov_14b4odhq0v().b[44][0]++;
        cov_14b4odhq0v().s[135]++;
        throw new Error("Course must have at least one published section to be published");
      } else
      /* istanbul ignore next */
      {
        cov_14b4odhq0v().b[44][1]++;
      }
      const lecturesResult =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[136]++, await CourseLecture.find({
        course_id,
        is_published: true
      }));
      /* istanbul ignore next */
      cov_14b4odhq0v().s[137]++;
      if (lecturesResult.rows.length === 0) {
        /* istanbul ignore next */
        cov_14b4odhq0v().b[45][0]++;
        cov_14b4odhq0v().s[138]++;
        throw new Error("Course must have at least one published lecture to be published");
      } else
      /* istanbul ignore next */
      {
        cov_14b4odhq0v().b[45][1]++;
      }
      const updatedCourse =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[139]++, await this.updateCourseById(course_id, {
        status: "published",
        last_updated_by: user_id,
        updated_at: new Date()
      }, campus_id));
      /* istanbul ignore next */
      cov_14b4odhq0v().s[140]++;
      return {
        success: true,
        data: updatedCourse,
        message: "Course published successfully"
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_14b4odhq0v().s[141]++;
      throw new Error(`Failed to publish course: ${error}`);
    }
  }
  /**
   * Delete course (soft delete by archiving)
   */
  static async deleteCourse(course_id, campus_id, user_id) {
    /* istanbul ignore next */
    cov_14b4odhq0v().f[33]++;
    cov_14b4odhq0v().s[142]++;
    try {
      const course =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[143]++, await this.findCourseById(course_id, campus_id));
      /* istanbul ignore next */
      cov_14b4odhq0v().s[144]++;
      if (!course) {
        /* istanbul ignore next */
        cov_14b4odhq0v().b[46][0]++;
        cov_14b4odhq0v().s[145]++;
        throw new Error("Course not found");
      } else
      /* istanbul ignore next */
      {
        cov_14b4odhq0v().b[46][1]++;
      }
      // Check if course has active enrollments
      const enrollmentsResult =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[146]++, await CourseEnrollment.find({
        course_id,
        enrollment_status: "active"
      }));
      /* istanbul ignore next */
      cov_14b4odhq0v().s[147]++;
      if (enrollmentsResult.rows.length > 0) {
        /* istanbul ignore next */
        cov_14b4odhq0v().b[47][0]++;
        cov_14b4odhq0v().s[148]++;
        throw new Error("Cannot delete course with active enrollments. Archive it instead.");
      } else
      /* istanbul ignore next */
      {
        cov_14b4odhq0v().b[47][1]++;
      }
      cov_14b4odhq0v().s[149]++;
      await this.updateCourseById(course_id, {
        status: "archived",
        last_updated_by: user_id,
        updated_at: new Date()
      }, campus_id);
      /* istanbul ignore next */
      cov_14b4odhq0v().s[150]++;
      return {
        success: true,
        message: "Course archived successfully"
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_14b4odhq0v().s[151]++;
      throw new Error(`Failed to delete course: ${error}`);
    }
  }
  // ==================== SECTION MANAGEMENT ====================
  /**
   * Create course section
   */
  static async createCourseSection(course_id, campus_id, sectionData) {
    /* istanbul ignore next */
    cov_14b4odhq0v().f[34]++;
    cov_14b4odhq0v().s[152]++;
    try {
      // Verify course exists
      const course =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[153]++, await this.findCourseById(course_id, campus_id));
      /* istanbul ignore next */
      cov_14b4odhq0v().s[154]++;
      if (!course) {
        /* istanbul ignore next */
        cov_14b4odhq0v().b[48][0]++;
        cov_14b4odhq0v().s[155]++;
        throw new Error("Course not found");
      } else
      /* istanbul ignore next */
      {
        cov_14b4odhq0v().b[48][1]++;
      }
      const section =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[156]++, await CourseSection.create(Object.assign(Object.assign({
        id: nanoid(),
        course_id,
        campus_id
      }, sectionData), {
        created_at: new Date(),
        updated_at: new Date()
      })));
      /* istanbul ignore next */
      cov_14b4odhq0v().s[157]++;
      return {
        success: true,
