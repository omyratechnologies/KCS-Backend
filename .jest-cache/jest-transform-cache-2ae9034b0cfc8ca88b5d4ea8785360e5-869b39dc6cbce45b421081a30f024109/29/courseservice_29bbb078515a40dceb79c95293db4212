      });
    } else
    /* istanbul ignore next */
    {
      cov_14b4odhq0v().b[163][1]++;
    }
    // Lecture completion achievements
    const completedLectures =
    /* istanbul ignore next */
    (cov_14b4odhq0v().s[432]++, progressRecords.filter(p => {
      /* istanbul ignore next */
      cov_14b4odhq0v().f[86]++;
      cov_14b4odhq0v().s[433]++;
      return p.progress_status === "completed";
    }).length);
    /* istanbul ignore next */
