            /* istanbul ignore next */
            (cov_14b4odhq0v().b[112][1]++, 0),
            is_bookmarked:
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[115][0]++,
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[117][0]++, (_b =
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[119][0]++, progress === null) ||
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[119][1]++, progress === void 0) ?
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[118][0]++, void 0) :
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[118][1]++, progress.interaction_data)) === null) ||
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[117][1]++, _b === void 0) ?
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[116][0]++, void 0) :
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[116][1]++, _b.bookmarked)) ||
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[115][1]++, false),
            notes_count:
            /* istanbul ignore next */
