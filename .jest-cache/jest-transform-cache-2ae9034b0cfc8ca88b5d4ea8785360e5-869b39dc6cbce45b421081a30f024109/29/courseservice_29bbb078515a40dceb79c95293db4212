    if (completedLectures >= 10) {
      /* istanbul ignore next */
      cov_14b4odhq0v().b[164][0]++;
      cov_14b4odhq0v().s[435]++;
      achievements.push({
        type: "progress",
        title: "Lecture Lover",
        description: "Completed 10 lectures"
      });
    } else
    /* istanbul ignore next */
    {
      cov_14b4odhq0v().b[164][1]++;
