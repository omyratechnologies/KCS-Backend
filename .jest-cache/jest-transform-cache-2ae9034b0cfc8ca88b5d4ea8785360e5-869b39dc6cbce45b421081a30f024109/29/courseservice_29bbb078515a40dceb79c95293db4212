  // ==================== ENROLLMENT MANAGEMENT ====================
  /**
   * Enroll user in course
   */
  static async enrollInCourse(course_id, user_id, campus_id, enrollmentData =
  /* istanbul ignore next */
  (cov_14b4odhq0v().b[51][0]++, {})) {
    /* istanbul ignore next */
    cov_14b4odhq0v().f[40]++;
    cov_14b4odhq0v().s[178]++;
    try {
      // Check if course exists and is published
      const course =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[179]++, await this.findCourseById(course_id, campus_id));
      /* istanbul ignore next */
      cov_14b4odhq0v().s[180]++;
      if (!course) {
        /* istanbul ignore next */
        cov_14b4odhq0v().b[52][0]++;
        cov_14b4odhq0v().s[181]++;
        throw new Error("Course not found");
      } else
      /* istanbul ignore next */
      {
        cov_14b4odhq0v().b[52][1]++;
      }
      cov_14b4odhq0v().s[182]++;
      if (course.status !== "published") {
        /* istanbul ignore next */
        cov_14b4odhq0v().b[53][0]++;
        cov_14b4odhq0v().s[183]++;
        throw new Error("Can only enroll in published courses");
      } else
      /* istanbul ignore next */
      {
        cov_14b4odhq0v().b[53][1]++;
      }
      // Check if user is already enrolled
      const existingEnrollment =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[184]++, await CourseEnrollment.find({
        course_id,
        user_id
      }));
      /* istanbul ignore next */
      cov_14b4odhq0v().s[185]++;
      if (existingEnrollment.rows.length > 0) {
        /* istanbul ignore next */
        cov_14b4odhq0v().b[54][0]++;
        cov_14b4odhq0v().s[186]++;
        throw new Error("User is already enrolled in this course");
      } else
      /* istanbul ignore next */
      {
        cov_14b4odhq0v().b[54][1]++;
      }
      // Check enrollment limits
      cov_14b4odhq0v().s[187]++;
      if (course.max_enrollments) {
        /* istanbul ignore next */
        cov_14b4odhq0v().b[55][0]++;
        const currentEnrollments =
        /* istanbul ignore next */
        (cov_14b4odhq0v().s[188]++, await CourseEnrollment.find({
          course_id,
          enrollment_status: {
            $in: ["active", "completed"]
          }
        }));
        /* istanbul ignore next */
        cov_14b4odhq0v().s[189]++;
        if (currentEnrollments.rows.length >= course.max_enrollments) {
          /* istanbul ignore next */
          cov_14b4odhq0v().b[56][0]++;
          cov_14b4odhq0v().s[190]++;
          throw new Error("Course enrollment limit reached");
        } else
        /* istanbul ignore next */
        {
          cov_14b4odhq0v().b[56][1]++;
        }
      } else
      /* istanbul ignore next */
      {
        cov_14b4odhq0v().b[55][1]++;
      }
      // Check enrollment dates
      const now =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[191]++, new Date());
      /* istanbul ignore next */
      cov_14b4odhq0v().s[192]++;
      if (
      /* istanbul ignore next */
      (cov_14b4odhq0v().b[58][0]++, course.enrollment_start_date) &&
      /* istanbul ignore next */
      (cov_14b4odhq0v().b[58][1]++, now < new Date(course.enrollment_start_date))) {
        /* istanbul ignore next */
        cov_14b4odhq0v().b[57][0]++;
        cov_14b4odhq0v().s[193]++;
        throw new Error("Enrollment has not started yet");
      } else
      /* istanbul ignore next */
      {
        cov_14b4odhq0v().b[57][1]++;
      }
      cov_14b4odhq0v().s[194]++;
      if (
      /* istanbul ignore next */
      (cov_14b4odhq0v().b[60][0]++, course.enrollment_end_date) &&
      /* istanbul ignore next */
      (cov_14b4odhq0v().b[60][1]++, now > new Date(course.enrollment_end_date))) {
        /* istanbul ignore next */
        cov_14b4odhq0v().b[59][0]++;
        cov_14b4odhq0v().s[195]++;
        throw new Error("Enrollment period has ended");
      } else
      /* istanbul ignore next */
      {
        cov_14b4odhq0v().b[59][1]++;
      }
      // Get total lectures count
      const lecturesResult =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[196]++, await CourseLecture.find({
        course_id,
        is_published: true
      }));
      const totalLectures =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[197]++, lecturesResult.rows.length);
      // Create enrollment
      const enrollment =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[198]++, await CourseEnrollment.create({
        id: nanoid(),
        course_id,
        user_id,
        campus_id,
        enrollment_type:
        /* istanbul ignore next */
        (cov_14b4odhq0v().b[61][0]++, enrollmentData.enrollment_type) ||
        /* istanbul ignore next */
        (cov_14b4odhq0v().b[61][1]++, "free"),
        enrollment_status: "active",
        progress_percentage: 0,
        enrollment_date: now,
        payment_status: course.price > 0 ?
        /* istanbul ignore next */
        (cov_14b4odhq0v().b[62][0]++, "pending") :
        /* istanbul ignore next */
        (cov_14b4odhq0v().b[62][1]++, "completed"),
        certificate_issued: false,
        access_details: {
          total_lectures: totalLectures,
          completed_lectures: 0,
          completed_lecture_ids: [],
          bookmarked_lectures: [],
          notes_count: 0,
          quiz_attempts: 0,
          assignment_submissions: 0
        },
        enrollment_source:
        /* istanbul ignore next */
        (cov_14b4odhq0v().b[63][0]++, enrollmentData.enrollment_source) ||
        /* istanbul ignore next */
        (cov_14b4odhq0v().b[63][1]++, "web"),
        meta_data:
        /* istanbul ignore next */
        (cov_14b4odhq0v().b[64][0]++, enrollmentData.meta_data) ||
        /* istanbul ignore next */
        (cov_14b4odhq0v().b[64][1]++, {}),
        created_at: now,
        updated_at: now
      }));
      // Update course enrollment count
      /* istanbul ignore next */
      cov_14b4odhq0v().s[199]++;
      await this.updateCourseById(course_id, {
        enrollment_count: course.enrollment_count + 1,
        updated_at: now
      });
      /* istanbul ignore next */
      cov_14b4odhq0v().s[200]++;
      return {
        success: true,
        data: enrollment,
        message: "Successfully enrolled in course"
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_14b4odhq0v().s[201]++;
      throw new Error(`Failed to enroll in course: ${error}`);
    }
  }
  /**
   * Get user's enrolled courses
   */
  static async getUserEnrolledCourses(user_id, campus_id, filters =
  /* istanbul ignore next */
  (cov_14b4odhq0v().b[65][0]++, {})) {
    /* istanbul ignore next */
    cov_14b4odhq0v().f[41]++;
    cov_14b4odhq0v().s[202]++;
    try {
      const {
        status,
        progress,
        page =
        /* istanbul ignore next */
        (cov_14b4odhq0v().b[66][0]++, 1),
        limit =
        /* istanbul ignore next */
        (cov_14b4odhq0v().b[67][0]++, 20)
      } =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[203]++, filters);
      // Build query conditions
      const queryConditions =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[204]++, {
        user_id,
        campus_id
      });
      /* istanbul ignore next */
      cov_14b4odhq0v().s[205]++;
      if (status) {
        /* istanbul ignore next */
        cov_14b4odhq0v().b[68][0]++;
        cov_14b4odhq0v().s[206]++;
        queryConditions.enrollment_status = status;
      } else
      /* istanbul ignore next */
      {
        cov_14b4odhq0v().b[68][1]++;
      }
      const enrollmentsResult =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[207]++, await CourseEnrollment.find(queryConditions));
      let enrollments =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[208]++, enrollmentsResult.rows);
      // Filter by progress if specified
      /* istanbul ignore next */
      cov_14b4odhq0v().s[209]++;
      if (progress) {
        /* istanbul ignore next */
        cov_14b4odhq0v().b[69][0]++;
        cov_14b4odhq0v().s[210]++;
        switch (progress) {
          case "not_started":
            /* istanbul ignore next */
            cov_14b4odhq0v().b[70][0]++;
            {
              /* istanbul ignore next */
              cov_14b4odhq0v().s[211]++;
              enrollments = enrollments.filter(e => {
                /* istanbul ignore next */
                cov_14b4odhq0v().f[42]++;
                cov_14b4odhq0v().s[212]++;
                return e.progress_percentage === 0;
              });
              /* istanbul ignore next */
              cov_14b4odhq0v().s[213]++;
              break;
            }
          case "in_progress":
            /* istanbul ignore next */
            cov_14b4odhq0v().b[70][1]++;
            {
              /* istanbul ignore next */
              cov_14b4odhq0v().s[214]++;
              enrollments = enrollments.filter(e => {
                /* istanbul ignore next */
                cov_14b4odhq0v().f[43]++;
                cov_14b4odhq0v().s[215]++;
                return /* istanbul ignore next */(cov_14b4odhq0v().b[71][0]++, e.progress_percentage > 0) &&
                /* istanbul ignore next */
                (cov_14b4odhq0v().b[71][1]++, e.progress_percentage < 100);
              });
              /* istanbul ignore next */
              cov_14b4odhq0v().s[216]++;
              break;
            }
          case "completed":
            /* istanbul ignore next */
            cov_14b4odhq0v().b[70][2]++;
            {
              /* istanbul ignore next */
              cov_14b4odhq0v().s[217]++;
              enrollments = enrollments.filter(e => {
                /* istanbul ignore next */
                cov_14b4odhq0v().f[44]++;
                cov_14b4odhq0v().s[218]++;
                return e.progress_percentage === 100;
              });
              /* istanbul ignore next */
              cov_14b4odhq0v().s[219]++;
              break;
            }
          // No default
        }
      } else
      /* istanbul ignore next */
      {
        cov_14b4odhq0v().b[69][1]++;
      }
      // Pagination
      const skip =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[220]++, (page - 1) * limit);
      const paginatedEnrollments =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[221]++, enrollments.slice(skip, skip + limit));
      // Get course details for each enrollment
      const enrollmentsWithCourses =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[222]++, await Promise.all(paginatedEnrollments.map(async enrollment => {
        /* istanbul ignore next */
        cov_14b4odhq0v().f[45]++;
        var _a;
        const course =
        /* istanbul ignore next */
        (cov_14b4odhq0v().s[223]++, await this.findCourseById(enrollment.course_id));
        // Get total lectures and completed count for progress calculation
        const lecturesResult =
        /* istanbul ignore next */
        (cov_14b4odhq0v().s[224]++, await CourseLecture.find({
          course_id: enrollment.course_id,
          is_published: true
        }));
        const totalLectures =
        /* istanbul ignore next */
        (cov_14b4odhq0v().s[225]++, lecturesResult.rows.length);
        // Calculate time remaining based on watch progress
        const progressResult =
        /* istanbul ignore next */
        (cov_14b4odhq0v().s[226]++, await CourseProgress.find({
          course_id: enrollment.course_id,
          user_id: enrollment.user_id
        }));
        const totalWatchTime =
        /* istanbul ignore next */
        (cov_14b4odhq0v().s[227]++, progressResult.rows.reduce((sum, p) => {
          /* istanbul ignore next */
          cov_14b4odhq0v().f[46]++;
          cov_14b4odhq0v().s[228]++;
          return sum + p.watch_time_seconds;
        }, 0));
        const estimatedRemainingHours =
        /* istanbul ignore next */
        (cov_14b4odhq0v().s[229]++,
        /* istanbul ignore next */
        (cov_14b4odhq0v().b[73][0]++, course) &&
        /* istanbul ignore next */
        (cov_14b4odhq0v().b[73][1]++, course.estimated_duration_hours) ?
        /* istanbul ignore next */
        (cov_14b4odhq0v().b[72][0]++, Math.max(0, course.estimated_duration_hours - totalWatchTime / 3600)) :
        /* istanbul ignore next */
        (cov_14b4odhq0v().b[72][1]++, 0));
        /* istanbul ignore next */
        cov_14b4odhq0v().s[230]++;
        return Object.assign(Object.assign({}, enrollment), {
          course_details: course ?
          /* istanbul ignore next */
          (cov_14b4odhq0v().b[74][0]++, {
            title: course.title,
            thumbnail: course.thumbnail,
            category: course.category,
            difficulty_level: course.difficulty_level,
            estimated_duration_hours: course.estimated_duration_hours,
            rating: course.rating,
            total_lectures: totalLectures,
            completed_lectures:
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[75][0]++,
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[77][0]++, (_a = enrollment.access_details) === null) ||
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[77][1]++, _a === void 0) ?
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[76][0]++, void 0) :
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[76][1]++, _a.completed_lectures)) ||
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[75][1]++, 0),
            time_remaining_hours: estimatedRemainingHours,
            last_updated: course.updated_at
          }) :
          /* istanbul ignore next */
          (cov_14b4odhq0v().b[74][1]++, null)
        });
      })));
      /* istanbul ignore next */
      cov_14b4odhq0v().s[231]++;
      return {
        success: true,
        data: {
          enrollments: enrollmentsWithCourses,
          pagination: {
            current_page: page,
            per_page: limit,
            total_items: enrollments.length,
            total_pages: Math.ceil(enrollments.length / limit),
            has_next: page < Math.ceil(enrollments.length / limit),
            has_previous: page > 1
          }
        },
        message: "Enrolled courses retrieved successfully"
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_14b4odhq0v().s[232]++;
      throw new Error(`Failed to get enrolled courses: ${error}`);
    }
  }
  /**
   * Update course progress
   */
  static async updateCourseProgress(course_id, lecture_id, user_id, campus_id, progressData) {
    /* istanbul ignore next */
    cov_14b4odhq0v().f[47]++;
    cov_14b4odhq0v().s[233]++;
    try {
      // Check if user is enrolled
      const enrollmentResult =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[234]++, await CourseEnrollment.find({
        course_id,
        user_id,
        enrollment_status: "active"
      }));
      /* istanbul ignore next */
      cov_14b4odhq0v().s[235]++;
      if (enrollmentResult.rows.length === 0) {
        /* istanbul ignore next */
        cov_14b4odhq0v().b[78][0]++;
        cov_14b4odhq0v().s[236]++;
        throw new Error("User is not enrolled in this course");
      } else
      /* istanbul ignore next */
      {
        cov_14b4odhq0v().b[78][1]++;
      }
      const enrollment =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[237]++, enrollmentResult.rows[0]);
      // Get or create progress record
      const existingProgressResult =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[238]++, await CourseProgress.find({
        user_id,
        lecture_id
      }));
      let progress;
      const now =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[239]++, new Date());
      /* istanbul ignore next */
      cov_14b4odhq0v().s[240]++;
      if (existingProgressResult.rows.length === 0) {
        /* istanbul ignore next */
        cov_14b4odhq0v().b[79][0]++;
        cov_14b4odhq0v().s[241]++;
        // Create new progress record
        progress = await CourseProgress.create(Object.assign({
          id: nanoid(),
          course_id,
          user_id,
          lecture_id,
          campus_id,
          progress_status: "not_started",
          watch_time_seconds: 0,
          total_duration_seconds: 0,
          completion_percentage: 0,
          first_accessed_at: now,
          last_accessed_at: now,
          interaction_data: {
            play_count: 0,
            pause_count: 0,
            seek_count: 0,
            speed_changes: 0,
            quality_changes: 0,
            fullscreen_toggles: 0,
            notes_taken: 0,
            bookmarked: false,
            liked: false
          },
          notes: [],
          device_info: {
            device_type: "web"
          },
          meta_data: {},
          created_at: now,
          updated_at: now
        }, progressData));
      } else {
        /* istanbul ignore next */
        cov_14b4odhq0v().b[79][1]++;
        // Update existing progress
        const existingProgress =
        /* istanbul ignore next */
        (cov_14b4odhq0v().s[242]++, existingProgressResult.rows[0]);
        /* istanbul ignore next */
        cov_14b4odhq0v().s[243]++;
        progress = await CourseProgress.updateById(existingProgress.id, Object.assign(Object.assign({}, progressData), {
          last_accessed_at: now,
          updated_at: now
        }));
      }
      // Update enrollment progress if lecture is completed
      /* istanbul ignore next */
      cov_14b4odhq0v().s[244]++;
      if (progressData.progress_status === "completed") {
        /* istanbul ignore next */
        cov_14b4odhq0v().b[80][0]++;
        cov_14b4odhq0v().s[245]++;
        await this.updateEnrollmentProgress(course_id, user_id);
      } else
      /* istanbul ignore next */
      {
        cov_14b4odhq0v().b[80][1]++;
      }
      cov_14b4odhq0v().s[246]++;
      return {
        success: true,
        data: progress,
        message: "Progress updated successfully"
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_14b4odhq0v().s[247]++;
      throw new Error(`Failed to update progress: ${error}`);
    }
  }
  /**
   * Update overall enrollment progress
   */
  static async updateEnrollmentProgress(course_id, user_id) {
    /* istanbul ignore next */
    cov_14b4odhq0v().f[48]++;
    cov_14b4odhq0v().s[248]++;
    try {
      // Get all lectures in the course
      const lecturesResult =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[249]++, await CourseLecture.find({
        course_id,
        is_published: true,
        is_mandatory: true
      }));
      const totalMandatoryLectures =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[250]++, lecturesResult.rows.length);
      // Get completed lectures
      const completedProgressResult =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[251]++, await CourseProgress.find({
        course_id,
        user_id,
        progress_status: "completed"
      }));
      const completedLectures =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[252]++, completedProgressResult.rows.length);
      // Calculate progress percentage
      const progressPercentage =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[253]++, totalMandatoryLectures > 0 ?
      /* istanbul ignore next */
      (cov_14b4odhq0v().b[81][0]++, Math.round(completedLectures / totalMandatoryLectures * 100)) :
      /* istanbul ignore next */
      (cov_14b4odhq0v().b[81][1]++, 0));
      // Determine enrollment status
      let enrollmentStatus =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[254]++, "active");
      let completionDate =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[255]++, null);
      /* istanbul ignore next */
      cov_14b4odhq0v().s[256]++;
      if (progressPercentage >= 100) {
        /* istanbul ignore next */
        cov_14b4odhq0v().b[82][0]++;
        cov_14b4odhq0v().s[257]++;
        enrollmentStatus = "completed";
        /* istanbul ignore next */
        cov_14b4odhq0v().s[258]++;
        completionDate = new Date();
      } else
      /* istanbul ignore next */
      {
        cov_14b4odhq0v().b[82][1]++;
      }
      // Update enrollment
      const enrollment =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[259]++, await CourseEnrollment.find({
        course_id,
        user_id
      }));
      /* istanbul ignore next */
      cov_14b4odhq0v().s[260]++;
      if (enrollment.rows.length > 0) {
        /* istanbul ignore next */
        cov_14b4odhq0v().b[83][0]++;
        cov_14b4odhq0v().s[261]++;
        await CourseEnrollment.updateById(enrollment.rows[0].id, {
          progress_percentage: progressPercentage,
          enrollment_status: enrollmentStatus,
          completion_date: completionDate,
          access_details: Object.assign(Object.assign({}, enrollment.rows[0].access_details), {
            completed_lectures: completedLectures,
            completed_lecture_ids: completedProgressResult.rows.map(p => {
              /* istanbul ignore next */
              cov_14b4odhq0v().f[49]++;
              cov_14b4odhq0v().s[262]++;
              return p.lecture_id;
            })
          }),
          updated_at: new Date()
        });
        // If course is completed, generate certificate if enabled
        /* istanbul ignore next */
        cov_14b4odhq0v().s[263]++;
        if (progressPercentage >= 100) {
          /* istanbul ignore next */
          cov_14b4odhq0v().b[84][0]++;
          const course =
          /* istanbul ignore next */
          (cov_14b4odhq0v().s[264]++, await this.findCourseById(course_id));
          /* istanbul ignore next */
          cov_14b4odhq0v().s[265]++;
          if (
          /* istanbul ignore next */
          (cov_14b4odhq0v().b[86][0]++, course) &&
          /* istanbul ignore next */
          (cov_14b4odhq0v().b[86][1]++, course.is_certificate_enabled)) {
            /* istanbul ignore next */
            cov_14b4odhq0v().b[85][0]++;
            cov_14b4odhq0v().s[266]++;
            await this.generateCourseCertificate(course_id, user_id, enrollment.rows[0].id);
          } else
          /* istanbul ignore next */
          {
            cov_14b4odhq0v().b[85][1]++;
          }
          // Update course completion count
          cov_14b4odhq0v().s[267]++;
          if (course) {
            /* istanbul ignore next */
            cov_14b4odhq0v().b[87][0]++;
            cov_14b4odhq0v().s[268]++;
            await this.updateCourseById(course_id, {
              completion_count: course.completion_count + 1,
              updated_at: new Date()
            });
          } else
          /* istanbul ignore next */
          {
            cov_14b4odhq0v().b[87][1]++;
          }
        } else
        /* istanbul ignore next */
        {
          cov_14b4odhq0v().b[84][1]++;
        }
      } else
      /* istanbul ignore next */
      {
        cov_14b4odhq0v().b[83][1]++;
      }
    } catch (error) {
      /* istanbul ignore next */
      cov_14b4odhq0v().s[269]++;
      console.error("Failed to update enrollment progress:", error);
    }
  }
  /**
   * Generate course certificate
   */
  static async generateCourseCertificate(course_id, user_id, enrollment_id) {
    /* istanbul ignore next */
    cov_14b4odhq0v().f[50]++;
    cov_14b4odhq0v().s[270]++;
    try {
      const course =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[271]++, await this.findCourseById(course_id));
      // Note: User and CourseEnrollment may also need similar fixes if they have ID format issues
      const userResult =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[272]++, await User.find({
        id: user_id
      }));
      const user =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[273]++,
      /* istanbul ignore next */
      (cov_14b4odhq0v().b[89][0]++, userResult.rows) &&
      /* istanbul ignore next */
      (cov_14b4odhq0v().b[89][1]++, userResult.rows.length > 0) ?
      /* istanbul ignore next */
      (cov_14b4odhq0v().b[88][0]++, userResult.rows[0]) :
      /* istanbul ignore next */
      (cov_14b4odhq0v().b[88][1]++, null));
      const enrollmentResult =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[274]++, await CourseEnrollment.find({
        id: enrollment_id
      }));
      const enrollment =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[275]++,
      /* istanbul ignore next */
      (cov_14b4odhq0v().b[91][0]++, enrollmentResult.rows) &&
      /* istanbul ignore next */
      (cov_14b4odhq0v().b[91][1]++, enrollmentResult.rows.length > 0) ?
      /* istanbul ignore next */
      (cov_14b4odhq0v().b[90][0]++, enrollmentResult.rows[0]) :
      /* istanbul ignore next */
      (cov_14b4odhq0v().b[90][1]++, null));
      /* istanbul ignore next */
      cov_14b4odhq0v().s[276]++;
      if (
      /* istanbul ignore next */
      (cov_14b4odhq0v().b[93][0]++, !course) ||
      /* istanbul ignore next */
      (cov_14b4odhq0v().b[93][1]++, !user) ||
      /* istanbul ignore next */
      (cov_14b4odhq0v().b[93][2]++, !enrollment)) {
        /* istanbul ignore next */
        cov_14b4odhq0v().b[92][0]++;
        cov_14b4odhq0v().s[277]++;
        throw new Error("Missing required data for certificate generation");
      } else
      /* istanbul ignore next */
      {
        cov_14b4odhq0v().b[92][1]++;
      }
      const certificateNumber =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[278]++, `CERT-${course.campus_id}-${course_id.slice(0, 8)}-${user_id.slice(0, 8)}-${Date.now()}`);
      const verificationCode =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[279]++, nanoid().slice(0, 16).toUpperCase());
      const certificate =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[280]++, await CourseCertificate.create({
        id: nanoid(),
        course_id,
        user_id,
        enrollment_id,
        campus_id: course.campus_id,
        certificate_number: certificateNumber,
        certificate_type: "completion",
        status: "generated",
        issue_date: new Date(),
        grade: enrollment.grade,
        completion_time_hours:
        /* istanbul ignore next */
        (cov_14b4odhq0v().b[94][0]++, enrollment.completion_time_hours) ||
        /* istanbul ignore next */
        (cov_14b4odhq0v().b[94][1]++, 0),
        skills_acquired: course.learning_objectives,
        certificate_data: {
          template_id:
          /* istanbul ignore next */
          (cov_14b4odhq0v().b[95][0]++, course.certificate_template_id) ||
          /* istanbul ignore next */
          (cov_14b4odhq0v().b[95][1]++, "default"),
          template_version: "1.0",
          verification_url: `${process.env.FRONTEND_URL}/verify-certificate/${verificationCode}`
        },
        verification_details: {
          verification_code: verificationCode,
          is_verifiable: true,
          verification_link: `${process.env.FRONTEND_URL}/verify-certificate/${verificationCode}`,
          issuer_details: {
            institution_name: "KCS Learning Platform",
            instructor_name: `${user.first_name} ${user.last_name}`,
            instructor_credentials: ""
          }
        },
        recipient_details: {
          full_name: `${user.first_name} ${user.last_name}`,
          email: user.email,
          student_id: user.id
        },
        course_details: {
          course_title: course.title,
          course_duration_hours:
          /* istanbul ignore next */
          (cov_14b4odhq0v().b[96][0]++, course.estimated_duration_hours) ||
          /* istanbul ignore next */
          (cov_14b4odhq0v().b[96][1]++, 0),
          completion_percentage: 100,
          skills_covered: course.learning_objectives,
          learning_outcomes_achieved: course.learning_objectives
        },
        delivery_status: {
          email_sent: false,
          download_count: 0,
          shared_count: 0
        },
        meta_data: {},
        created_at: new Date(),
        updated_at: new Date()
      }));
      // Update enrollment with certificate info
      /* istanbul ignore next */
      cov_14b4odhq0v().s[281]++;
      await CourseEnrollment.updateById(enrollment_id, {
        certificate_issued: true,
        certificate_id: certificate.id,
        certificate_issued_at: new Date(),
        updated_at: new Date()
      });
      /* istanbul ignore next */
      cov_14b4odhq0v().s[282]++;
      return certificate;
    } catch (error) {
      /* istanbul ignore next */
      cov_14b4odhq0v().s[283]++;
      console.error("Failed to generate certificate:", error);
      /* istanbul ignore next */
      cov_14b4odhq0v().s[284]++;
      throw error;
    }
  }
  /**
   * Get detailed course progress with lecture completion status (like Udemy)
   */
  static async getCourseProgressDetails(course_id, user_id, campus_id) {
    /* istanbul ignore next */
    cov_14b4odhq0v().f[51]++;
    cov_14b4odhq0v().s[285]++;
    try {
      // Verify enrollment
      const enrollmentResult =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[286]++, await CourseEnrollment.find({
        course_id,
        user_id,
        campus_id
      }));
      /* istanbul ignore next */
      cov_14b4odhq0v().s[287]++;
      if (enrollmentResult.rows.length === 0) {
        /* istanbul ignore next */
        cov_14b4odhq0v().b[97][0]++;
        cov_14b4odhq0v().s[288]++;
        throw new Error("User is not enrolled in this course");
      } else
      /* istanbul ignore next */
      {
        cov_14b4odhq0v().b[97][1]++;
      }
      const enrollment =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[289]++, enrollmentResult.rows[0]);
      const course =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[290]++, await this.findCourseById(course_id, campus_id));
      /* istanbul ignore next */
      cov_14b4odhq0v().s[291]++;
      if (!course) {
        /* istanbul ignore next */
        cov_14b4odhq0v().b[98][0]++;
        cov_14b4odhq0v().s[292]++;
        throw new Error("Course not found");
      } else
      /* istanbul ignore next */
      {
        cov_14b4odhq0v().b[98][1]++;
      }
      // Get sections with detailed progress
      const sectionsResult =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[293]++, await CourseSection.find({
        course_id,
        campus_id,
        is_published: true
      }));
      const sections =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[294]++, sectionsResult.rows.sort((a, b) => {
        /* istanbul ignore next */
        cov_14b4odhq0v().f[52]++;
        cov_14b4odhq0v().s[295]++;
        return a.section_order - b.section_order;
      }));
      const sectionsWithProgress =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[296]++, await Promise.all(sections.map(async section => {
        /* istanbul ignore next */
        cov_14b4odhq0v().f[53]++;
        const lecturesResult =
        /* istanbul ignore next */
        (cov_14b4odhq0v().s[297]++, await CourseLecture.find({
          section_id: section.id,
          is_published: true
        }));
        const lectures =
        /* istanbul ignore next */
        (cov_14b4odhq0v().s[298]++, lecturesResult.rows.sort((a, b) => {
          /* istanbul ignore next */
          cov_14b4odhq0v().f[54]++;
          cov_14b4odhq0v().s[299]++;
          return a.lecture_order - b.lecture_order;
        }));
        const lecturesWithProgress =
        /* istanbul ignore next */
        (cov_14b4odhq0v().s[300]++, await Promise.all(lectures.map(async lecture => {
          /* istanbul ignore next */
          cov_14b4odhq0v().f[55]++;
          var _a, _b, _c;
          const progressResult =
          /* istanbul ignore next */
          (cov_14b4odhq0v().s[301]++, await CourseProgress.find({
            user_id,
            lecture_id: lecture.id
          }));
          const progress =
          /* istanbul ignore next */
          (cov_14b4odhq0v().s[302]++, progressResult.rows[0]);
          const isCompleted =
          /* istanbul ignore next */
          (cov_14b4odhq0v().s[303]++, (
          /* istanbul ignore next */
          (cov_14b4odhq0v().b[100][0]++, progress === null) ||
          /* istanbul ignore next */
          (cov_14b4odhq0v().b[100][1]++, progress === void 0) ?
          /* istanbul ignore next */
          (cov_14b4odhq0v().b[99][0]++, void 0) :
          /* istanbul ignore next */
          (cov_14b4odhq0v().b[99][1]++, progress.progress_status)) === "completed");
          const watchPercentage =
          /* istanbul ignore next */
          (cov_14b4odhq0v().s[304]++,
          /* istanbul ignore next */
          (cov_14b4odhq0v().b[101][0]++,
          /* istanbul ignore next */
          (cov_14b4odhq0v().b[103][0]++, progress === null) ||
          /* istanbul ignore next */
          (cov_14b4odhq0v().b[103][1]++, progress === void 0) ?
          /* istanbul ignore next */
          (cov_14b4odhq0v().b[102][0]++, void 0) :
          /* istanbul ignore next */
          (cov_14b4odhq0v().b[102][1]++, progress.completion_percentage)) ||
          /* istanbul ignore next */
          (cov_14b4odhq0v().b[101][1]++, 0));
          /* istanbul ignore next */
          cov_14b4odhq0v().s[305]++;
          return {
            id: lecture.id,
            title: lecture.title,
            lecture_type: lecture.lecture_type,
            estimated_duration_minutes: lecture.estimated_duration_minutes,
            is_mandatory: lecture.is_mandatory,
            is_preview: lecture.is_preview,
            lecture_order: lecture.lecture_order,
            resources_count:
            /* istanbul ignore next */
