            /* istanbul ignore next */
            (cov_14b4odhq0v().b[106][1]++, _a === void 0) ?
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[105][0]++, void 0) :
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[105][1]++, _a.length)) ||
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[104][1]++, 0),
            // Progress details (like Udemy)
            is_completed: isCompleted,
            watch_percentage: watchPercentage,
            watch_time_seconds:
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[107][0]++,
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[109][0]++, progress === null) ||
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[109][1]++, progress === void 0) ?
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[108][0]++, void 0) :
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[108][1]++, progress.watch_time_seconds)) ||
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[107][1]++, 0),
            last_accessed_at:
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[111][0]++, progress === null) ||
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[111][1]++, progress === void 0) ?
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[110][0]++, void 0) :
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[110][1]++, progress.last_accessed_at),
            resume_position_seconds:
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[112][0]++,
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[114][0]++, progress === null) ||
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[114][1]++, progress === void 0) ?
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[113][0]++, void 0) :
            /* istanbul ignore next */
