      cov_14b4odhq0v().s[421]++;
      achievements.push({
        type: "time",
        title: "First Hour",
        description: "Completed 1 hour of learning"
      });
    } else
    /* istanbul ignore next */
    {
      cov_14b4odhq0v().b[159][1]++;
