            /* istanbul ignore next */
            (cov_14b4odhq0v().b[122][0]++, (_c =
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[124][0]++, progress === null) ||
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[124][1]++, progress === void 0) ?
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[123][0]++, void 0) :
            /* istanbul ignore next */
