    /* istanbul ignore next */
    (cov_14b4odhq0v().s[419]++, totalWatchTime / 3600);
    /* istanbul ignore next */
    cov_14b4odhq0v().s[420]++;
    if (totalHours >= 1) {
      /* istanbul ignore next */
