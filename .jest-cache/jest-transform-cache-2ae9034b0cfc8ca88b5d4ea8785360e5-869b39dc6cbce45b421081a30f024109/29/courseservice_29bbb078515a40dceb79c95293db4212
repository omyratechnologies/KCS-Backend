      const totalWatchTime =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[315]++, sectionsWithProgress.reduce((sum, s) => {
        /* istanbul ignore next */
        cov_14b4odhq0v().f[61]++;
        cov_14b4odhq0v().s[316]++;
        return sum + s.progress.watched_duration_minutes;
      }, 0));
      const estimatedTotalTime =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[317]++, sectionsWithProgress.reduce((sum, s) => {
        /* istanbul ignore next */
        cov_14b4odhq0v().f[62]++;
        cov_14b4odhq0v().s[318]++;
        return sum + s.progress.total_duration_minutes;
      }, 0));
      /* istanbul ignore next */
      cov_14b4odhq0v().s[319]++;
      return {
        success: true,
        data: {
          course_info: {
            id: course.id,
            title: course.title,
            description: course.description,
            thumbnail: course.thumbnail,
            category: course.category,
