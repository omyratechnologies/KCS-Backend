      };
    } catch (error) {
      /* istanbul ignore next */
      cov_14b4odhq0v().s[158]++;
      throw new Error(`Failed to create course section: ${error}`);
    }
  }
  /**
   * Update section order
   */
  static async updateSectionOrder(course_id, campus_id, sectionOrders) {
    /* istanbul ignore next */
    cov_14b4odhq0v().f[35]++;
    cov_14b4odhq0v().s[159]++;
    try {
      const updatePromises =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[160]++, sectionOrders.map(({
        id,
        section_order
      }) => {
        /* istanbul ignore next */
        cov_14b4odhq0v().f[36]++;
        cov_14b4odhq0v().s[161]++;
        return CourseSection.updateById(id, {
          section_order,
          updated_at: new Date()
        });
      }));
      /* istanbul ignore next */
      cov_14b4odhq0v().s[162]++;
      await Promise.all(updatePromises);
      /* istanbul ignore next */
      cov_14b4odhq0v().s[163]++;
      return {
        success: true,
        message: "Section order updated successfully"
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_14b4odhq0v().s[164]++;
      throw new Error(`Failed to update section order: ${error}`);
    }
  }
  // ==================== LECTURE MANAGEMENT ====================
  /**
   * Create course lecture
   */
  static async createCourseLecture(section_id, campus_id, lectureData) {
    /* istanbul ignore next */
    cov_14b4odhq0v().f[37]++;
    cov_14b4odhq0v().s[165]++;
    try {
      // Verify section exists
      const section =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[166]++, await CourseSection.findById(section_id));
      /* istanbul ignore next */
      cov_14b4odhq0v().s[167]++;
      if (
      /* istanbul ignore next */
      (cov_14b4odhq0v().b[50][0]++, !section) ||
      /* istanbul ignore next */
      (cov_14b4odhq0v().b[50][1]++, section.campus_id !== campus_id)) {
        /* istanbul ignore next */
        cov_14b4odhq0v().b[49][0]++;
        cov_14b4odhq0v().s[168]++;
        throw new Error("Course section not found");
      } else
      /* istanbul ignore next */
      {
        cov_14b4odhq0v().b[49][1]++;
      }
      const lecture =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[169]++, await CourseLecture.create(Object.assign(Object.assign({
        id: nanoid(),
        course_id: section.course_id,
        section_id,
        campus_id
      }, lectureData), {
        created_at: new Date(),
        updated_at: new Date()
      })));
      /* istanbul ignore next */
      cov_14b4odhq0v().s[170]++;
      return {
        success: true,
        data: lecture,
        message: "Course lecture created successfully"
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_14b4odhq0v().s[171]++;
      throw new Error(`Failed to create course lecture: ${error}`);
    }
  }
  /**
