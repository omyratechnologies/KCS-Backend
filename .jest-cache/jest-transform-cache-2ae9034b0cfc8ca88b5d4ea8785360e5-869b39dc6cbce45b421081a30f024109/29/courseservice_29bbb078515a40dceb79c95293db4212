    cov_14b4odhq0v().s[436]++;
    if (completedLectures >= 50) {
      /* istanbul ignore next */
      cov_14b4odhq0v().b[165][0]++;
      cov_14b4odhq0v().s[437]++;
      achievements.push({
        type: "progress",
        title: "Content Consumer",
        description: "Completed 50 lectures"
      });
    } else
    /* istanbul ignore next */
    {
      cov_14b4odhq0v().b[165][1]++;
    }
    cov_14b4odhq0v().s[438]++;
    return achievements;
  }
  // ==================== ANALYTICS ====================
  /**
   * Get course analytics
   */
  static async getCourseAnalytics(course_id, campus_id) {
    /* istanbul ignore next */
    cov_14b4odhq0v().f[87]++;
    cov_14b4odhq0v().s[439]++;
    try {
      const course =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[440]++, await this.findCourseById(course_id, campus_id));
      /* istanbul ignore next */
      cov_14b4odhq0v().s[441]++;
      if (!course) {
        /* istanbul ignore next */
        cov_14b4odhq0v().b[166][0]++;
        cov_14b4odhq0v().s[442]++;
        throw new Error("Course not found");
      } else
      /* istanbul ignore next */
      {
        cov_14b4odhq0v().b[166][1]++;
      }
      // Get enrollment analytics
      const enrollmentsResult =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[443]++, await CourseEnrollment.find({
        course_id
      }));
      const enrollments =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[444]++, enrollmentsResult.rows);
      const activeEnrollments =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[445]++, enrollments.filter(e => {
        /* istanbul ignore next */
        cov_14b4odhq0v().f[88]++;
        cov_14b4odhq0v().s[446]++;
        return e.enrollment_status === "active";
      }).length);
      const completedEnrollments =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[447]++, enrollments.filter(e => {
        /* istanbul ignore next */
        cov_14b4odhq0v().f[89]++;
        cov_14b4odhq0v().s[448]++;
        return e.enrollment_status === "completed";
      }).length);
      const completionRate =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[449]++, enrollments.length > 0 ?
      /* istanbul ignore next */
      (cov_14b4odhq0v().b[167][0]++, completedEnrollments / enrollments.length * 100) :
      /* istanbul ignore next */
      (cov_14b4odhq0v().b[167][1]++, 0));
      // Get progress analytics
      const progressResult =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[450]++, await CourseProgress.find({
        course_id
      }));
      const progressRecords =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[451]++, progressResult.rows);
      const totalWatchTime =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[452]++, progressRecords.reduce((sum, p) => {
        /* istanbul ignore next */
        cov_14b4odhq0v().f[90]++;
        cov_14b4odhq0v().s[453]++;
        return sum + p.watch_time_seconds;
      }, 0));
      const averageSessionDuration =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[454]++, progressRecords.length > 0 ?
      /* istanbul ignore next */
      (cov_14b4odhq0v().b[168][0]++, progressRecords.reduce((sum, p) => {
        /* istanbul ignore next */
        cov_14b4odhq0v().f[91]++;
        cov_14b4odhq0v().s[455]++;
        return sum + p.watch_time_seconds;
      }, 0) / progressRecords.length / 60) :
      /* istanbul ignore next */
      (cov_14b4odhq0v().b[168][1]++, 0));
      // Get lecture performance
      const lecturesResult =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[456]++, await CourseLecture.find({
        course_id
      }));
      const lectures =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[457]++, lecturesResult.rows);
      const lecturePerformance =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[458]++, await Promise.all(lectures.map(async lecture => {
        /* istanbul ignore next */
        cov_14b4odhq0v().f[92]++;
        const lectureProgressResult =
        /* istanbul ignore next */
        (cov_14b4odhq0v().s[459]++, await CourseProgress.find({
          lecture_id: lecture.id
        }));
        const lectureProgress =
        /* istanbul ignore next */
        (cov_14b4odhq0v().s[460]++, lectureProgressResult.rows);
        const viewCount =
        /* istanbul ignore next */
        (cov_14b4odhq0v().s[461]++, lectureProgress.length);
        const completionRate =
        /* istanbul ignore next */
        (cov_14b4odhq0v().s[462]++, viewCount > 0 ?
        /* istanbul ignore next */
        (cov_14b4odhq0v().b[169][0]++, lectureProgress.filter(p => {
          /* istanbul ignore next */
          cov_14b4odhq0v().f[93]++;
          cov_14b4odhq0v().s[463]++;
          return p.progress_status === "completed";
        }).length / viewCount * 100) :
        /* istanbul ignore next */
        (cov_14b4odhq0v().b[169][1]++, 0));
        const averageWatchTime =
        /* istanbul ignore next */
        (cov_14b4odhq0v().s[464]++, viewCount > 0 ?
        /* istanbul ignore next */
        (cov_14b4odhq0v().b[170][0]++, lectureProgress.reduce((sum, p) => {
          /* istanbul ignore next */
          cov_14b4odhq0v().f[94]++;
          cov_14b4odhq0v().s[465]++;
          return sum + p.completion_percentage;
        }, 0) / viewCount) :
        /* istanbul ignore next */
        (cov_14b4odhq0v().b[170][1]++, 0));
        /* istanbul ignore next */
        cov_14b4odhq0v().s[466]++;
        return {
          lecture_id: lecture.id,
          lecture_title: lecture.title,
          lecture_type: lecture.lecture_type,
          view_count: viewCount,
          completion_rate: completionRate,
          average_watch_time_percentage: averageWatchTime,
          dropout_rate: 100 - completionRate,
          engagement_score: (completionRate + averageWatchTime) / 2
        };
      })));
      /* istanbul ignore next */
      cov_14b4odhq0v().s[467]++;
      return {
        success: true,
        data: {
          course_overview: {
            total_enrollments: enrollments.length,
            active_enrollments: activeEnrollments,
            completed_enrollments: completedEnrollments,
            completion_rate: completionRate,
            average_completion_time_hours: enrollments.length > 0 ?
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[171][0]++, enrollments.reduce((sum, e) => {
              /* istanbul ignore next */
              cov_14b4odhq0v().f[95]++;
              cov_14b4odhq0v().s[468]++;
              return sum + (
              /* istanbul ignore next */
              (cov_14b4odhq0v().b[172][0]++, e.completion_time_hours) ||
              /* istanbul ignore next */
              (cov_14b4odhq0v().b[172][1]++, 0));
            }, 0) / enrollments.length) :
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[171][1]++, 0),
            average_rating: course.rating,
            total_revenue: enrollments.reduce((sum, e) => {
              /* istanbul ignore next */
              cov_14b4odhq0v().f[96]++;
              cov_14b4odhq0v().s[469]++;
              return sum + (e.enrollment_type === "paid" ?
              /* istanbul ignore next */
              (cov_14b4odhq0v().b[173][0]++, course.price) :
              /* istanbul ignore next */
              (cov_14b4odhq0v().b[173][1]++, 0));
            }, 0)
          },
          engagement_metrics: {
            total_watch_time_hours: totalWatchTime / 3600,
            average_session_duration_minutes: averageSessionDuration,
            video_completion_rate:
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[174][0]++, lecturePerformance.filter(l => {
              /* istanbul ignore next */
              cov_14b4odhq0v().f[97]++;
              cov_14b4odhq0v().s[470]++;
              return l.lecture_type === "video";
            }).reduce((sum, l) => {
              /* istanbul ignore next */
              cov_14b4odhq0v().f[98]++;
              cov_14b4odhq0v().s[471]++;
              return sum + l.completion_rate;
            }, 0) / lecturePerformance.filter(l => {
              /* istanbul ignore next */
              cov_14b4odhq0v().f[99]++;
              cov_14b4odhq0v().s[472]++;
              return l.lecture_type === "video";
            }).length) ||
            /* istanbul ignore next */
            (cov_14b4odhq0v().b[174][1]++, 0),
            quiz_attempt_rate: 0,
            // TODO: Calculate from quiz data
            assignment_submission_rate: 0,
            // TODO: Calculate from assignment data
            discussion_participation_rate: 0 // TODO: Calculate from discussion data
          },
          content_performance: lecturePerformance,
          student_progress: enrollments.slice(0, 50).map(enrollment => {
            /* istanbul ignore next */
            cov_14b4odhq0v().f[100]++;
            cov_14b4odhq0v().s[473]++;
            return {
              user_id: enrollment.user_id,
              student_name: "Student",
              // TODO: Get from user data
              enrollment_date: enrollment.enrollment_date,
              progress_percentage: enrollment.progress_percentage,
              last_accessed: enrollment.last_accessed_at,
              completion_status: enrollment.enrollment_status,
              grade: enrollment.grade
            };
          }),
          time_series_data: {
            daily_enrollments: [],
            // TODO: Implement time series data
            weekly_engagement: []
          }
        },
        message: "Course analytics retrieved successfully"
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_14b4odhq0v().s[474]++;
      throw new Error(`Failed to get course analytics: ${error}`);
    }
  }
}