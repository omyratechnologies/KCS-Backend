  /**
   * Get course statistics
   */
  static async getCourseStatistics(campus_id) {
    /* istanbul ignore next */
    cov_14b4odhq0v().f[16]++;
    cov_14b4odhq0v().s[77]++;
    try {
      const allCourses =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[78]++, await Course.find({
        campus_id
      }));
      const courses =
      /* istanbul ignore next */
      (cov_14b4odhq0v().s[79]++, allCourses.rows);
      /* istanbul ignore next */
      cov_14b4odhq0v().s[80]++;
      return {
        total_courses: courses.length,
        published_courses: courses.filter(c => {
          /* istanbul ignore next */
          cov_14b4odhq0v().f[17]++;
          cov_14b4odhq0v().s[81]++;
          return c.status === "published";
        }).length,
        draft_courses: courses.filter(c => {
          /* istanbul ignore next */
          cov_14b4odhq0v().f[18]++;
          cov_14b4odhq0v().s[82]++;
          return c.status === "draft";
        }).length,
        featured_courses: courses.filter(c => {
          /* istanbul ignore next */
          cov_14b4odhq0v().f[19]++;
          cov_14b4odhq0v().s[83]++;
          return c.is_featured;
        }).length,
        free_courses: courses.filter(c => {
          /* istanbul ignore next */
          cov_14b4odhq0v().f[20]++;
          cov_14b4odhq0v().s[84]++;
          return c.price === 0;
        }).length,
        paid_courses: courses.filter(c => {
          /* istanbul ignore next */
          cov_14b4odhq0v().f[21]++;
          cov_14b4odhq0v().s[85]++;
          return c.price > 0;
        }).length
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_14b4odhq0v().s[86]++;
      throw new Error(`Failed to get course statistics: ${error}`);
    }
  }
  /**
   * Get course by ID with detailed information
   */
  static async getCourseById(course_id, campus_id, user_id) {
