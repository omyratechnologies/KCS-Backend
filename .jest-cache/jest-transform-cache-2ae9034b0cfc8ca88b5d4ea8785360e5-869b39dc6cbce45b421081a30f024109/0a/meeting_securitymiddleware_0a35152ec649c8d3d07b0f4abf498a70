    /* istanbul ignore next */
    (cov_2lv5vl8084().s[14]++, ctx.get("user_type"));
    const method =
    /* istanbul ignore next */
    (cov_2lv5vl8084().s[15]++, ctx.req.method);
    const path =
    /* istanbul ignore next */
    (cov_2lv5vl8084().s[16]++, ctx.req.path);
