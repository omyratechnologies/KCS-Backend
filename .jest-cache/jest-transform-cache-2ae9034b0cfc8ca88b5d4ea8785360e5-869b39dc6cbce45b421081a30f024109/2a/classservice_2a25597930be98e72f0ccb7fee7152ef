  // Get all students by academic year filtered by class_id
  async getStudentsByYearAndClassId(campus_id, academic_year, class_id) {
    /* istanbul ignore next */
    cov_1v8pj5tq7h().f[55]++;
    cov_1v8pj5tq7h().s[384]++;
    try {
      const classesQuery =
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[385]++, {
        campus_id,
        academic_year,
        is_active: true,
        is_deleted: false
      });
      // If class_id is provided, filter by specific class
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[386]++;
      if (class_id) {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().b[86][0]++;
        cov_1v8pj5tq7h().s[387]++;
        classesQuery.id = class_id;
      } else
      /* istanbul ignore next */
      {
        cov_1v8pj5tq7h().b[86][1]++;
      }
      // Get classes for the specified academic year (and optionally specific class)
      const classes =
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[388]++, await Class.find(classesQuery, {
        sort: {
          name: "ASC"
        }
      }));
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[389]++;
      if (classes.rows.length === 0) {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().b[87][0]++;
        cov_1v8pj5tq7h().s[390]++;
        throw new Error(class_id ?
        /* istanbul ignore next */
        (cov_1v8pj5tq7h().b[88][0]++, `No class found with ID: ${class_id} for academic year: ${academic_year}`) :
        /* istanbul ignore next */
        (cov_1v8pj5tq7h().b[88][1]++, `No classes found for academic year: ${academic_year}`));
      } else
      /* istanbul ignore next */
      {
        cov_1v8pj5tq7h().b[87][1]++;
      }
      // Collect all unique student IDs from all classes
      const studentIds =
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[391]++, new Set());
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[392]++;
      for (const classData of classes.rows) {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().s[393]++;
        if (
        /* istanbul ignore next */
        (cov_1v8pj5tq7h().b[90][0]++, classData.student_ids) &&
        /* istanbul ignore next */
        (cov_1v8pj5tq7h().b[90][1]++, classData.student_ids.length > 0)) {
          /* istanbul ignore next */
          cov_1v8pj5tq7h().b[89][0]++;
          cov_1v8pj5tq7h().s[394]++;
          for (const studentId of classData.student_ids) {
            /* istanbul ignore next */
            cov_1v8pj5tq7h().s[395]++;
            studentIds.add(studentId);
          }
        } else
        /* istanbul ignore next */
        {
          cov_1v8pj5tq7h().b[89][1]++;
        }
      }
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[396]++;
      if (studentIds.size === 0) {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().b[91][0]++;
        cov_1v8pj5tq7h().s[397]++;
        return {
          students: [],
          academic_year,
          total_students: 0,
          classes_included: classes.rows
        };
      } else
      /* istanbul ignore next */
      {
        cov_1v8pj5tq7h().b[91][1]++;
      }
      // Get detailed student information
      const studentsData =
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[398]++, await Promise.all([...studentIds].map(async studentId => {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().f[56]++;
        cov_1v8pj5tq7h().s[399]++;
        try {
          /* istanbul ignore next */
          cov_1v8pj5tq7h().s[400]++;
          return await UserService.getUser(studentId);
        } catch (error) {
          /* istanbul ignore next */
          cov_1v8pj5tq7h().s[401]++;
          console.warn(`Failed to get student data for ID: ${studentId}`, error);
          /* istanbul ignore next */
          cov_1v8pj5tq7h().s[402]++;
          return null;
        }
      })));
      // Filter out null values (failed to fetch students)
      const validStudents =
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[403]++, studentsData.filter(student => {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().f[57]++;
        cov_1v8pj5tq7h().s[404]++;
        return student !== null;
      }));
      // Sort students by name
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[405]++;
      validStudents.sort((a, b) => {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().f[58]++;
        const nameA =
        /* istanbul ignore next */
        (cov_1v8pj5tq7h().s[406]++, `${a.first_name} ${a.last_name}`.toLowerCase());
        const nameB =
        /* istanbul ignore next */
        (cov_1v8pj5tq7h().s[407]++, `${b.first_name} ${b.last_name}`.toLowerCase());
        /* istanbul ignore next */
        cov_1v8pj5tq7h().s[408]++;
        return nameA.localeCompare(nameB);
      });
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[409]++;
      return {
        students: validStudents,
        academic_year,
        total_students: validStudents.length,
        classes_included: classes.rows
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[410]++;
      console.error("Error fetching students by year and class ID:", error);
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[411]++;
      throw error;
    }
  }
  // Get students grouped by class for a specific academic year
  async getStudentsGroupedByClassForYear(campus_id, academic_year) {
    /* istanbul ignore next */
    cov_1v8pj5tq7h().f[59]++;
    cov_1v8pj5tq7h().s[412]++;
    try {
      // Get all classes for the academic year
      const classes =
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[413]++, await Class.find({
        campus_id,
        academic_year,
        is_active: true,
        is_deleted: false
      }, {
        sort: {
          name: "ASC"
        }
      }));
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[414]++;
      if (classes.rows.length === 0) {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().b[92][0]++;
        cov_1v8pj5tq7h().s[415]++;
        throw new Error(`No classes found for academic year: ${academic_year}`);
      } else
      /* istanbul ignore next */
      {
        cov_1v8pj5tq7h().b[92][1]++;
      }
      // Get students for each class
      const classesWithStudents =
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[416]++, await Promise.all(classes.rows.map(async classData => {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().f[60]++;
        let students =
        /* istanbul ignore next */
        (cov_1v8pj5tq7h().s[417]++, []);
        /* istanbul ignore next */
        cov_1v8pj5tq7h().s[418]++;
        if (
        /* istanbul ignore next */
        (cov_1v8pj5tq7h().b[94][0]++, classData.student_ids) &&
        /* istanbul ignore next */
        (cov_1v8pj5tq7h().b[94][1]++, classData.student_ids.length > 0)) {
          /* istanbul ignore next */
          cov_1v8pj5tq7h().b[93][0]++;
          const studentsData =
          /* istanbul ignore next */
          (cov_1v8pj5tq7h().s[419]++, await Promise.all(classData.student_ids.map(async studentId => {
            /* istanbul ignore next */
            cov_1v8pj5tq7h().f[61]++;
            cov_1v8pj5tq7h().s[420]++;
            try {
              /* istanbul ignore next */
              cov_1v8pj5tq7h().s[421]++;
              return await UserService.getUser(studentId);
            } catch (error) {
              /* istanbul ignore next */
              cov_1v8pj5tq7h().s[422]++;
              console.warn(`Failed to get student data for ID: ${studentId}`, error);
              /* istanbul ignore next */
              cov_1v8pj5tq7h().s[423]++;
              return null;
            }
          })));
          /* istanbul ignore next */
          cov_1v8pj5tq7h().s[424]++;
          students = studentsData.filter(student => {
            /* istanbul ignore next */
            cov_1v8pj5tq7h().f[62]++;
            cov_1v8pj5tq7h().s[425]++;
            return student !== null;
          });
          // Sort students by name
          /* istanbul ignore next */
          cov_1v8pj5tq7h().s[426]++;
          students.sort((a, b) => {
            /* istanbul ignore next */
            cov_1v8pj5tq7h().f[63]++;
            const nameA =
            /* istanbul ignore next */
            (cov_1v8pj5tq7h().s[427]++, `${a.first_name} ${a.last_name}`.toLowerCase());
            const nameB =
            /* istanbul ignore next */
            (cov_1v8pj5tq7h().s[428]++, `${b.first_name} ${b.last_name}`.toLowerCase());
            /* istanbul ignore next */
            cov_1v8pj5tq7h().s[429]++;
            return nameA.localeCompare(nameB);
          });
        } else
        /* istanbul ignore next */
        {
          cov_1v8pj5tq7h().b[93][1]++;
        }
        cov_1v8pj5tq7h().s[430]++;
        return {
          class_info: classData,
          students,
          student_count: students.length
        };
      })));
      // Calculate total students across all classes
      const totalStudents =
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[431]++, classesWithStudents.reduce((total, classWithStudents) => {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().f[64]++;
        cov_1v8pj5tq7h().s[432]++;
        return total + classWithStudents.student_count;
      }, 0));
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[433]++;
      return {
        academic_year,
        total_students: totalStudents,
        total_classes: classes.rows.length,
        classes: classesWithStudents
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[434]++;
      console.error("Error fetching students grouped by class for year:", error);
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[435]++;
      throw error;
    }
  }
  // Get all unique academic years for a campus
  async getAcademicYearsByCampus(campus_id) {
    /* istanbul ignore next */
    cov_1v8pj5tq7h().f[65]++;
    cov_1v8pj5tq7h().s[436]++;
    try {
      const classes =
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[437]++, await Class.find({
        campus_id,
        is_active: true,
        is_deleted: false
      }, {
        sort: {
          academic_year: "DESC"
        }
      }));
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[438]++;
      if (classes.rows.length === 0) {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().b[95][0]++;
        cov_1v8pj5tq7h().s[439]++;
        return [];
      } else
      /* istanbul ignore next */
      {
        cov_1v8pj5tq7h().b[95][1]++;
      }
      // Get unique academic years
      const academicYears =
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[440]++, [...new Set(classes.rows.map(classData => {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().f[66]++;
        cov_1v8pj5tq7h().s[441]++;
        return classData.academic_year;
      }))]);
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[442]++;
      return academicYears.sort().reverse(); // Most recent first
    } catch (error) {
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[443]++;
      console.error("Error fetching academic years:", error);
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[444]++;
      return [];
    }
  }
  // Grade an assignment submission
  async gradeAssignmentSubmission(submission_id, grade, feedback) {
    /* istanbul ignore next */
    cov_1v8pj5tq7h().f[67]++;
    cov_1v8pj5tq7h().s[445]++;
    try {
      const submission =
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[446]++, await AssignmentSubmission.findById(submission_id));
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[447]++;
      if (!submission) {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().b[96][0]++;
        cov_1v8pj5tq7h().s[448]++;
        throw new Error("Assignment submission not found");
      } else
      /* istanbul ignore next */
      {
        cov_1v8pj5tq7h().b[96][1]++;
      }
      const updatedSubmission =
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[449]++, await AssignmentSubmission.updateById(submission_id, {
        grade,
        feedback:
        /* istanbul ignore next */
        (cov_1v8pj5tq7h().b[97][0]++, feedback) ||
        /* istanbul ignore next */
        (cov_1v8pj5tq7h().b[97][1]++, submission.feedback),
        updated_at: new Date()
      }));
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[450]++;
      if (!updatedSubmission) {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().b[98][0]++;
        cov_1v8pj5tq7h().s[451]++;
        throw new Error("Failed to update assignment submission");
      } else
      /* istanbul ignore next */
      {
        cov_1v8pj5tq7h().b[98][1]++;
      }
      cov_1v8pj5tq7h().s[452]++;
      return updatedSubmission;
    } catch (error) {
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[453]++;
      console.error("Error grading assignment submission:", error);
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[454]++;
      throw error;
    }
  }
  // Get student assignments with submission status and grades
  async getStudentAssignmentsWithSubmissions(student_id, campus_id) {
    /* istanbul ignore next */
    cov_1v8pj5tq7h().f[68]++;
    cov_1v8pj5tq7h().s[455]++;
    try {
      // First, get all classes the student is enrolled in
      const studentClasses =
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[456]++, await Class.find({
        campus_id,
        student_ids: {
          $in: [student_id]
        },
        is_active: true,
        is_deleted: false
      }));
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[457]++;
      if (studentClasses.rows.length === 0) {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().b[99][0]++;
        cov_1v8pj5tq7h().s[458]++;
        return [];
      } else
      /* istanbul ignore next */
      {
        cov_1v8pj5tq7h().b[99][1]++;
      }
      const assignments =
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[459]++, []);
      // For each class, get all assignments and check submission status
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[460]++;
      for (const classData of studentClasses.rows) {
        const classAssignments =
        /* istanbul ignore next */
        (cov_1v8pj5tq7h().s[461]++, await Assignment.find({
          campus_id,
          class_id: classData.id,
          is_active: true,
          is_deleted: false
        }));
        /* istanbul ignore next */
        cov_1v8pj5tq7h().s[462]++;
        for (const assignment of classAssignments.rows) {
          // Check if student has submitted this assignment
          const submission =
          /* istanbul ignore next */
          (cov_1v8pj5tq7h().s[463]++, await AssignmentSubmission.find({
            assignment_id: assignment.id,
            user_id: student_id
          }));
          let status =
          /* istanbul ignore next */
          (cov_1v8pj5tq7h().s[464]++, "not_submitted");
          let submissionData =
          /* istanbul ignore next */
          (cov_1v8pj5tq7h().s[465]++, null);
          /* istanbul ignore next */
          cov_1v8pj5tq7h().s[466]++;
          if (submission.rows.length > 0) {
            /* istanbul ignore next */
            cov_1v8pj5tq7h().b[100][0]++;
            cov_1v8pj5tq7h().s[467]++;
            submissionData = submission.rows[0];
            // Check if assignment is overdue
            const currentDate =
            /* istanbul ignore next */
            (cov_1v8pj5tq7h().s[468]++, new Date());
            const dueDate =
            /* istanbul ignore next */
            (cov_1v8pj5tq7h().s[469]++, new Date(assignment.due_date));
            /* istanbul ignore next */
            cov_1v8pj5tq7h().s[470]++;
            if (
            /* istanbul ignore next */
            (cov_1v8pj5tq7h().b[102][0]++, submissionData.grade !== null) &&
            /* istanbul ignore next */
            (cov_1v8pj5tq7h().b[102][1]++, submissionData.grade !== undefined)) {
              /* istanbul ignore next */
              cov_1v8pj5tq7h().b[101][0]++;
              cov_1v8pj5tq7h().s[471]++;
              status = "graded";
            } else {
              /* istanbul ignore next */
              cov_1v8pj5tq7h().b[101][1]++;
              cov_1v8pj5tq7h().s[472]++;
              if (currentDate > dueDate) {
                /* istanbul ignore next */
                cov_1v8pj5tq7h().b[103][0]++;
                cov_1v8pj5tq7h().s[473]++;
                status = "overdue";
              } else {
                /* istanbul ignore next */
                cov_1v8pj5tq7h().b[103][1]++;
                cov_1v8pj5tq7h().s[474]++;
                status = "submitted";
              }
            }
          } else {
            /* istanbul ignore next */
            cov_1v8pj5tq7h().b[100][1]++;
            // Check if assignment is overdue
            const currentDate =
            /* istanbul ignore next */
            (cov_1v8pj5tq7h().s[475]++, new Date());
            const dueDate =
            /* istanbul ignore next */
            (cov_1v8pj5tq7h().s[476]++, new Date(assignment.due_date));
            /* istanbul ignore next */
            cov_1v8pj5tq7h().s[477]++;
            if (currentDate > dueDate) {
              /* istanbul ignore next */
              cov_1v8pj5tq7h().b[104][0]++;
              cov_1v8pj5tq7h().s[478]++;
              status = "overdue";
            } else
            /* istanbul ignore next */
            {
              cov_1v8pj5tq7h().b[104][1]++;
            }
          }
          /* istanbul ignore next */
          cov_1v8pj5tq7h().s[479]++;
          assignments.push({
            assignment,
            submission: submissionData,
            status,
            class_info: {
              id: classData.id,
              name: classData.name,
              academic_year: classData.academic_year
            }
          });
        }
      }
      // Sort by assignment due date (newest first)
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[480]++;
      assignments.sort((a, b) => {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().f[69]++;
        const dateA =
        /* istanbul ignore next */
        (cov_1v8pj5tq7h().s[481]++, new Date(a.assignment.due_date));
        const dateB =
        /* istanbul ignore next */
        (cov_1v8pj5tq7h().s[482]++, new Date(b.assignment.due_date));
        /* istanbul ignore next */
        cov_1v8pj5tq7h().s[483]++;
        return dateB.getTime() - dateA.getTime();
      });
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[484]++;
      return assignments;
    } catch (error) {
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[485]++;
      console.error("Error fetching student assignments with submissions:", error);
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[486]++;
      throw error;
    }
  }
  // Update an assignment submission (for resubmission)
  async updateAssignmentSubmission(submission_id, updateData) {
    /* istanbul ignore next */
    cov_1v8pj5tq7h().f[70]++;
    cov_1v8pj5tq7h().s[487]++;
    try {
      const submission =
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[488]++, await AssignmentSubmission.findById(submission_id));
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[489]++;
      if (!submission) {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().b[105][0]++;
        cov_1v8pj5tq7h().s[490]++;
        throw new Error("Assignment submission not found");
      } else
      /* istanbul ignore next */
      {
        cov_1v8pj5tq7h().b[105][1]++;
      }
      const updatedSubmission =
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[491]++, await AssignmentSubmission.updateById(submission_id, Object.assign(Object.assign({}, updateData), {
        submission_date: updateData.submission_date ?
        /* istanbul ignore next */
        (cov_1v8pj5tq7h().b[106][0]++, new Date(updateData.submission_date)) :
        /* istanbul ignore next */
        (cov_1v8pj5tq7h().b[106][1]++, submission.submission_date),
        updated_at: new Date()
      })));
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[492]++;
      if (!updatedSubmission) {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().b[107][0]++;
        cov_1v8pj5tq7h().s[493]++;
        throw new Error("Failed to update assignment submission");
      } else
      /* istanbul ignore next */
      {
        cov_1v8pj5tq7h().b[107][1]++;
      }
      cov_1v8pj5tq7h().s[494]++;
      return updatedSubmission;
    } catch (error) {
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[495]++;
      console.error("Error updating assignment submission:", error);
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[496]++;
      throw error;
    }
  }
  // Get assignments due soon for a student
  async getAssignmentsDueSoon(student_id, campus_id, daysAhead =
  /* istanbul ignore next */
  (cov_1v8pj5tq7h().b[108][0]++, 7)) {
    /* istanbul ignore next */
    cov_1v8pj5tq7h().f[71]++;
    cov_1v8pj5tq7h().s[497]++;
    try {
      // Get current date and future date
      const currentDate =
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[498]++, new Date());
      const futureDate =
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[499]++, new Date());
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[500]++;
      futureDate.setDate(currentDate.getDate() + daysAhead);
      // Get all classes the student is enrolled in
      const studentClasses =
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[501]++, await Class.find({
        campus_id,
        student_ids: {
          $in: [student_id]
        },
        is_active: true,
        is_deleted: false
      }));
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[502]++;
      if (studentClasses.rows.length === 0) {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().b[109][0]++;
        cov_1v8pj5tq7h().s[503]++;
        return [];
      } else
      /* istanbul ignore next */
      {
        cov_1v8pj5tq7h().b[109][1]++;
      }
      const dueSoonAssignments =
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[504]++, []);
      // For each class, get assignments due within the specified days
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[505]++;
      for (const classData of studentClasses.rows) {
        const classAssignments =
        /* istanbul ignore next */
        (cov_1v8pj5tq7h().s[506]++, await Assignment.find({
          campus_id,
          class_id: classData.id,
          is_active: true,
          is_deleted: false,
          due_date: {
            $gte: currentDate.toISOString(),
            $lte: futureDate.toISOString()
          }
        }));
        /* istanbul ignore next */
        cov_1v8pj5tq7h().s[507]++;
        for (const assignment of classAssignments.rows) {
          // Check if student has submitted this assignment
          const submission =
          /* istanbul ignore next */
          (cov_1v8pj5tq7h().s[508]++, await AssignmentSubmission.find({
            assignment_id: assignment.id,
            user_id: student_id
          }));
          const isSubmitted =
          /* istanbul ignore next */
          (cov_1v8pj5tq7h().s[509]++, submission.rows.length > 0);
          // Calculate days until due
          const dueDate =
          /* istanbul ignore next */
          (cov_1v8pj5tq7h().s[510]++, new Date(assignment.due_date));
          const timeDiff =
          /* istanbul ignore next */
          (cov_1v8pj5tq7h().s[511]++, dueDate.getTime() - currentDate.getTime());
          const daysUntilDue =
          /* istanbul ignore next */
          (cov_1v8pj5tq7h().s[512]++, Math.ceil(timeDiff / (1000 * 3600 * 24)));
          /* istanbul ignore next */
          cov_1v8pj5tq7h().s[513]++;
          dueSoonAssignments.push({
            assignment,
            days_until_due: daysUntilDue,
            is_submitted: isSubmitted,
            class_info: {
              id: classData.id,
              name: classData.name,
              academic_year: classData.academic_year
            }
          });
        }
      }
      // Sort by days until due (most urgent first)
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[514]++;
      dueSoonAssignments.sort((a, b) => {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().f[72]++;
        cov_1v8pj5tq7h().s[515]++;
        return a.days_until_due - b.days_until_due;
      });
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[516]++;
      return dueSoonAssignments;
    } catch (error) {
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[517]++;
      console.error("Error fetching assignments due soon:", error);
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[518]++;
      throw error;
    }
  }
}