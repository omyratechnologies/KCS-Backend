  async getClassesByStudentId(studentId) {
    /* istanbul ignore next */
    cov_1v8pj5tq7h().f[41]++;
    cov_1v8pj5tq7h().s[247]++;
    try {
      // First, let's try with the correct Ottoman syntax for array contains
      const classes =
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[248]++, await Class.find({
        student_ids: studentId,
        is_active: true,
        is_deleted: false
      }, {
        sort: {
          updated_at: "DESC"
        }
      }));
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[249]++;
      if (classes.rows.length === 0) {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().b[44][0]++;
        cov_1v8pj5tq7h().s[250]++;
        // If no results found, try alternative approach
        console.log("No classes found with simple contains, trying alternative query");
        // Get all active classes and filter manually
        const allClasses =
        /* istanbul ignore next */
        (cov_1v8pj5tq7h().s[251]++, await Class.find({
          is_active: true,
          is_deleted: false
        }, {
          sort: {
            updated_at: "DESC"
          }
        }));
        const filteredClasses =
        /* istanbul ignore next */
        (cov_1v8pj5tq7h().s[252]++, allClasses.rows.filter(classItem => {
          /* istanbul ignore next */
          cov_1v8pj5tq7h().f[42]++;
          cov_1v8pj5tq7h().s[253]++;
          return /* istanbul ignore next */(cov_1v8pj5tq7h().b[45][0]++, classItem.student_ids) &&
          /* istanbul ignore next */
          (cov_1v8pj5tq7h().b[45][1]++, classItem.student_ids.includes(studentId));
        }));
        /* istanbul ignore next */
        cov_1v8pj5tq7h().s[254]++;
        if (filteredClasses.length === 0) {
          /* istanbul ignore next */
          cov_1v8pj5tq7h().b[46][0]++;
          cov_1v8pj5tq7h().s[255]++;
          throw new Error("No classes found for this student");
