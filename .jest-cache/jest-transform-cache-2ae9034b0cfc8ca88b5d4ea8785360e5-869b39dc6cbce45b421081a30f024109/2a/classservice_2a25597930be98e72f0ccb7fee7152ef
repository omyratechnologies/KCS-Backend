  // Create class subject
  async createClassSubject(campus_id, class_id, classSubjectData) {
    /* istanbul ignore next */
    cov_1v8pj5tq7h().f[16]++;
    cov_1v8pj5tq7h().s[105]++;
    try {
      const newClassSubject =
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[106]++, await ClassSubject.create(Object.assign(Object.assign({
        campus_id,
        class_id
      }, classSubjectData), {
        created_at: new Date(),
        updated_at: new Date()
      })));
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[107]++;
      if (!newClassSubject) {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().b[22][0]++;
        cov_1v8pj5tq7h().s[108]++;
        throw new Error("Failed to create class subject");
      } else
      /* istanbul ignore next */
      {
        cov_1v8pj5tq7h().b[22][1]++;
      }
      const teacher =
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[109]++, await TeacherService.getTeacherById(classSubjectData.teacher_id));
      // append class to teacher's subjects
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[110]++;
      if (teacher) {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().b[23][0]++;
        cov_1v8pj5tq7h().s[111]++;
        teacher.subjects.push(classSubjectData.subject_id);
        /* istanbul ignore next */
        cov_1v8pj5tq7h().s[112]++;
        await TeacherService.updateTeacher(teacher.id, {
          subjects: teacher.subjects
        });
      } else
      /* istanbul ignore next */
      {
        cov_1v8pj5tq7h().b[23][1]++;
      }
      cov_1v8pj5tq7h().s[113]++;
      return newClassSubject;
    } catch (error) {
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[114]++;
      console.error("Error creating class subject:", error);
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[115]++;
      throw error;
    }
  }
  // Update class subject
  async updateClassSubject(id, classSubjectData) {
    /* istanbul ignore next */
    cov_1v8pj5tq7h().f[17]++;
    cov_1v8pj5tq7h().s[116]++;
    try {
