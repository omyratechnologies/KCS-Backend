          cov_1v8pj5tq7h().s[313]++;
          throw new Error(`Teacher with ID ${teacherId} not found`);
        } else
        /* istanbul ignore next */
        {
          cov_1v8pj5tq7h().b[65][1]++;
        }
        cov_1v8pj5tq7h().s[314]++;
        validTeachers.push(teacherId);
      }
      // Get current teacher IDs to prevent duplicates
      const currentTeacherIds =
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[315]++,
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().b[66][0]++, existingClass.teacher_ids) ||
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().b[66][1]++, []));
      const duplicateTeachers =
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[316]++, validTeachers.filter(teacherId => {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().f[46]++;
        cov_1v8pj5tq7h().s[317]++;
        return currentTeacherIds.includes(teacherId);
      }));
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[318]++;
      if (duplicateTeachers.length > 0) {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().b[67][0]++;
        cov_1v8pj5tq7h().s[319]++;
        throw new Error(`Teachers with IDs ${duplicateTeachers.join(", ")} are already assigned to this class`);
      } else
      /* istanbul ignore next */
      {
        cov_1v8pj5tq7h().b[67][1]++;
      }
      // Merge new teacher IDs with existing ones
      const updatedTeacherIds =
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[320]++, [...currentTeacherIds, ...validTeachers]);
      // Update class with new teacher IDs
      const updatedClass =
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[321]++, await Class.findOneAndUpdate({
        id: classId,
        is_active: true,
        is_deleted: false
      }, {
        teacher_ids: updatedTeacherIds,
        updated_at: new Date()
      }, {
        new: true
      }));
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[322]++;
      if (!updatedClass) {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().b[68][0]++;
        cov_1v8pj5tq7h().s[323]++;
        throw new Error("Failed to update class");
      } else
      /* istanbul ignore next */
      {
        cov_1v8pj5tq7h().b[68][1]++;
      }
      // Update each teacher's classes array
      cov_1v8pj5tq7h().s[324]++;
      for (const teacherId of validTeachers) {
        const teacher =
        /* istanbul ignore next */
        (cov_1v8pj5tq7h().s[325]++, await TeacherService.getTeacherById(teacherId));
        /* istanbul ignore next */
        cov_1v8pj5tq7h().s[326]++;
        if (teacher) {
          /* istanbul ignore next */
          cov_1v8pj5tq7h().b[69][0]++;
          cov_1v8pj5tq7h().s[327]++;
          if (!teacher.classes) {
            /* istanbul ignore next */
            cov_1v8pj5tq7h().b[70][0]++;
            cov_1v8pj5tq7h().s[328]++;
            teacher.classes = [];
          } else
          /* istanbul ignore next */
          {
            cov_1v8pj5tq7h().b[70][1]++;
          }
          cov_1v8pj5tq7h().s[329]++;
          if (!teacher.classes.includes(classId)) {
            /* istanbul ignore next */
            cov_1v8pj5tq7h().b[71][0]++;
            cov_1v8pj5tq7h().s[330]++;
            teacher.classes.push(classId);
            /* istanbul ignore next */
            cov_1v8pj5tq7h().s[331]++;
            await TeacherService.updateTeacher(teacher.id, {
              classes: teacher.classes
            });
          } else
          /* istanbul ignore next */
          {
            cov_1v8pj5tq7h().b[71][1]++;
          }
        } else
        /* istanbul ignore next */
        {
          cov_1v8pj5tq7h().b[69][1]++;
        }
      }
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[332]++;
      return updatedClass;
    } catch (error) {
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[333]++;
      console.error("Error assigning teachers to class:", error);
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[334]++;
      throw error;
    }
  }
  // Remove students from class
  async removeStudentsFromClass(classId, studentIds) {
    /* istanbul ignore next */
    cov_1v8pj5tq7h().f[47]++;
    cov_1v8pj5tq7h().s[335]++;
    try {
      // Validate class exists
      const existingClass =
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[336]++, await Class.findById(classId));
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[337]++;
      if (!existingClass) {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().b[72][0]++;
        cov_1v8pj5tq7h().s[338]++;
        throw new Error("Class not found");
      } else
      /* istanbul ignore next */
      {
        cov_1v8pj5tq7h().b[72][1]++;
      }
      cov_1v8pj5tq7h().s[339]++;
      if (
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().b[74][0]++, !existingClass.is_active) ||
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().b[74][1]++, existingClass.is_deleted)) {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().b[73][0]++;
        cov_1v8pj5tq7h().s[340]++;
        throw new Error("Class is not active or has been deleted");
      } else
      /* istanbul ignore next */
      {
        cov_1v8pj5tq7h().b[73][1]++;
      }
      // Get current student IDs
      const currentStudentIds =
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[341]++,
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().b[75][0]++, existingClass.student_ids) ||
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().b[75][1]++, []));
      const studentsNotInClass =
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[342]++, studentIds.filter(studentId => {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().f[48]++;
        cov_1v8pj5tq7h().s[343]++;
        return !currentStudentIds.includes(studentId);
      }));
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[344]++;
      if (studentsNotInClass.length > 0) {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().b[76][0]++;
        cov_1v8pj5tq7h().s[345]++;
        throw new Error(`Students with IDs ${studentsNotInClass.join(", ")} are not assigned to this class`);
      } else
      /* istanbul ignore next */
      {
        cov_1v8pj5tq7h().b[76][1]++;
      }
      // Remove student IDs from class
      const updatedStudentIds =
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[346]++, currentStudentIds.filter(studentId => {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().f[49]++;
        cov_1v8pj5tq7h().s[347]++;
        return !studentIds.includes(studentId);
      }));
      // Update class
      const updatedClass =
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[348]++, await Class.findOneAndUpdate({
        id: classId,
        is_active: true,
        is_deleted: false
      }, {
        student_ids: updatedStudentIds,
        student_count: updatedStudentIds.length,
        updated_at: new Date()
      }, {
        new: true
      }));
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[349]++;
      if (!updatedClass) {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().b[77][0]++;
        cov_1v8pj5tq7h().s[350]++;
        throw new Error("Failed to update class");
      } else
      /* istanbul ignore next */
      {
        cov_1v8pj5tq7h().b[77][1]++;
      }
      cov_1v8pj5tq7h().s[351]++;
      return updatedClass;
    } catch (error) {
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[352]++;
      console.error("Error removing students from class:", error);
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[353]++;
      throw error;
    }
  }
