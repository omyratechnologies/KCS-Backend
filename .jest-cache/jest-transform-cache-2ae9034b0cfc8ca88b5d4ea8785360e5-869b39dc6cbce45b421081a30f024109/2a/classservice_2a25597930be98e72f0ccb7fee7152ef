            /* istanbul ignore next */
            cov_1v8pj5tq7h().f[53]++;
            cov_1v8pj5tq7h().s[374]++;
            return id !== classId;
          }));
          /* istanbul ignore next */
          cov_1v8pj5tq7h().s[375]++;
          await TeacherService.updateTeacher(teacher.id, {
            classes: updatedClasses
          });
        } else
        /* istanbul ignore next */
        {
          cov_1v8pj5tq7h().b[84][1]++;
        }
      }
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[376]++;
      return updatedClass;
    } catch (error) {
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[377]++;
      console.error("Error removing teachers from class:", error);
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[378]++;
      throw error;
    }
  }
