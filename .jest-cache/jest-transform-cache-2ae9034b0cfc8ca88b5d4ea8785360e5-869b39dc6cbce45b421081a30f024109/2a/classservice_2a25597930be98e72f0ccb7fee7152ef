        /* istanbul ignore next */
        cov_1v8pj5tq7h().b[82][0]++;
        cov_1v8pj5tq7h().s[364]++;
        throw new Error(`Teachers with IDs ${teachersNotInClass.join(", ")} are not assigned to this class`);
      } else
      /* istanbul ignore next */
      {
        cov_1v8pj5tq7h().b[82][1]++;
      }
      // Remove teacher IDs from class
      const updatedTeacherIds =
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[365]++, currentTeacherIds.filter(teacherId => {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().f[52]++;
        cov_1v8pj5tq7h().s[366]++;
        return !teacherIds.includes(teacherId);
