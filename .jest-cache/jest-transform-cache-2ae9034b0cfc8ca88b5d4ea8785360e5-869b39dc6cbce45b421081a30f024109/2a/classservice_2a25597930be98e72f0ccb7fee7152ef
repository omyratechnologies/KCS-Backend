  // Assign students to class
  async assignStudentsToClass(classId, studentIds) {
    /* istanbul ignore next */
    cov_1v8pj5tq7h().f[43]++;
    cov_1v8pj5tq7h().s[260]++;
    try {
      // Validate class exists
      const existingClass =
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[261]++, await Class.findById(classId));
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[262]++;
      if (!existingClass) {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().b[47][0]++;
        cov_1v8pj5tq7h().s[263]++;
        throw new Error("Class not found");
      } else
      /* istanbul ignore next */
      {
        cov_1v8pj5tq7h().b[47][1]++;
      }
      cov_1v8pj5tq7h().s[264]++;
      if (
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().b[49][0]++, !existingClass.is_active) ||
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().b[49][1]++, existingClass.is_deleted)) {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().b[48][0]++;
        cov_1v8pj5tq7h().s[265]++;
        throw new Error("Class is not active or has been deleted");
      } else
      /* istanbul ignore next */
      {
        cov_1v8pj5tq7h().b[48][1]++;
      }
      // Validate student IDs exist and are valid
      const validStudents =
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[266]++, []);
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[267]++;
      for (const studentId of studentIds) {
        const student =
        /* istanbul ignore next */
        (cov_1v8pj5tq7h().s[268]++, await UserService.getUser(studentId));
        /* istanbul ignore next */
        cov_1v8pj5tq7h().s[269]++;
        if (!student) {
          /* istanbul ignore next */
          cov_1v8pj5tq7h().b[50][0]++;
          cov_1v8pj5tq7h().s[270]++;
          throw new Error(`Student with ID ${studentId} not found`);
        } else
        /* istanbul ignore next */
        {
          cov_1v8pj5tq7h().b[50][1]++;
        }
        cov_1v8pj5tq7h().s[271]++;
        if (student.user_type !== "Student") {
          /* istanbul ignore next */
          cov_1v8pj5tq7h().b[51][0]++;
          cov_1v8pj5tq7h().s[272]++;
          throw new Error(`User with ID ${studentId} is not a student`);
        } else
        /* istanbul ignore next */
        {
          cov_1v8pj5tq7h().b[51][1]++;
        }
        cov_1v8pj5tq7h().s[273]++;
        if (
        /* istanbul ignore next */
        (cov_1v8pj5tq7h().b[53][0]++, !student.is_active) ||
        /* istanbul ignore next */
        (cov_1v8pj5tq7h().b[53][1]++, student.is_deleted)) {
          /* istanbul ignore next */
          cov_1v8pj5tq7h().b[52][0]++;
          cov_1v8pj5tq7h().s[274]++;
          throw new Error(`Student with ID ${studentId} is not active`);
        } else
        /* istanbul ignore next */
        {
          cov_1v8pj5tq7h().b[52][1]++;
        }
        cov_1v8pj5tq7h().s[275]++;
        validStudents.push(studentId);
      }
      // Get current student IDs to prevent duplicates
      const currentStudentIds =
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[276]++,
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().b[54][0]++, existingClass.student_ids) ||
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().b[54][1]++, []));
      const duplicateStudents =
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[277]++, validStudents.filter(studentId => {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().f[44]++;
        cov_1v8pj5tq7h().s[278]++;
        return currentStudentIds.includes(studentId);
      }));
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[279]++;
      if (duplicateStudents.length > 0) {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().b[55][0]++;
        cov_1v8pj5tq7h().s[280]++;
        throw new Error(`Students with IDs ${duplicateStudents.join(", ")} are already assigned to this class`);
      } else
      /* istanbul ignore next */
      {
        cov_1v8pj5tq7h().b[55][1]++;
      }
      // Merge new student IDs with existing ones
      const updatedStudentIds =
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[281]++, [...currentStudentIds, ...validStudents]);
      // Update class with new student IDs
      const updatedClass =
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[282]++, await Class.findOneAndUpdate({
        id: classId,
        is_active: true,
        is_deleted: false
      }, {
        student_ids: updatedStudentIds,
        student_count: updatedStudentIds.length,
        updated_at: new Date()
      }, {
        new: true
      }));
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[283]++;
      if (!updatedClass) {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().b[56][0]++;
        cov_1v8pj5tq7h().s[284]++;
        throw new Error("Failed to update class");
      } else
      /* istanbul ignore next */
      {
        cov_1v8pj5tq7h().b[56][1]++;
      }
      // Update each student's meta_data to include the class
      cov_1v8pj5tq7h().s[285]++;
      for (const studentId of validStudents) {
        const student =
        /* istanbul ignore next */
        (cov_1v8pj5tq7h().s[286]++, await UserService.getUser(studentId));
        /* istanbul ignore next */
        cov_1v8pj5tq7h().s[287]++;
        if (student) {
          /* istanbul ignore next */
          cov_1v8pj5tq7h().b[57][0]++;
          // Parse meta_data if it's a string, otherwise use as object
          let currentMetaData =
          /* istanbul ignore next */
          (cov_1v8pj5tq7h().s[288]++, {});
          /* istanbul ignore next */
          cov_1v8pj5tq7h().s[289]++;
          if (student.meta_data) {
            /* istanbul ignore next */
            cov_1v8pj5tq7h().b[58][0]++;
            cov_1v8pj5tq7h().s[290]++;
            if (typeof student.meta_data === "string") {
              /* istanbul ignore next */
              cov_1v8pj5tq7h().b[59][0]++;
              cov_1v8pj5tq7h().s[291]++;
              try {
                /* istanbul ignore next */
                cov_1v8pj5tq7h().s[292]++;
                currentMetaData = JSON.parse(student.meta_data);
              } catch (_a) {
                /* istanbul ignore next */
                cov_1v8pj5tq7h().s[293]++;
                currentMetaData = {};
              }
            } else {
              /* istanbul ignore next */
              cov_1v8pj5tq7h().b[59][1]++;
              cov_1v8pj5tq7h().s[294]++;
              currentMetaData = student.meta_data;
            }
          } else
          /* istanbul ignore next */
          {
            cov_1v8pj5tq7h().b[58][1]++;
          }
          const currentClasses =
          /* istanbul ignore next */
          (cov_1v8pj5tq7h().s[295]++,
          /* istanbul ignore next */
          (cov_1v8pj5tq7h().b[60][0]++, currentMetaData.classes) ||
          /* istanbul ignore next */
          (cov_1v8pj5tq7h().b[60][1]++, []));
          // Check if class is already assigned to avoid duplicates
          /* istanbul ignore next */
          cov_1v8pj5tq7h().s[296]++;
          if (!currentClasses.includes(classId)) {
            /* istanbul ignore next */
            cov_1v8pj5tq7h().b[61][0]++;
            const updatedClasses =
            /* istanbul ignore next */
            (cov_1v8pj5tq7h().s[297]++, [...currentClasses, classId]);
            const updatedMetaData =
            /* istanbul ignore next */
            (cov_1v8pj5tq7h().s[298]++, Object.assign(Object.assign({}, currentMetaData), {
              classes: updatedClasses
            }));
            /* istanbul ignore next */
            cov_1v8pj5tq7h().s[299]++;
            await UserService.updateUsers(student.id, {
              meta_data: JSON.stringify(updatedMetaData)
            });
          } else
          /* istanbul ignore next */
          {
            cov_1v8pj5tq7h().b[61][1]++;
          }
        } else
        /* istanbul ignore next */
        {
          cov_1v8pj5tq7h().b[57][1]++;
        }
      }
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[300]++;
      return updatedClass;
    } catch (error) {
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[301]++;
      console.error("Error assigning students to class:", error);
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[302]++;
      throw error;
    }
  }
