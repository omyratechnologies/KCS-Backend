  // Get all students by class ID
  async getStudentsByClassId(classId) {
    /* istanbul ignore next */
    cov_1v8pj5tq7h().f[26]++;
    cov_1v8pj5tq7h().s[151]++;
    try {
      // Get the class data
      const classData =
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[152]++, await Class.findById(classId));
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[153]++;
      if (!classData) {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().b[27][0]++;
        cov_1v8pj5tq7h().s[154]++;
        throw new Error("Class not found");
      } else
      /* istanbul ignore next */
      {
        cov_1v8pj5tq7h().b[27][1]++;
      }
      cov_1v8pj5tq7h().s[155]++;
      if (
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().b[29][0]++, !classData.is_active) ||
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().b[29][1]++, classData.is_deleted)) {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().b[28][0]++;
        cov_1v8pj5tq7h().s[156]++;
        throw new Error("Class is not active or has been deleted");
      } else
      /* istanbul ignore next */
      {
        cov_1v8pj5tq7h().b[28][1]++;
      }
      // Check if class has students
      cov_1v8pj5tq7h().s[157]++;
      if (
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().b[31][0]++, !classData.student_ids) ||
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().b[31][1]++, classData.student_ids.length === 0)) {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().b[30][0]++;
        cov_1v8pj5tq7h().s[158]++;
        return [];
      } else
      /* istanbul ignore next */
      {
        cov_1v8pj5tq7h().b[30][1]++;
      }
      // Get all students data
      const students =
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[159]++, await Promise.all(classData.student_ids.map(async studentId => {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().f[27]++;
        cov_1v8pj5tq7h().s[160]++;
        try {
          const student =
          /* istanbul ignore next */
          (cov_1v8pj5tq7h().s[161]++, await UserService.getUser(studentId));
          /* istanbul ignore next */
          cov_1v8pj5tq7h().s[162]++;
          if (!student) {
            /* istanbul ignore next */
            cov_1v8pj5tq7h().b[32][0]++;
            cov_1v8pj5tq7h().s[163]++;
            console.warn(`Student with ID ${studentId} not found`);
            /* istanbul ignore next */
            cov_1v8pj5tq7h().s[164]++;
            return null;
          } else
          /* istanbul ignore next */
          {
            cov_1v8pj5tq7h().b[32][1]++;
          }
          cov_1v8pj5tq7h().s[165]++;
          return {
            id: student.id,
            user_id: student.user_id,
            // Assuming user_id is the same as id
            name: `${student.first_name} ${student.last_name}`
          };
        } catch (error) {
          /* istanbul ignore next */
          cov_1v8pj5tq7h().s[166]++;
          console.warn(`Error fetching student with ID ${studentId}:`, error);
          /* istanbul ignore next */
          cov_1v8pj5tq7h().s[167]++;
          return null;
        }
      })));
      // Filter out null values and return the results
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[168]++;
      return students.filter(student => {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().f[28]++;
        cov_1v8pj5tq7h().s[169]++;
        return student !== null;
      });
    } catch (error) {
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[170]++;
      console.error("Error fetching students by class ID:", error);
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[171]++;
      throw error;
    }
  }
