  // create a assignment submission
  async createAssignmentSubmission(assignment_id, assignmentSubmissionData) {
    /* istanbul ignore next */
    cov_1v8pj5tq7h().f[36]++;
    cov_1v8pj5tq7h().s[207]++;
    try {
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[208]++;
      // Validate required fields
      if (!assignmentSubmissionData.campus_id) {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().b[34][0]++;
        cov_1v8pj5tq7h().s[209]++;
        throw new Error("campus_id is required for assignment submission");
      } else
      /* istanbul ignore next */
      {
        cov_1v8pj5tq7h().b[34][1]++;
      }
      cov_1v8pj5tq7h().s[210]++;
      if (!assignmentSubmissionData.user_id) {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().b[35][0]++;
        cov_1v8pj5tq7h().s[211]++;
        throw new Error("user_id is required for assignment submission");
      } else
      /* istanbul ignore next */
      {
        cov_1v8pj5tq7h().b[35][1]++;
      }
      const submissionData =
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[212]++, {
        assignment_id,
        campus_id: assignmentSubmissionData.campus_id,
        user_id: assignmentSubmissionData.user_id,
        submission_date:
        /* istanbul ignore next */
        (cov_1v8pj5tq7h().b[36][0]++, assignmentSubmissionData.submission_date) ||
        /* istanbul ignore next */
        (cov_1v8pj5tq7h().b[36][1]++, new Date()),
        meta_data:
        /* istanbul ignore next */
        (cov_1v8pj5tq7h().b[37][0]++, assignmentSubmissionData.meta_data) ||
        /* istanbul ignore next */
        (cov_1v8pj5tq7h().b[37][1]++, {}),
        created_at: new Date(),
        updated_at: new Date()
      });
      // Add optional fields if provided
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[213]++;
      if (assignmentSubmissionData.grade !== undefined) {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().b[38][0]++;
        cov_1v8pj5tq7h().s[214]++;
        submissionData.grade = assignmentSubmissionData.grade;
      } else
      /* istanbul ignore next */
      {
        cov_1v8pj5tq7h().b[38][1]++;
      }
      cov_1v8pj5tq7h().s[215]++;
      if (assignmentSubmissionData.feedback !== undefined) {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().b[39][0]++;
        cov_1v8pj5tq7h().s[216]++;
        submissionData.feedback = assignmentSubmissionData.feedback;
      } else
      /* istanbul ignore next */
      {
        cov_1v8pj5tq7h().b[39][1]++;
      }
      const result =
      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[217]++, await AssignmentSubmission.create(submissionData));
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[218]++;
      if (!result) {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().b[40][0]++;
        cov_1v8pj5tq7h().s[219]++;
        throw new Error("Failed to create assignment submission");
      } else
      /* istanbul ignore next */
      {
        cov_1v8pj5tq7h().b[40][1]++;
      }
      cov_1v8pj5tq7h().s[220]++;
      return result;
    } catch (error) {
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[221]++;
      console.error("Error creating assignment submission:", error);
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[222]++;
      return null;
