      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[77]++, uniqueSubjectIds.map(async subjectId => {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().f[9]++;
        cov_1v8pj5tq7h().s[78]++;
        return await Subject.findById(subjectId);
      }));
