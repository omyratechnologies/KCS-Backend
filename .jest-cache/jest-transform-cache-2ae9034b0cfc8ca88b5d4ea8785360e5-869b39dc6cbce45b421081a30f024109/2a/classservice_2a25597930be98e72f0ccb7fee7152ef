      /* istanbul ignore next */
      (cov_1v8pj5tq7h().s[79]++, await Promise.all(subjectPromises));
      /* istanbul ignore next */
      cov_1v8pj5tq7h().s[80]++;
      return subjectData.filter(subject => {
        /* istanbul ignore next */
        cov_1v8pj5tq7h().f[10]++;
        cov_1v8pj5tq7h().s[81]++;
        return subject !== null;
      });
