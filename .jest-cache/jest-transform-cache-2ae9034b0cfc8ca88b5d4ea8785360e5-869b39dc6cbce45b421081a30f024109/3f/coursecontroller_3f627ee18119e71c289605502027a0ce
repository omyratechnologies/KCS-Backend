// ==================== ADMIN BULK OPERATIONS ====================
/**
 * Bulk enroll students
 * Admin only
 */
/* istanbul ignore next */
cov_210suhh07q().s[201]++;
CourseController.bulkEnrollStudents = async ctx => {
  /* istanbul ignore next */
  cov_210suhh07q().f[15]++;
  cov_210suhh07q().s[202]++;
  try {
    const course_id =
    /* istanbul ignore next */
    (cov_210suhh07q().s[203]++, ctx.req.param("course_id"));
    const campus_id =
    /* istanbul ignore next */
    (cov_210suhh07q().s[204]++, ctx.get("campus_id"));
    const user_id =
    /* istanbul ignore next */
    (cov_210suhh07q().s[205]++, ctx.get("user_id"));
    const user_type =
    /* istanbul ignore next */
    (cov_210suhh07q().s[206]++, ctx.get("user_type"));
    const {
      student_ids,
      enrollment_type =
      /* istanbul ignore next */
      (cov_210suhh07q().b[50][0]++, "admin_assigned")
    } =
    /* istanbul ignore next */
    (cov_210suhh07q().s[207]++, await ctx.req.json());
    /* istanbul ignore next */
    cov_210suhh07q().s[208]++;
    if (!["Admin", "Super Admin"].includes(user_type)) {
      /* istanbul ignore next */
      cov_210suhh07q().b[51][0]++;
      cov_210suhh07q().s[209]++;
      return ctx.json({
        success: false,
        error: "Only admins can perform bulk enrollment"
      }, 403);
    } else
    /* istanbul ignore next */
    {
      cov_210suhh07q().b[51][1]++;
    }
    cov_210suhh07q().s[210]++;
    if (
    /* istanbul ignore next */
    (cov_210suhh07q().b[53][0]++, !course_id) ||
    /* istanbul ignore next */
    (cov_210suhh07q().b[53][1]++, !student_ids) ||
    /* istanbul ignore next */
    (cov_210suhh07q().b[53][2]++, !Array.isArray(student_ids))) {
      /* istanbul ignore next */
      cov_210suhh07q().b[52][0]++;
      cov_210suhh07q().s[211]++;
      return ctx.json({
        success: false,
        error: "Course ID and student_ids array are required"
      }, 400);
    } else
    /* istanbul ignore next */
    {
      cov_210suhh07q().b[52][1]++;
    }
    const results =
    /* istanbul ignore next */
    (cov_210suhh07q().s[212]++, []);
    const errors =
    /* istanbul ignore next */
    (cov_210suhh07q().s[213]++, []);
    /* istanbul ignore next */
    cov_210suhh07q().s[214]++;
    for (const student_id of student_ids) {
      /* istanbul ignore next */
      cov_210suhh07q().s[215]++;
      try {
        const result =
        /* istanbul ignore next */
        (cov_210suhh07q().s[216]++, await CourseService.enrollInCourse(course_id, student_id, campus_id, {
          enrollment_type,
          enrollment_source: "admin",
          meta_data: {
            custom_fields: {
              enrolled_by: user_id,
              bulk_enrollment: true
            }
          }
        }));
        /* istanbul ignore next */
        cov_210suhh07q().s[217]++;
        results.push({
          student_id,
          success: true,
          data: result.data
        });
      } catch (error) {
        /* istanbul ignore next */
        cov_210suhh07q().s[218]++;
        errors.push({
          student_id,
          success: false,
          error: error instanceof Error ?
          /* istanbul ignore next */
          (cov_210suhh07q().b[54][0]++, error.message) :
          /* istanbul ignore next */
          (cov_210suhh07q().b[54][1]++, "Unknown error")
        });
      }
    }
    /* istanbul ignore next */
    cov_210suhh07q().s[219]++;
    return ctx.json({
      success: true,
      data: {
        successful_enrollments: results.length,
        failed_enrollments: errors.length,
        results,
        errors
      },
      message: `Bulk enrollment completed: ${results.length} successful, ${errors.length} failed`
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_210suhh07q().s[220]++;
    console.error("Error in bulk enrollment:", error);
    /* istanbul ignore next */
    cov_210suhh07q().s[221]++;
    return ctx.json({
      success: false,
      error: error instanceof Error ?
      /* istanbul ignore next */
      (cov_210suhh07q().b[55][0]++, error.message) :
      /* istanbul ignore next */
      (cov_210suhh07q().b[55][1]++, "Failed to perform bulk enrollment")
    }, 500);
  }
};
