cov_210suhh07q().s[184]++;
CourseController.getCourseAnalytics = async ctx => {
  /* istanbul ignore next */
  cov_210suhh07q().f[14]++;
  cov_210suhh07q().s[185]++;
  try {
    const course_id =
    /* istanbul ignore next */
    (cov_210suhh07q().s[186]++, ctx.req.param("course_id"));
    const campus_id =
    /* istanbul ignore next */
    (cov_210suhh07q().s[187]++, ctx.get("campus_id"));
    const user_id =
    /* istanbul ignore next */
    (cov_210suhh07q().s[188]++, ctx.get("user_id"));
    const user_type =
    /* istanbul ignore next */
    (cov_210suhh07q().s[189]++, ctx.get("user_type"));
    /* istanbul ignore next */
    cov_210suhh07q().s[190]++;
    if (!course_id) {
      /* istanbul ignore next */
      cov_210suhh07q().b[46][0]++;
      cov_210suhh07q().s[191]++;
      return ctx.json({
        success: false,
        error: "Course ID is required"
      }, 400);
    } else
    /* istanbul ignore next */
    {
      cov_210suhh07q().b[46][1]++;
    }
    // Check permissions
    const courseResult =
    /* istanbul ignore next */
    (cov_210suhh07q().s[192]++, await CourseService.getCourseById(course_id, campus_id));
    const course =
    /* istanbul ignore next */
    (cov_210suhh07q().s[193]++, courseResult.data);
    const canViewAnalytics =
    /* istanbul ignore next */
    (cov_210suhh07q().s[194]++,
    /* istanbul ignore next */
    (cov_210suhh07q().b[47][0]++, ["Admin", "Super Admin"].includes(user_type)) ||
    /* istanbul ignore next */
    (cov_210suhh07q().b[47][1]++, course.created_by === user_id) ||
    /* istanbul ignore next */
    (cov_210suhh07q().b[47][2]++, course.instructor_ids.includes(user_id)));
    /* istanbul ignore next */
    cov_210suhh07q().s[195]++;
    if (!canViewAnalytics) {
      /* istanbul ignore next */
      cov_210suhh07q().b[48][0]++;
      cov_210suhh07q().s[196]++;
      return ctx.json({
        success: false,
        error: "Insufficient permissions to view course analytics"
      }, 403);
    } else
    /* istanbul ignore next */
    {
      cov_210suhh07q().b[48][1]++;
    }
    const result =
    /* istanbul ignore next */
    (cov_210suhh07q().s[197]++, await CourseService.getCourseAnalytics(course_id, campus_id));
    /* istanbul ignore next */
    cov_210suhh07q().s[198]++;
    return ctx.json({
      success: true,
      data: result.data,
      message: result.message
