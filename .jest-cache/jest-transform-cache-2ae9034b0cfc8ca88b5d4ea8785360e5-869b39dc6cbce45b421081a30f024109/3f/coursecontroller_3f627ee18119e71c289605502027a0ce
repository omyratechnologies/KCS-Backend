      cov_210suhh07q().b[8][1]++;
    }
    const result =
    /* istanbul ignore next */
    (cov_210suhh07q().s[30]++, await CourseService.getCourseById(course_id, campus_id, user_id));
    // Check if user can access this course
    /* istanbul ignore next */
    cov_210suhh07q().s[31]++;
    if (
    /* istanbul ignore next */
    (cov_210suhh07q().b[10][0]++, result.data.status !== "published") &&
    /* istanbul ignore next */
    (cov_210suhh07q().b[10][1]++, !["Admin", "Super Admin", "Teacher"].includes(user_type)) &&
    /* istanbul ignore next */
    (cov_210suhh07q().b[10][2]++, result.data.created_by !== user_id)) {
      /* istanbul ignore next */
      cov_210suhh07q().b[9][0]++;
      cov_210suhh07q().s[32]++;
      return ctx.json({
        success: false,
        error: "Course not found or access denied"
      }, 404);
    } else
    /* istanbul ignore next */
    {
      cov_210suhh07q().b[9][1]++;
    }
    cov_210suhh07q().s[33]++;
    return ctx.json({
      success: true,
      data: result.data,
      message: result.message
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_210suhh07q().s[34]++;
    console.error("Error getting course:", error);
    /* istanbul ignore next */
    cov_210suhh07q().s[35]++;
    return ctx.json({
      success: false,
      error: error instanceof Error ?
      /* istanbul ignore next */
      (cov_210suhh07q().b[11][0]++, error.message) :
      /* istanbul ignore next */
      (cov_210suhh07q().b[11][1]++, "Failed to get course")
    }, 500);
