    /* istanbul ignore next */
    cov_1u75iudgaa().s[47]++;
    for (const record of examRecords) {
      /* istanbul ignore next */
      cov_1u75iudgaa().s[48]++;
      if (
      /* istanbul ignore next */
      (cov_1u75iudgaa().b[16][0]++, record.record_data) &&
      /* istanbul ignore next */
      (cov_1u75iudgaa().b[16][1]++, Array.isArray(record.record_data))) {
        /* istanbul ignore next */
        cov_1u75iudgaa().b[15][0]++;
        cov_1u75iudgaa().s[49]++;
        record.record_data.forEach(termData => {
          /* istanbul ignore next */
          cov_1u75iudgaa().f[5]++;
          cov_1u75iudgaa().s[50]++;
          if (
          /* istanbul ignore next */
          (cov_1u75iudgaa().b[18][0]++, termData.marks) &&
          /* istanbul ignore next */
          (cov_1u75iudgaa().b[18][1]++, Array.isArray(termData.marks))) {
            /* istanbul ignore next */
            cov_1u75iudgaa().b[17][0]++;
            cov_1u75iudgaa().s[51]++;
            termData.marks.forEach(mark => {
              /* istanbul ignore next */
              cov_1u75iudgaa().f[6]++;
              cov_1u75iudgaa().s[52]++;
              if (
              /* istanbul ignore next */
              (cov_1u75iudgaa().b[20][0]++, mark.subject_id) &&
              /* istanbul ignore next */
              (cov_1u75iudgaa().b[20][1]++, mark.mark_gained) &&
              /* istanbul ignore next */
              (cov_1u75iudgaa().b[20][2]++, mark.total_marks)) {
                /* istanbul ignore next */
                cov_1u75iudgaa().b[19][0]++;
                const subjectId =
                /* istanbul ignore next */
                (cov_1u75iudgaa().s[53]++, mark.subject_id);
                const percentage =
                /* istanbul ignore next */
                (cov_1u75iudgaa().s[54]++, mark.mark_gained / mark.total_marks * 100);
                /* istanbul ignore next */
                cov_1u75iudgaa().s[55]++;
                subjectIds.add(subjectId);
                /* istanbul ignore next */
                cov_1u75iudgaa().s[56]++;
                if (!subjectMap.has(subjectId)) {
                  /* istanbul ignore next */
                  cov_1u75iudgaa().b[21][0]++;
                  cov_1u75iudgaa().s[57]++;
                  subjectMap.set(subjectId, {
                    name: "Subject",
                    totalScore: 0,
                    maxScore: 0,
                    count: 0,
                    scores: []
                  });
                } else
                /* istanbul ignore next */
                {
                  cov_1u75iudgaa().b[21][1]++;
                }
                const subject =
                /* istanbul ignore next */
                (cov_1u75iudgaa().s[58]++, subjectMap.get(subjectId));
                /* istanbul ignore next */
                cov_1u75iudgaa().s[59]++;
                subject.totalScore += percentage;
                /* istanbul ignore next */
                cov_1u75iudgaa().s[60]++;
                subject.maxScore += 100;
                /* istanbul ignore next */
                cov_1u75iudgaa().s[61]++;
                subject.count++;
                /* istanbul ignore next */
                cov_1u75iudgaa().s[62]++;
                subject.scores.push(percentage);
              } else
              /* istanbul ignore next */
              {
                cov_1u75iudgaa().b[19][1]++;
              }
            });
          } else
          /* istanbul ignore next */
          {
            cov_1u75iudgaa().b[17][1]++;
          }
        });
      } else
      /* istanbul ignore next */
      {
        cov_1u75iudgaa().b[15][1]++;
      }
    }
    // Get subject names
    const subjectNames =
    /* istanbul ignore next */
    (cov_1u75iudgaa().s[63]++, {});
    /* istanbul ignore next */
    cov_1u75iudgaa().s[64]++;
    if (subjectIds.size > 0) {
      /* istanbul ignore next */
      cov_1u75iudgaa().b[22][0]++;
      cov_1u75iudgaa().s[65]++;
      try {
        const subjectResult =
        /* istanbul ignore next */
        (cov_1u75iudgaa().s[66]++, await Subject.find({
          campus_id,
          id: {
            $in: [...subjectIds]
          },
          is_active: true,
          is_deleted: false
        }));
        const subjects =
        /* istanbul ignore next */
        (cov_1u75iudgaa().s[67]++,
        /* istanbul ignore next */
        (cov_1u75iudgaa().b[23][0]++, subjectResult.rows) ||
        /* istanbul ignore next */
        (cov_1u75iudgaa().b[23][1]++, []));
        /* istanbul ignore next */
        cov_1u75iudgaa().s[68]++;
        for (const subject of subjects) {
          /* istanbul ignore next */
          cov_1u75iudgaa().s[69]++;
          subjectNames[subject.id] = subject.name;
        }
      } catch (error) {
        /* istanbul ignore next */
        cov_1u75iudgaa().s[70]++;
        console.error("Error fetching subject names:", error);
      }
    } else
    /* istanbul ignore next */
    {
      cov_1u75iudgaa().b[22][1]++;
    }
    // Convert to result array
    cov_1u75iudgaa().s[71]++;
    return [...subjectMap.entries()].map(([subjectId, data]) => {
      /* istanbul ignore next */
      cov_1u75iudgaa().f[7]++;
      cov_1u75iudgaa().s[72]++;
      return {
        subjectId,
        subjectName:
        /* istanbul ignore next */
        (cov_1u75iudgaa().b[24][0]++, subjectNames[subjectId]) ||
        /* istanbul ignore next */
        (cov_1u75iudgaa().b[24][1]++, "Unknown Subject"),
        averageScore: data.count > 0 ?
        /* istanbul ignore next */
        (cov_1u75iudgaa().b[25][0]++, Math.round(data.totalScore / data.count * 100) / 100) :
        /* istanbul ignore next */
        (cov_1u75iudgaa().b[25][1]++, 0),
        totalAssessments: data.count,
        trend: this.calculateSubjectTrend(data.scores)
      };
    });
  }
  /**
   * Calculate monthly performance trend
   */
  static async calculateMonthlyTrend(user_id, campus_id) {
    /* istanbul ignore next */
    cov_1u75iudgaa().f[8]++;
    const months =
    /* istanbul ignore next */
    (cov_1u75iudgaa().s[73]++, []);
    const currentDate =
    /* istanbul ignore next */
    (cov_1u75iudgaa().s[74]++, new Date());
    /* istanbul ignore next */
    cov_1u75iudgaa().s[75]++;
    for (let i =
    /* istanbul ignore next */
    (cov_1u75iudgaa().s[76]++, 5); i >= 0; i--) {
      const monthDate =
      /* istanbul ignore next */
      (cov_1u75iudgaa().s[77]++, new Date(currentDate.getFullYear(), currentDate.getMonth() - i, 1));
      const nextMonth =
      /* istanbul ignore next */
      (cov_1u75iudgaa().s[78]++, new Date(currentDate.getFullYear(), currentDate.getMonth() - i + 1, 1));
      /* istanbul ignore next */
      cov_1u75iudgaa().s[79]++;
      try {
        // Get submissions for this month
        const monthSubmissions =
        /* istanbul ignore next */
        (cov_1u75iudgaa().s[80]++, await AssignmentSubmission.find({
          campus_id,
          user_id,
          created_at: {
            $gte: monthDate,
            $lt: nextMonth
          }
        }));
        const submissions =
        /* istanbul ignore next */
        (cov_1u75iudgaa().s[81]++,
        /* istanbul ignore next */
        (cov_1u75iudgaa().b[26][0]++, monthSubmissions.rows) ||
        /* istanbul ignore next */
        (cov_1u75iudgaa().b[26][1]++, []));
        const avgScore =
        /* istanbul ignore next */
        (cov_1u75iudgaa().s[82]++, submissions.length > 0 ?
        /* istanbul ignore next */
        (cov_1u75iudgaa().b[27][0]++, submissions.reduce((sum, sub) => {
          /* istanbul ignore next */
          cov_1u75iudgaa().f[9]++;
          cov_1u75iudgaa().s[83]++;
          return sum + (
          /* istanbul ignore next */
          (cov_1u75iudgaa().b[28][0]++, sub.grade) ||
          /* istanbul ignore next */
          (cov_1u75iudgaa().b[28][1]++, 0));
        }, 0) / submissions.length) :
        /* istanbul ignore next */
        (cov_1u75iudgaa().b[27][1]++, 0));
        /* istanbul ignore next */
        cov_1u75iudgaa().s[84]++;
        months.push({
          month: monthDate.toLocaleDateString("en-US", {
            month: "short",
            year: "numeric"
          }),
          score: Math.round(avgScore * 100) / 100,
          assessments: submissions.length
        });
      } catch (error) {
        /* istanbul ignore next */
        cov_1u75iudgaa().s[85]++;
        console.error(`Error calculating trend for month ${i}:`, error);
        /* istanbul ignore next */
        cov_1u75iudgaa().s[86]++;
        months.push({
          month: monthDate.toLocaleDateString("en-US", {
            month: "short",
            year: "numeric"
          }),
          score: 0,
          assessments: 0
        });
      }
    }
    /* istanbul ignore next */
    cov_1u75iudgaa().s[87]++;
    return months;
  }
  /**
   * Calculate class rank
   */
  static async calculateClassRank(user_id, campus_id, classIds, userScore) {
    /* istanbul ignore next */
    cov_1u75iudgaa().f[10]++;
    cov_1u75iudgaa().s[88]++;
    try {
      /* istanbul ignore next */
      cov_1u75iudgaa().s[89]++;
      if (classIds.length === 0) {
        /* istanbul ignore next */
        cov_1u75iudgaa().b[29][0]++;
        cov_1u75iudgaa().s[90]++;
        return {
          rank: 0,
          totalStudents: 0
        };
      } else
      /* istanbul ignore next */
      {
        cov_1u75iudgaa().b[29][1]++;
      }
      // Get all students in the same classes
      const classResults =
      /* istanbul ignore next */
      (cov_1u75iudgaa().s[91]++, await Class.find({
        campus_id,
        id: {
          $in: classIds
        },
        is_active: true,
        is_deleted: false
      }));
      const classes =
      /* istanbul ignore next */
      (cov_1u75iudgaa().s[92]++,
      /* istanbul ignore next */
      (cov_1u75iudgaa().b[30][0]++, classResults.rows) ||
      /* istanbul ignore next */
      (cov_1u75iudgaa().b[30][1]++, []));
      const allStudentIds =
      /* istanbul ignore next */
      (cov_1u75iudgaa().s[93]++, new Set());
      /* istanbul ignore next */
      cov_1u75iudgaa().s[94]++;
      for (const cls of classes) {
        /* istanbul ignore next */
        cov_1u75iudgaa().s[95]++;
        if (
        /* istanbul ignore next */
        (cov_1u75iudgaa().b[32][0]++, cls.student_ids) &&
        /* istanbul ignore next */
        (cov_1u75iudgaa().b[32][1]++, Array.isArray(cls.student_ids))) {
          /* istanbul ignore next */
          cov_1u75iudgaa().b[31][0]++;
          cov_1u75iudgaa().s[96]++;
          cls.student_ids.forEach(id => {
            /* istanbul ignore next */
            cov_1u75iudgaa().f[11]++;
            cov_1u75iudgaa().s[97]++;
            return allStudentIds.add(id);
          });
        } else
        /* istanbul ignore next */
        {
          cov_1u75iudgaa().b[31][1]++;
        }
      }
      const totalStudents =
      /* istanbul ignore next */
      (cov_1u75iudgaa().s[98]++, allStudentIds.size);
      // Simplified ranking calculation - in practice, you'd calculate scores for all students
      // For now, assume a normal distribution
      const rank =
      /* istanbul ignore next */
      (cov_1u75iudgaa().s[99]++, Math.max(1, Math.ceil(totalStudents * (100 - userScore) / 100)));
      /* istanbul ignore next */
      cov_1u75iudgaa().s[100]++;
      return {
        rank: Math.min(rank, totalStudents),
        totalStudents
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_1u75iudgaa().s[101]++;
      console.error("Error calculating class rank:", error);
      /* istanbul ignore next */
      cov_1u75iudgaa().s[102]++;
      return {
        rank: 0,
        totalStudents: 0
      };
    }
  }
  /**
   * Calculate trend for a subject based on recent scores
   */
  static calculateSubjectTrend(scores) {
    /* istanbul ignore next */
    cov_1u75iudgaa().f[12]++;
    cov_1u75iudgaa().s[103]++;
    if (scores.length < 3) {
      /* istanbul ignore next */
      cov_1u75iudgaa().b[33][0]++;
      cov_1u75iudgaa().s[104]++;
      return "insufficient_data";
    } else
    /* istanbul ignore next */
    {
      cov_1u75iudgaa().b[33][1]++;
    }
    const recentScores =
    /* istanbul ignore next */
    (cov_1u75iudgaa().s[105]++, scores.slice(-3)); // Last 3 scores
    const earlyScores =
    /* istanbul ignore next */
    (cov_1u75iudgaa().s[106]++, scores.slice(0, 3)); // First 3 scores
    const recentAvg =
    /* istanbul ignore next */
    (cov_1u75iudgaa().s[107]++, recentScores.reduce((sum, score) => {
      /* istanbul ignore next */
      cov_1u75iudgaa().f[13]++;
      cov_1u75iudgaa().s[108]++;
      return sum + score;
    }, 0) / recentScores.length);
    const earlyAvg =
    /* istanbul ignore next */
    (cov_1u75iudgaa().s[109]++, earlyScores.reduce((sum, score) => {
      /* istanbul ignore next */
      cov_1u75iudgaa().f[14]++;
      cov_1u75iudgaa().s[110]++;
      return sum + score;
    }, 0) / earlyScores.length);
    const difference =
    /* istanbul ignore next */
    (cov_1u75iudgaa().s[111]++, recentAvg - earlyAvg);
    /* istanbul ignore next */
    cov_1u75iudgaa().s[112]++;
    if (difference > 5) {
      /* istanbul ignore next */
      cov_1u75iudgaa().b[34][0]++;
      cov_1u75iudgaa().s[113]++;
      return "improving";
    } else
    /* istanbul ignore next */
    {
      cov_1u75iudgaa().b[34][1]++;
    }
    cov_1u75iudgaa().s[114]++;
    if (difference < -5) {
      /* istanbul ignore next */
      cov_1u75iudgaa().b[35][0]++;
      cov_1u75iudgaa().s[115]++;
      return "declining";
    } else
    /* istanbul ignore next */
    {
      cov_1u75iudgaa().b[35][1]++;
    }
    cov_1u75iudgaa().s[116]++;
    return "stable";
  }
  /**
   * Convert score to grade points (4.0 scale)
   */
  static convertGradeToPoints(score) {
    /* istanbul ignore next */
    cov_1u75iudgaa().f[15]++;
    cov_1u75iudgaa().s[117]++;
    if (score >= 90) {
      /* istanbul ignore next */
      cov_1u75iudgaa().b[36][0]++;
      cov_1u75iudgaa().s[118]++;
      return 4;
    } else
    /* istanbul ignore next */
    {
      cov_1u75iudgaa().b[36][1]++;
    }
    cov_1u75iudgaa().s[119]++;
    if (score >= 80) {
      /* istanbul ignore next */
      cov_1u75iudgaa().b[37][0]++;
      cov_1u75iudgaa().s[120]++;
      return 3;
    } else
    /* istanbul ignore next */
    {
      cov_1u75iudgaa().b[37][1]++;
    }
    cov_1u75iudgaa().s[121]++;
    if (score >= 70) {
      /* istanbul ignore next */
      cov_1u75iudgaa().b[38][0]++;
      cov_1u75iudgaa().s[122]++;
      return 2;
    } else
    /* istanbul ignore next */
    {
      cov_1u75iudgaa().b[38][1]++;
    }
    cov_1u75iudgaa().s[123]++;
    if (score >= 60) {
      /* istanbul ignore next */
      cov_1u75iudgaa().b[39][0]++;
      cov_1u75iudgaa().s[124]++;
      return 1;
    } else
    /* istanbul ignore next */
    {
      cov_1u75iudgaa().b[39][1]++;
    }
    cov_1u75iudgaa().s[125]++;
    return 0;
  }
  /**
   * Convert grade points to letter grade
   */
  static convertPointsToGrade(points) {
    /* istanbul ignore next */
    cov_1u75iudgaa().f[16]++;
    cov_1u75iudgaa().s[126]++;
    if (points >= 3.5) {
      /* istanbul ignore next */
      cov_1u75iudgaa().b[40][0]++;
      cov_1u75iudgaa().s[127]++;
      return "A";
    } else
    /* istanbul ignore next */
    {
      cov_1u75iudgaa().b[40][1]++;
    }
    cov_1u75iudgaa().s[128]++;
    if (points >= 2.5) {
      /* istanbul ignore next */
      cov_1u75iudgaa().b[41][0]++;
      cov_1u75iudgaa().s[129]++;
      return "B";
    } else
    /* istanbul ignore next */
    {
      cov_1u75iudgaa().b[41][1]++;
    }
    cov_1u75iudgaa().s[130]++;
    if (points >= 1.5) {
      /* istanbul ignore next */
      cov_1u75iudgaa().b[42][0]++;
      cov_1u75iudgaa().s[131]++;
      return "C";
    } else
    /* istanbul ignore next */
    {
      cov_1u75iudgaa().b[42][1]++;
    }
    cov_1u75iudgaa().s[132]++;
    if (points >= 0.5) {
      /* istanbul ignore next */
      cov_1u75iudgaa().b[43][0]++;
      cov_1u75iudgaa().s[133]++;
      return "D";
    } else
    /* istanbul ignore next */
    {
      cov_1u75iudgaa().b[43][1]++;
    }
    cov_1u75iudgaa().s[134]++;
    return "F";
  }
  /**
   * Get default performance metrics for error cases
   */
  static getDefaultPerformanceMetrics() {
    /* istanbul ignore next */
    cov_1u75iudgaa().f[17]++;
    cov_1u75iudgaa().s[135]++;
    return {
      overallGrade: "N/A",
      gradePoint: 0,
      percentageScore: 0,
      subjectPerformance: [],
      monthlyTrend: [],
      rankInClass: 0,
      totalStudents: 0
    };
  }
}