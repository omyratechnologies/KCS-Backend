      /* istanbul ignore next */
      (cov_d4i2hc1ff().b[28][0]++, findError instanceof Error) &&
      /* istanbul ignore next */
      (cov_d4i2hc1ff().b[28][1]++, findError.message.includes("document not found"))) {
