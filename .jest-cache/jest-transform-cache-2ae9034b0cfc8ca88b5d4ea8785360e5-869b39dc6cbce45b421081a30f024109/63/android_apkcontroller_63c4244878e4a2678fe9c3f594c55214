/**
 * Get latest version of APK by package name
 */
/* istanbul ignore next */
cov_d4i2hc1ff().s[102]++;
AndroidApkController.getLatestApkByPackageName = async ctx => {
  /* istanbul ignore next */
  cov_d4i2hc1ff().f[8]++;
  cov_d4i2hc1ff().s[103]++;
  try {
    const {
      packageName
    } =
    /* istanbul ignore next */
    (cov_d4i2hc1ff().s[104]++, ctx.req.param());
    /* istanbul ignore next */
    cov_d4i2hc1ff().s[105]++;
    if (!packageName) {
      /* istanbul ignore next */
      cov_d4i2hc1ff().b[30][0]++;
      cov_d4i2hc1ff().s[106]++;
      return ctx.json({
        success: false,
        message: "Package name is required"
      }, 400);
    } else
    /* istanbul ignore next */
    {
      cov_d4i2hc1ff().b[30][1]++;
    }
    const result =
    /* istanbul ignore next */
    (cov_d4i2hc1ff().s[107]++, await AndroidApk.find({
      packageName
    }));
    const apks =
    /* istanbul ignore next */
    (cov_d4i2hc1ff().s[108]++, Array.isArray(result) ?
    /* istanbul ignore next */
    (cov_d4i2hc1ff().b[31][0]++, result) :
    /* istanbul ignore next */
    (cov_d4i2hc1ff().b[31][1]++,
    /* istanbul ignore next */
    (cov_d4i2hc1ff().b[32][0]++, result.rows) ||
    /* istanbul ignore next */
    (cov_d4i2hc1ff().b[32][1]++, [])));
    /* istanbul ignore next */
    cov_d4i2hc1ff().s[109]++;
    if (
    /* istanbul ignore next */
    (cov_d4i2hc1ff().b[34][0]++, !apks) ||
    /* istanbul ignore next */
    (cov_d4i2hc1ff().b[34][1]++, apks.length === 0)) {
      /* istanbul ignore next */
      cov_d4i2hc1ff().b[33][0]++;
      cov_d4i2hc1ff().s[110]++;
      return ctx.json({
        success: false,
        message: "No APK found with this package name"
      }, 404);
    } else
    /* istanbul ignore next */
    {
      cov_d4i2hc1ff().b[33][1]++;
    }
    // Sort by upload date to get the latest
    const latestApk =
    /* istanbul ignore next */
    (cov_d4i2hc1ff().s[111]++, apks.sort((a, b) => {
      /* istanbul ignore next */
      cov_d4i2hc1ff().f[9]++;
      cov_d4i2hc1ff().s[112]++;
      return new Date(b.uploadDate).getTime() - new Date(a.uploadDate).getTime();
    })[0]);
    /* istanbul ignore next */
    cov_d4i2hc1ff().s[113]++;
    return ctx.json({
      success: true,
      message: "Latest APK retrieved successfully",
      data: {
        id: latestApk.id,
        packageName: latestApk.packageName,
        version: latestApk.version,
        uploadDate: latestApk.uploadDate,
        fileSize: latestApk.fileSize,
        downloadUrl: latestApk.filePath
      }
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_d4i2hc1ff().s[114]++;
    log(`Error fetching latest APK: ${error}`, LogTypes.ERROR, "APK Controller");
    /* istanbul ignore next */
    cov_d4i2hc1ff().s[115]++;
    return ctx.json({
      success: false,
      message: "Internal server error while fetching latest APK"
    }, 500);
  }
};
