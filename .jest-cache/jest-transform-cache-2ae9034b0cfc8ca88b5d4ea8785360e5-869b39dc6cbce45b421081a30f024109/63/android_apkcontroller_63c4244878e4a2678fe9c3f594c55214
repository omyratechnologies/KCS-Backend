AndroidApkController.deleteApk = async ctx => {
  /* istanbul ignore next */
  cov_d4i2hc1ff().f[10]++;
  cov_d4i2hc1ff().s[117]++;
  try {
    const {
      id
    } =
    /* istanbul ignore next */
    (cov_d4i2hc1ff().s[118]++, ctx.req.param());
    /* istanbul ignore next */
    cov_d4i2hc1ff().s[119]++;
    if (!id) {
      /* istanbul ignore next */
      cov_d4i2hc1ff().b[35][0]++;
      cov_d4i2hc1ff().s[120]++;
      return ctx.json({
        success: false,
        message: "APK ID is required"
      }, 400);
    } else
    /* istanbul ignore next */
    {
      cov_d4i2hc1ff().b[35][1]++;
    }
    const apk =
    /* istanbul ignore next */
    (cov_d4i2hc1ff().s[121]++, await AndroidApk.findById(id));
    /* istanbul ignore next */
    cov_d4i2hc1ff().s[122]++;
    if (!apk) {
      /* istanbul ignore next */
      cov_d4i2hc1ff().b[36][0]++;
      cov_d4i2hc1ff().s[123]++;
      return ctx.json({
        success: false,
        message: "APK not found"
      }, 404);
    } else
    /* istanbul ignore next */
    {
      cov_d4i2hc1ff().b[36][1]++;
    }
    cov_d4i2hc1ff().s[124]++;
    await AndroidApk.removeById(id);
    /* istanbul ignore next */
    cov_d4i2hc1ff().s[125]++;
    log(`APK deleted successfully: ${apk.packageName} v${apk.version}`, LogTypes.LOGS, "APK Controller");
    /* istanbul ignore next */
    cov_d4i2hc1ff().s[126]++;
    return ctx.json({
      success: true,
      message: "APK deleted successfully"
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_d4i2hc1ff().s[127]++;
    log(`Error deleting APK: ${error}`, LogTypes.ERROR, "APK Controller");
    /* istanbul ignore next */
    cov_d4i2hc1ff().s[128]++;
    return ctx.json({
      success: false,
      message: "Internal server error while deleting APK"
    }, 500);
  }
};
/**
 * Download APK file - redirects to the S3 URL
 */
/* istanbul ignore next */
cov_d4i2hc1ff().s[129]++;
AndroidApkController.downloadApk = async ctx => {
  /* istanbul ignore next */
  cov_d4i2hc1ff().f[11]++;
  cov_d4i2hc1ff().s[130]++;
  try {
    const {
      id
    } =
    /* istanbul ignore next */
    (cov_d4i2hc1ff().s[131]++, ctx.req.param());
    /* istanbul ignore next */
    cov_d4i2hc1ff().s[132]++;
    if (!id) {
      /* istanbul ignore next */
      cov_d4i2hc1ff().b[37][0]++;
      cov_d4i2hc1ff().s[133]++;
      return ctx.json({
        success: false,
        message: "APK ID is required"
      }, 400);
    } else
    /* istanbul ignore next */
    {
      cov_d4i2hc1ff().b[37][1]++;
    }
    const apk =
    /* istanbul ignore next */
    (cov_d4i2hc1ff().s[134]++, await AndroidApk.findById(id));
    /* istanbul ignore next */
    cov_d4i2hc1ff().s[135]++;
    if (!apk) {
      /* istanbul ignore next */
      cov_d4i2hc1ff().b[38][0]++;
      cov_d4i2hc1ff().s[136]++;
      return ctx.json({
        success: false,
        message: "APK not found"
      }, 404);
    } else
    /* istanbul ignore next */
    {
      cov_d4i2hc1ff().b[38][1]++;
    }
    // Redirect to the S3 URL for download
    cov_d4i2hc1ff().s[137]++;
    return ctx.redirect(apk.filePath);
  } catch (error) {
    /* istanbul ignore next */
    cov_d4i2hc1ff().s[138]++;
    log(`Error downloading APK: ${error}`, LogTypes.ERROR, "APK Controller");
    /* istanbul ignore next */
    cov_d4i2hc1ff().s[139]++;
    return ctx.json({
      success: false,
      message: "Internal server error while downloading APK"
    }, 500);
  }
};