  cov_d4i2hc1ff().s[71]++;
  try {
    const {
      packageName
    } =
    /* istanbul ignore next */
    (cov_d4i2hc1ff().s[72]++, ctx.req.param());
    /* istanbul ignore next */
    cov_d4i2hc1ff().s[73]++;
    if (!packageName) {
      /* istanbul ignore next */
      cov_d4i2hc1ff().b[18][0]++;
      cov_d4i2hc1ff().s[74]++;
      return ctx.json({
        success: false,
        message: "Package name is required"
      }, 400);
    } else
    /* istanbul ignore next */
    {
      cov_d4i2hc1ff().b[18][1]++;
    }
    let result;
    /* istanbul ignore next */
    cov_d4i2hc1ff().s[75]++;
    try {
      /* istanbul ignore next */
      cov_d4i2hc1ff().s[76]++;
      result = await AndroidApk.find({
        packageName
      });
    } catch (findError) {
      /* istanbul ignore next */
      cov_d4i2hc1ff().s[77]++;
      // If collection doesn't exist yet, return not found
      if (
      /* istanbul ignore next */
      (cov_d4i2hc1ff().b[20][0]++, findError instanceof Error) &&
      /* istanbul ignore next */
      (cov_d4i2hc1ff().b[20][1]++, findError.message.includes("document not found"))) {
        /* istanbul ignore next */
        cov_d4i2hc1ff().b[19][0]++;
        cov_d4i2hc1ff().s[78]++;
        return ctx.json({
          success: false,
          message: "No APK found with this package name"
        }, 404);
      } else
      /* istanbul ignore next */
      {
        cov_d4i2hc1ff().b[19][1]++;
      }
      cov_d4i2hc1ff().s[79]++;
      throw findError;
    }
    const apks =
    /* istanbul ignore next */
    (cov_d4i2hc1ff().s[80]++, Array.isArray(result) ?
    /* istanbul ignore next */
    (cov_d4i2hc1ff().b[21][0]++, result) :
    /* istanbul ignore next */
    (cov_d4i2hc1ff().b[21][1]++,
    /* istanbul ignore next */
    (cov_d4i2hc1ff().b[22][0]++, result.rows) ||
    /* istanbul ignore next */
    (cov_d4i2hc1ff().b[22][1]++, [])));
    /* istanbul ignore next */
    cov_d4i2hc1ff().s[81]++;
    if (
    /* istanbul ignore next */
    (cov_d4i2hc1ff().b[24][0]++, !apks) ||
    /* istanbul ignore next */
    (cov_d4i2hc1ff().b[24][1]++, apks.length === 0)) {
      /* istanbul ignore next */
      cov_d4i2hc1ff().b[23][0]++;
      cov_d4i2hc1ff().s[82]++;
      return ctx.json({
        success: false,
        message: "No APK found with this package name"
      }, 404);
    } else
    /* istanbul ignore next */
    {
      cov_d4i2hc1ff().b[23][1]++;
    }
    cov_d4i2hc1ff().s[83]++;
    return ctx.json({
      success: true,
      message: "APK files retrieved successfully",
      data: apks.map(apk => {
        /* istanbul ignore next */
        cov_d4i2hc1ff().f[6]++;
        cov_d4i2hc1ff().s[84]++;
        return {
          id: apk.id,
          packageName: apk.packageName,
          version: apk.version,
          uploadDate: apk.uploadDate,
          fileSize: apk.fileSize,
          downloadUrl: apk.filePath
        };
      })
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_d4i2hc1ff().s[85]++;
    log(`Error fetching APK by package name: ${error}`, LogTypes.ERROR, "APK Controller");
    /* istanbul ignore next */
    cov_d4i2hc1ff().s[86]++;
    return ctx.json({
      success: false,
      message: "Internal server error while fetching APK"
    }, 500);
  }
};
/**
 * Get specific APK by package name and version
 */
/* istanbul ignore next */
cov_d4i2hc1ff().s[87]++;
AndroidApkController.getApkByPackageAndVersion = async ctx => {
  /* istanbul ignore next */
  cov_d4i2hc1ff().f[7]++;
  cov_d4i2hc1ff().s[88]++;
  try {
    const {
      packageName,
      version
    } =
    /* istanbul ignore next */
    (cov_d4i2hc1ff().s[89]++, ctx.req.param());
    /* istanbul ignore next */
    cov_d4i2hc1ff().s[90]++;
    if (
    /* istanbul ignore next */
    (cov_d4i2hc1ff().b[26][0]++, !packageName) ||
    /* istanbul ignore next */
    (cov_d4i2hc1ff().b[26][1]++, !version)) {
      /* istanbul ignore next */
      cov_d4i2hc1ff().b[25][0]++;
      cov_d4i2hc1ff().s[91]++;
      return ctx.json({
        success: false,
        message: "Package name and version are required"
      }, 400);
    } else
    /* istanbul ignore next */
    {
      cov_d4i2hc1ff().b[25][1]++;
