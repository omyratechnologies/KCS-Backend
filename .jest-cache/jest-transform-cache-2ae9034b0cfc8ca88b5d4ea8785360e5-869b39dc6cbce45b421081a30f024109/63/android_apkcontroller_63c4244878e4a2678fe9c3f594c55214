        cov_d4i2hc1ff().s[95]++;
        return ctx.json({
          success: false,
          message: "APK not found"
        }, 404);
      } else
      /* istanbul ignore next */
      {
        cov_d4i2hc1ff().b[27][1]++;
      }
      cov_d4i2hc1ff().s[96]++;
      throw findError;
    }
    /* istanbul ignore next */
    cov_d4i2hc1ff().s[97]++;
    if (!apk) {
      /* istanbul ignore next */
      cov_d4i2hc1ff().b[29][0]++;
      cov_d4i2hc1ff().s[98]++;
      return ctx.json({
        success: false,
        message: "APK not found"
      }, 404);
