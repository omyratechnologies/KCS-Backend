      /* istanbul ignore next */
      (cov_20mi81ad58().s[20]++, await DiscountRule.findOne({
        id: rule_id,
        campus_id
      }));
      /* istanbul ignore next */
      cov_20mi81ad58().s[21]++;
      if (!rule) {
        /* istanbul ignore next */
        cov_20mi81ad58().b[10][0]++;
        cov_20mi81ad58().s[22]++;
        throw new Error("Discount rule not found");
      } else
      /* istanbul ignore next */
      {
        cov_20mi81ad58().b[10][1]++;
      }
      const updatedRule =
      /* istanbul ignore next */
      (cov_20mi81ad58().s[23]++, Object.assign(Object.assign(Object.assign({}, rule), updates), {
        updated_at: new Date()
      }));
      /* istanbul ignore next */
      cov_20mi81ad58().s[24]++;
      await this.validateDiscountRule(updatedRule);
      /* istanbul ignore next */
      cov_20mi81ad58().s[25]++;
      await DiscountRule.updateById(rule_id, updatedRule);
      /* istanbul ignore next */
      cov_20mi81ad58().s[26]++;
      return updatedRule;
    } catch (error) {
      /* istanbul ignore next */
      cov_20mi81ad58().s[27]++;
      throw new Error(`Failed to update discount rule: ${error}`);
    }
  }
  /**
   * Delete discount rule
   */
  static async deleteDiscountRule(rule_id, campus_id) {
    /* istanbul ignore next */
    cov_20mi81ad58().f[3]++;
    cov_20mi81ad58().s[28]++;
    try {
      const {
        DiscountRule
      } =
      /* istanbul ignore next */
      (cov_20mi81ad58().s[29]++, await import("@/models/discount_rule.model"));
      const rule =
      /* istanbul ignore next */
      (cov_20mi81ad58().s[30]++, await DiscountRule.findOne({
        id: rule_id,
        campus_id
      }));
      /* istanbul ignore next */
      cov_20mi81ad58().s[31]++;
      if (!rule) {
        /* istanbul ignore next */
        cov_20mi81ad58().b[11][0]++;
        cov_20mi81ad58().s[32]++;
        throw new Error("Discount rule not found");
      } else
      /* istanbul ignore next */
      {
        cov_20mi81ad58().b[11][1]++;
      }
      // Check if rule is being used
      const {
        DiscountApplication
      } =
      /* istanbul ignore next */
      (cov_20mi81ad58().s[33]++, await import("@/models/discount_rule.model"));
      const applications =
      /* istanbul ignore next */
      (cov_20mi81ad58().s[34]++, await DiscountApplication.find({
        discount_rule_id: rule_id,
        status: {
          $in: ["approved", "applied"]
        }
      }));
      /* istanbul ignore next */
      cov_20mi81ad58().s[35]++;
      if (
      /* istanbul ignore next */
      (cov_20mi81ad58().b[13][0]++, applications.rows) &&
      /* istanbul ignore next */
      (cov_20mi81ad58().b[13][1]++, applications.rows.length > 0)) {
        /* istanbul ignore next */
        cov_20mi81ad58().b[12][0]++;
        cov_20mi81ad58().s[36]++;
        throw new Error("Cannot delete discount rule that has active applications");
      } else
      /* istanbul ignore next */
      {
        cov_20mi81ad58().b[12][1]++;
      }
      cov_20mi81ad58().s[37]++;
      await DiscountRule.deleteById(rule_id);
    } catch (error) {
      /* istanbul ignore next */
      cov_20mi81ad58().s[38]++;
      throw new Error(`Failed to delete discount rule: ${error}`);
    }
  }
  // ========================= DISCOUNT ELIGIBILITY & APPLICATION =========================
  /**
   * Check discount eligibility for a fee
   */
  static async checkDiscountEligibility(campus_id, fee, student_id) {
    /* istanbul ignore next */
    cov_20mi81ad58().f[4]++;
    cov_20mi81ad58().s[39]++;
    try {
      const {
        UserService
      } =
      /* istanbul ignore next */
      (cov_20mi81ad58().s[40]++, await import("@/services/users.service"));
      const {
        Class
      } =
      /* istanbul ignore next */
      (cov_20mi81ad58().s[41]++, await import("@/models/class.model"));
      const student =
      /* istanbul ignore next */
      (cov_20mi81ad58().s[42]++, await UserService.getUser(student_id));
      const studentClass =
      /* istanbul ignore next */
      (cov_20mi81ad58().s[43]++, await Class.findOne({
        campus_id,
        student_ids: {
          $in: [student_id]
        }
      }));
      const rules =
      /* istanbul ignore next */
      (cov_20mi81ad58().s[44]++, await this.getDiscountRules(campus_id, {
        is_active: true
      }));
      const applicableDiscounts =
      /* istanbul ignore next */
      (cov_20mi81ad58().s[45]++, []);
      let totalDiscount =
      /* istanbul ignore next */
      (cov_20mi81ad58().s[46]++, 0);
      /* istanbul ignore next */
      cov_20mi81ad58().s[47]++;
      for (const rule of rules) {
        const eligibility =
        /* istanbul ignore next */
        (cov_20mi81ad58().s[48]++, await this.evaluateRuleConditions(rule, fee, student, studentClass));
        /* istanbul ignore next */
        cov_20mi81ad58().s[49]++;
        if (eligibility.eligible) {
          /* istanbul ignore next */
          cov_20mi81ad58().b[14][0]++;
          const discountAmount =
          /* istanbul ignore next */
          (cov_20mi81ad58().s[50]++, this.calculateDiscountAmount(rule, fee.total_amount));
          const discountPercentage =
          /* istanbul ignore next */
          (cov_20mi81ad58().s[51]++, discountAmount / fee.total_amount * 100);
          /* istanbul ignore next */
          cov_20mi81ad58().s[52]++;
          applicableDiscounts.push({
            rule,
            discount_amount: discountAmount,
            discount_percentage: discountPercentage,
            reason: eligibility.reason
          });
          /* istanbul ignore next */
          cov_20mi81ad58().s[53]++;
          if (rule.is_stackable) {
            /* istanbul ignore next */
            cov_20mi81ad58().b[15][0]++;
            cov_20mi81ad58().s[54]++;
            totalDiscount += discountAmount;
          } else {
            /* istanbul ignore next */
            cov_20mi81ad58().b[15][1]++;
            cov_20mi81ad58().s[55]++;
            // If not stackable, use the highest discount
            totalDiscount = Math.max(totalDiscount, discountAmount);
          }
        } else
        /* istanbul ignore next */
        {
          cov_20mi81ad58().b[14][1]++;
        }
      }
      // Apply priority and max limits
      const finalDiscount =
      /* istanbul ignore next */
      (cov_20mi81ad58().s[56]++, Math.min(totalDiscount, fee.total_amount));
      const finalAmount =
      /* istanbul ignore next */
      (cov_20mi81ad58().s[57]++, fee.total_amount - finalDiscount);
      /* istanbul ignore next */
      cov_20mi81ad58().s[58]++;
      return {
        eligible: applicableDiscounts.length > 0,
        applicable_discounts: applicableDiscounts,
        total_discount: finalDiscount,
        total_discount_percentage: finalDiscount / fee.total_amount * 100,
        original_amount: fee.total_amount,
        final_amount: finalAmount
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_20mi81ad58().s[59]++;
      throw new Error(`Failed to check discount eligibility: ${error}`);
    }
  }
  /**
   * Apply discount to a fee
   */
  static async applyDiscount(campus_id, fee_id, student_id, discount_rule_id, applied_by, application_reason) {
    /* istanbul ignore next */
    cov_20mi81ad58().f[5]++;
    cov_20mi81ad58().s[60]++;
    try {
      const {
        Fee
      } =
      /* istanbul ignore next */
      (cov_20mi81ad58().s[61]++, await import("@/models/fee.model"));
      const {
        DiscountApplication
      } =
      /* istanbul ignore next */
      (cov_20mi81ad58().s[62]++, await import("@/models/discount_rule.model"));
      const fee =
      /* istanbul ignore next */
      (cov_20mi81ad58().s[63]++, await Fee.findById(fee_id));
      /* istanbul ignore next */
      cov_20mi81ad58().s[64]++;
      if (
      /* istanbul ignore next */
      (cov_20mi81ad58().b[17][0]++, !fee) ||
      /* istanbul ignore next */
      (cov_20mi81ad58().b[17][1]++, fee.campus_id !== campus_id)) {
        /* istanbul ignore next */
        cov_20mi81ad58().b[16][0]++;
        cov_20mi81ad58().s[65]++;
        throw new Error("Fee not found");
      } else
      /* istanbul ignore next */
      {
        cov_20mi81ad58().b[16][1]++;
      }
      const eligibility =
      /* istanbul ignore next */
      (cov_20mi81ad58().s[66]++, await this.checkDiscountEligibility(campus_id, fee, student_id));
      const applicableDiscount =
      /* istanbul ignore next */
      (cov_20mi81ad58().s[67]++, eligibility.applicable_discounts.find(d => {
        /* istanbul ignore next */
        cov_20mi81ad58().f[6]++;
        cov_20mi81ad58().s[68]++;
        return d.rule.id === discount_rule_id;
      }));
      /* istanbul ignore next */
      cov_20mi81ad58().s[69]++;
      if (!applicableDiscount) {
        /* istanbul ignore next */
        cov_20mi81ad58().b[18][0]++;
        cov_20mi81ad58().s[70]++;
        throw new Error("Student is not eligible for this discount");
      } else
      /* istanbul ignore next */
      {
        cov_20mi81ad58().b[18][1]++;
      }
      const rule =
      /* istanbul ignore next */
      (cov_20mi81ad58().s[71]++, applicableDiscount.rule);
      const discountApplication =
      /* istanbul ignore next */
      (cov_20mi81ad58().s[72]++, {
        id: `discount_app_${Date.now()}_${Math.random().toString(36).slice(2, 11)}`,
        campus_id,
        discount_rule_id,
        fee_id,
        student_id,
        parent_id: fee.parent_id,
        discount_amount: applicableDiscount.discount_amount,
        applied_percentage: applicableDiscount.discount_percentage,
        original_amount: fee.total_amount,
        discounted_amount: fee.total_amount - applicableDiscount.discount_amount,
        status: rule.requires_approval ?
        /* istanbul ignore next */
        (cov_20mi81ad58().b[19][0]++, "pending") :
        /* istanbul ignore next */
        (cov_20mi81ad58().b[19][1]++, "approved"),
        application_reason,
        applied_by,
        created_at: new Date(),
        updated_at: new Date()
      });
      /* istanbul ignore next */
      cov_20mi81ad58().s[73]++;
      if (!rule.requires_approval) {
        /* istanbul ignore next */
        cov_20mi81ad58().b[20][0]++;
        cov_20mi81ad58().s[74]++;
        discountApplication.approved_by = applied_by;
        /* istanbul ignore next */
        cov_20mi81ad58().s[75]++;
        discountApplication.approved_at = new Date();
        /* istanbul ignore next */
        cov_20mi81ad58().s[76]++;
        discountApplication.status = "applied";
        // Update fee with discount
        /* istanbul ignore next */
        cov_20mi81ad58().s[77]++;
        await this.applyDiscountToFee(fee, applicableDiscount.discount_amount);
      } else
      /* istanbul ignore next */
      {
        cov_20mi81ad58().b[20][1]++;
      }
      cov_20mi81ad58().s[78]++;
      await DiscountApplication.create(discountApplication);
      // Update rule usage count
      /* istanbul ignore next */
      cov_20mi81ad58().s[79]++;
      await this.updateRuleUsageCount(discount_rule_id);
      /* istanbul ignore next */
      cov_20mi81ad58().s[80]++;
      return discountApplication;
    } catch (error) {
      /* istanbul ignore next */
      cov_20mi81ad58().s[81]++;
      throw new Error(`Failed to apply discount: ${error}`);
    }
  }
  /**
   * Approve discount application
   */
  static async approveDiscountApplication(application_id, campus_id, approved_by) {
    /* istanbul ignore next */
    cov_20mi81ad58().f[7]++;
    cov_20mi81ad58().s[82]++;
    try {
      const {
        DiscountApplication
      } =
      /* istanbul ignore next */
      (cov_20mi81ad58().s[83]++, await import("@/models/discount_rule.model"));
      const {
        Fee
      } =
      /* istanbul ignore next */
      (cov_20mi81ad58().s[84]++, await import("@/models/fee.model"));
      const application =
      /* istanbul ignore next */
      (cov_20mi81ad58().s[85]++, await DiscountApplication.findOne({
        id: application_id,
        campus_id,
        status: "pending"
      }));
      /* istanbul ignore next */
      cov_20mi81ad58().s[86]++;
      if (!application) {
        /* istanbul ignore next */
        cov_20mi81ad58().b[21][0]++;
        cov_20mi81ad58().s[87]++;
        throw new Error("Discount application not found or already processed");
      } else
      /* istanbul ignore next */
      {
        cov_20mi81ad58().b[21][1]++;
      }
      const fee =
      /* istanbul ignore next */
      (cov_20mi81ad58().s[88]++, await Fee.findById(application.fee_id));
      /* istanbul ignore next */
      cov_20mi81ad58().s[89]++;
      if (!fee) {
        /* istanbul ignore next */
        cov_20mi81ad58().b[22][0]++;
        cov_20mi81ad58().s[90]++;
        throw new Error("Associated fee not found");
      } else
      /* istanbul ignore next */
      {
        cov_20mi81ad58().b[22][1]++;
      }
      // Apply discount to fee
      cov_20mi81ad58().s[91]++;
      await this.applyDiscountToFee(fee, application.discount_amount);
      // Update application status
      /* istanbul ignore next */
      cov_20mi81ad58().s[92]++;
      await DiscountApplication.updateById(application_id, {
        status: "applied",
        approved_by,
        approved_at: new Date(),
        updated_at: new Date()
      });
    } catch (error) {
      /* istanbul ignore next */
      cov_20mi81ad58().s[93]++;
      throw new Error(`Failed to approve discount application: ${error}`);
    }
  }
  /**
   * Reject discount application
   */
  static async rejectDiscountApplication(application_id, campus_id, rejected_by, rejection_reason) {
    /* istanbul ignore next */
    cov_20mi81ad58().f[8]++;
    cov_20mi81ad58().s[94]++;
    try {
      const {
        DiscountApplication
      } =
      /* istanbul ignore next */
      (cov_20mi81ad58().s[95]++, await import("@/models/discount_rule.model"));
      /* istanbul ignore next */
      cov_20mi81ad58().s[96]++;
      await DiscountApplication.updateOne({
        id: application_id,
        campus_id,
        status: "pending"
      }, {
        status: "rejected",
        approved_by: rejected_by,
        approved_at: new Date(),
        rejection_reason,
        updated_at: new Date()
      });
    } catch (error) {
      /* istanbul ignore next */
      cov_20mi81ad58().s[97]++;
      throw new Error(`Failed to reject discount application: ${error}`);
    }
  }
  // ========================= BULK OPERATIONS =========================
  /**
   * Apply discount to multiple students
   */
  static async applyBulkDiscount(campus_id, request, applied_by) {
    /* istanbul ignore next */
    cov_20mi81ad58().f[9]++;
    const results =
    /* istanbul ignore next */
    (cov_20mi81ad58().s[98]++, {
      successful: 0,
      failed: 0,
      applications: [],
      errors: []
    });
    /* istanbul ignore next */
    cov_20mi81ad58().s[99]++;
    for (const student_id of request.student_ids) {
      /* istanbul ignore next */
      cov_20mi81ad58().s[100]++;
      try {
        // Get student's fees
        const {
          Fee
        } =
        /* istanbul ignore next */
        (cov_20mi81ad58().s[101]++, await import("@/models/fee.model"));
        const studentFees =
        /* istanbul ignore next */
        (cov_20mi81ad58().s[102]++, await Fee.find({
          campus_id,
          user_id: student_id,
          payment_status: {
            $ne: "paid"
          }
        }));
        /* istanbul ignore next */
        cov_20mi81ad58().s[103]++;
        for (const fee of
        /* istanbul ignore next */
        (cov_20mi81ad58().b[23][0]++, studentFees.rows) ||
        /* istanbul ignore next */
        (cov_20mi81ad58().b[23][1]++, [])) {
          const application =
          /* istanbul ignore next */
          (cov_20mi81ad58().s[104]++, await this.applyDiscount(campus_id, fee.id, student_id, request.discount_rule_id, applied_by, request.reason));
          /* istanbul ignore next */
          cov_20mi81ad58().s[105]++;
          results.applications.push(application);
        }
        /* istanbul ignore next */
        cov_20mi81ad58().s[106]++;
        results.successful++;
      } catch (error) {
        /* istanbul ignore next */
        cov_20mi81ad58().s[107]++;
        results.failed++;
        /* istanbul ignore next */
        cov_20mi81ad58().s[108]++;
        results.errors.push({
          student_id,
          error: error instanceof Error ?
          /* istanbul ignore next */
          (cov_20mi81ad58().b[24][0]++, error.message) :
          /* istanbul ignore next */
          (cov_20mi81ad58().b[24][1]++, "Unknown error")
        });
      }
    }
    /* istanbul ignore next */
    cov_20mi81ad58().s[109]++;
    return results;
  }
  // ========================= ANALYTICS & REPORTING =========================
  /**
   * Get discount summary and analytics
   */
  static async getDiscountSummary(campus_id, date_range) {
    /* istanbul ignore next */
    cov_20mi81ad58().f[10]++;
    cov_20mi81ad58().s[110]++;
    try {
      const {
        DiscountApplication
      } =
      /* istanbul ignore next */
      (cov_20mi81ad58().s[111]++, await import("@/models/discount_rule.model"));
      const {
        Class
      } =
      /* istanbul ignore next */
      (cov_20mi81ad58().s[112]++, await import("@/models/class.model"));
      const query =
      /* istanbul ignore next */
      (cov_20mi81ad58().s[113]++, {
        campus_id,
        status: "applied"
      });
      /* istanbul ignore next */
      cov_20mi81ad58().s[114]++;
      if (date_range) {
        /* istanbul ignore next */
        cov_20mi81ad58().b[25][0]++;
        cov_20mi81ad58().s[115]++;
        query.applied_at = {
          $gte: date_range.start_date,
          $lte: date_range.end_date
        };
      } else
      /* istanbul ignore next */
      {
        cov_20mi81ad58().b[25][1]++;
      }
      const applications =
      /* istanbul ignore next */
      (cov_20mi81ad58().s[116]++, await DiscountApplication.find(query));
      const applicationData =
      /* istanbul ignore next */
      (cov_20mi81ad58().s[117]++,
      /* istanbul ignore next */
      (cov_20mi81ad58().b[26][0]++, applications.rows) ||
      /* istanbul ignore next */
      (cov_20mi81ad58().b[26][1]++, []));
      const totalDiscountsGiven =
      /* istanbul ignore next */
      (cov_20mi81ad58().s[118]++, applicationData.length);
      const totalDiscountAmount =
      /* istanbul ignore next */
      (cov_20mi81ad58().s[119]++, applicationData.reduce((sum, app) => {
        /* istanbul ignore next */
        cov_20mi81ad58().f[11]++;
        cov_20mi81ad58().s[120]++;
        return sum + app.discount_amount;
      }, 0));
      const uniqueStudents =
      /* istanbul ignore next */
      (cov_20mi81ad58().s[121]++, new Set(applicationData.map(app => {
        /* istanbul ignore next */
        cov_20mi81ad58().f[12]++;
        cov_20mi81ad58().s[122]++;
        return app.student_id;
      })));
      const totalStudentsBenefited =
      /* istanbul ignore next */
      (cov_20mi81ad58().s[123]++, uniqueStudents.size);
      // Group by discount type
      const discountByType =
      /* istanbul ignore next */
      (cov_20mi81ad58().s[124]++, new Map());
      /* istanbul ignore next */
      cov_20mi81ad58().s[125]++;
      for (const app of applicationData) {
        const rule =
        /* istanbul ignore next */
        (cov_20mi81ad58().s[126]++, await this.getDiscountRuleById(app.discount_rule_id));
        /* istanbul ignore next */
        cov_20mi81ad58().s[127]++;
        if (rule) {
          /* istanbul ignore next */
          cov_20mi81ad58().b[27][0]++;
          const existing =
          /* istanbul ignore next */
          (cov_20mi81ad58().s[128]++,
          /* istanbul ignore next */
          (cov_20mi81ad58().b[28][0]++, discountByType.get(rule.discount_type)) ||
          /* istanbul ignore next */
          (cov_20mi81ad58().b[28][1]++, {
            count: 0,
            amount: 0
          }));
          /* istanbul ignore next */
          cov_20mi81ad58().s[129]++;
          discountByType.set(rule.discount_type, {
            count: existing.count + 1,
            amount: existing.amount + app.discount_amount
          });
        } else
        /* istanbul ignore next */
        {
          cov_20mi81ad58().b[27][1]++;
        }
      }
      // Group by class
      const discountByClass =
      /* istanbul ignore next */
      (cov_20mi81ad58().s[130]++, new Map());
      const classes =
      /* istanbul ignore next */
      (cov_20mi81ad58().s[131]++, await Class.find({
        campus_id
      }));
      /* istanbul ignore next */
      cov_20mi81ad58().s[132]++;
      for (const app of applicationData) {
        const studentClass =
        /* istanbul ignore next */
        (cov_20mi81ad58().s[133]++, (
        /* istanbul ignore next */
        (cov_20mi81ad58().b[29][0]++, classes.rows) ||
        /* istanbul ignore next */
        (cov_20mi81ad58().b[29][1]++, [])).find(cls => {
          /* istanbul ignore next */
          cov_20mi81ad58().f[13]++;
          cov_20mi81ad58().s[134]++;
          return /* istanbul ignore next */(cov_20mi81ad58().b[30][0]++, cls.student_ids) &&
          /* istanbul ignore next */
          (cov_20mi81ad58().b[30][1]++, cls.student_ids.includes(app.student_id));
        }));
        const className =
        /* istanbul ignore next */
        (cov_20mi81ad58().s[135]++,
        /* istanbul ignore next */
        (cov_20mi81ad58().b[31][0]++,
        /* istanbul ignore next */
        (cov_20mi81ad58().b[33][0]++, studentClass === null) ||
        /* istanbul ignore next */
        (cov_20mi81ad58().b[33][1]++, studentClass === void 0) ?
        /* istanbul ignore next */
        (cov_20mi81ad58().b[32][0]++, void 0) :
        /* istanbul ignore next */
        (cov_20mi81ad58().b[32][1]++, studentClass.name)) ||
        /* istanbul ignore next */
        (cov_20mi81ad58().b[31][1]++, "Unknown"));
        const existing =
        /* istanbul ignore next */
        (cov_20mi81ad58().s[136]++,
        /* istanbul ignore next */
        (cov_20mi81ad58().b[34][0]++, discountByClass.get(className)) ||
        /* istanbul ignore next */
        (cov_20mi81ad58().b[34][1]++, {
          count: 0,
          amount: 0
        }));
        /* istanbul ignore next */
        cov_20mi81ad58().s[137]++;
        discountByClass.set(className, {
          count: existing.count + 1,
          amount: existing.amount + app.discount_amount
        });
      }
      // Top discount rules
      const ruleUsage =
      /* istanbul ignore next */
      (cov_20mi81ad58().s[138]++, new Map());
      /* istanbul ignore next */
      cov_20mi81ad58().s[139]++;
      for (const app of applicationData) {
        const rule =
        /* istanbul ignore next */
        (cov_20mi81ad58().s[140]++, await this.getDiscountRuleById(app.discount_rule_id));
        /* istanbul ignore next */
        cov_20mi81ad58().s[141]++;
        if (rule) {
          /* istanbul ignore next */
          cov_20mi81ad58().b[35][0]++;
          const existing =
          /* istanbul ignore next */
          (cov_20mi81ad58().s[142]++,
          /* istanbul ignore next */
          (cov_20mi81ad58().b[36][0]++, ruleUsage.get(rule.id)) ||
          /* istanbul ignore next */
          (cov_20mi81ad58().b[36][1]++, {
            count: 0,
            amount: 0,
            name: rule.name
          }));
          /* istanbul ignore next */
          cov_20mi81ad58().s[143]++;
          ruleUsage.set(rule.id, {
            count: existing.count + 1,
            amount: existing.amount + app.discount_amount,
            name: rule.name
          });
        } else
        /* istanbul ignore next */
        {
          cov_20mi81ad58().b[35][1]++;
        }
      }
      /* istanbul ignore next */
      cov_20mi81ad58().s[144]++;
      return {
        total_discounts_given: totalDiscountsGiven,
        total_discount_amount: totalDiscountAmount,
        total_students_benefited: totalStudentsBenefited,
        discount_by_type: [...discountByType.entries()].map(([type, data]) => {
          /* istanbul ignore next */
          cov_20mi81ad58().f[14]++;
          cov_20mi81ad58().s[145]++;
          return {
            type,
            count: data.count,
            amount: data.amount
          };
        }),
        discount_by_class: [...discountByClass.entries()].map(([className, data]) => {
          /* istanbul ignore next */
          cov_20mi81ad58().f[15]++;
          cov_20mi81ad58().s[146]++;
          return {
            class_name: className,
            count: data.count,
            amount: data.amount
          };
        }),
        top_discount_rules: [...ruleUsage.values()].sort((a, b) => {
          /* istanbul ignore next */
          cov_20mi81ad58().f[16]++;
          cov_20mi81ad58().s[147]++;
          return b.amount - a.amount;
        }).slice(0, 10).map(rule => {
          /* istanbul ignore next */
          cov_20mi81ad58().f[17]++;
          cov_20mi81ad58().s[148]++;
          return {
            rule_name: rule.name,
            usage_count: rule.count,
            total_amount: rule.amount
          };
        })
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_20mi81ad58().s[149]++;
      throw new Error(`Failed to get discount summary: ${error}`);
    }
  }
  // ========================= HELPER METHODS =========================
  static async validateDiscountRule(rule) {
    /* istanbul ignore next */
    cov_20mi81ad58().f[18]++;
    cov_20mi81ad58().s[150]++;
    // Validate discount value
    if (
    /* istanbul ignore next */
    (cov_20mi81ad58().b[38][0]++, rule.discount_type === "percentage") && (
    /* istanbul ignore next */
    (cov_20mi81ad58().b[38][1]++, rule.discount_value < 0) ||
    /* istanbul ignore next */
    (cov_20mi81ad58().b[38][2]++, rule.discount_value > 100))) {
      /* istanbul ignore next */
      cov_20mi81ad58().b[37][0]++;
      cov_20mi81ad58().s[151]++;
      throw new Error("Percentage discount must be between 0 and 100");
    } else
    /* istanbul ignore next */
    {
      cov_20mi81ad58().b[37][1]++;
    }
    cov_20mi81ad58().s[152]++;
    if (
    /* istanbul ignore next */
    (cov_20mi81ad58().b[40][0]++, rule.discount_type === "fixed_amount") &&
    /* istanbul ignore next */
    (cov_20mi81ad58().b[40][1]++, rule.discount_value < 0)) {
      /* istanbul ignore next */
      cov_20mi81ad58().b[39][0]++;
      cov_20mi81ad58().s[153]++;
      throw new Error("Fixed amount discount must be positive");
    } else
    /* istanbul ignore next */
    {
      cov_20mi81ad58().b[39][1]++;
    }
    // Validate conditions
    cov_20mi81ad58().s[154]++;
    if (
    /* istanbul ignore next */
    (cov_20mi81ad58().b[42][0]++, rule.conditions.valid_from) &&
    /* istanbul ignore next */
    (cov_20mi81ad58().b[42][1]++, rule.conditions.valid_until) &&
    /* istanbul ignore next */
    (cov_20mi81ad58().b[42][2]++, rule.conditions.valid_from >= rule.conditions.valid_until)) {
      /* istanbul ignore next */
      cov_20mi81ad58().b[41][0]++;
      cov_20mi81ad58().s[155]++;
      throw new Error("Valid from date must be before valid until date");
    } else
    /* istanbul ignore next */
    {
      cov_20mi81ad58().b[41][1]++;
    }
    // Additional validations...
  }
  static async evaluateRuleConditions(rule, fee, student, studentClass) {
    /* istanbul ignore next */
    cov_20mi81ad58().f[19]++;
    const conditions =
    /* istanbul ignore next */
    (cov_20mi81ad58().s[156]++, rule.conditions);
    // Check academic year
    /* istanbul ignore next */
    cov_20mi81ad58().s[157]++;
    if (
    /* istanbul ignore next */
    (cov_20mi81ad58().b[44][0]++, conditions.academic_year) &&
    /* istanbul ignore next */
    (cov_20mi81ad58().b[44][1]++, fee.academic_year !== conditions.academic_year)) {
      /* istanbul ignore next */
      cov_20mi81ad58().b[43][0]++;
      cov_20mi81ad58().s[158]++;
      return {
        eligible: false,
        reason: "Academic year mismatch"
      };
    } else
    /* istanbul ignore next */
    {
      cov_20mi81ad58().b[43][1]++;
    }
    // Check fee categories
    cov_20mi81ad58().s[159]++;
    if (
    /* istanbul ignore next */
    (cov_20mi81ad58().b[46][0]++, conditions.applicable_fee_categories) &&
    /* istanbul ignore next */
    (cov_20mi81ad58().b[46][1]++, conditions.applicable_fee_categories.length > 0)) {
      /* istanbul ignore next */
      cov_20mi81ad58().b[45][0]++;
      const feeCategories =
      /* istanbul ignore next */
      (cov_20mi81ad58().s[160]++, new Set(fee.items.map(item => {
        /* istanbul ignore next */
        cov_20mi81ad58().f[20]++;
        cov_20mi81ad58().s[161]++;
        return item.category_id;
      })));
      const hasApplicableCategory =
      /* istanbul ignore next */
      (cov_20mi81ad58().s[162]++, conditions.applicable_fee_categories.some(cat => {
        /* istanbul ignore next */
        cov_20mi81ad58().f[21]++;
        cov_20mi81ad58().s[163]++;
        return feeCategories.has(cat);
      }));
      /* istanbul ignore next */
      cov_20mi81ad58().s[164]++;
      if (!hasApplicableCategory) {
        /* istanbul ignore next */
        cov_20mi81ad58().b[47][0]++;
        cov_20mi81ad58().s[165]++;
        return {
          eligible: false,
          reason: "Fee category not applicable"
        };
      } else
      /* istanbul ignore next */
      {
        cov_20mi81ad58().b[47][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_20mi81ad58().b[45][1]++;
    }
    // Check classes
    cov_20mi81ad58().s[166]++;
    if (
    /* istanbul ignore next */
    (cov_20mi81ad58().b[49][0]++, conditions.applicable_classes) &&
    /* istanbul ignore next */
    (cov_20mi81ad58().b[49][1]++, conditions.applicable_classes.length > 0) && (
    /* istanbul ignore next */
    (cov_20mi81ad58().b[49][2]++, !studentClass) ||
    /* istanbul ignore next */
    (cov_20mi81ad58().b[49][3]++, !conditions.applicable_classes.includes(studentClass.id)))) {
      /* istanbul ignore next */
      cov_20mi81ad58().b[48][0]++;
      cov_20mi81ad58().s[167]++;
      return {
        eligible: false,
        reason: "Class not applicable"
      };
    } else
    /* istanbul ignore next */
    {
      cov_20mi81ad58().b[48][1]++;
    }
    // Check amount limits
    cov_20mi81ad58().s[168]++;
    if (
    /* istanbul ignore next */
    (cov_20mi81ad58().b[51][0]++, conditions.min_amount) &&
    /* istanbul ignore next */
    (cov_20mi81ad58().b[51][1]++, fee.total_amount < conditions.min_amount)) {
      /* istanbul ignore next */
      cov_20mi81ad58().b[50][0]++;
      cov_20mi81ad58().s[169]++;
      return {
        eligible: false,
        reason: "Amount below minimum"
      };
    } else
    /* istanbul ignore next */
    {
      cov_20mi81ad58().b[50][1]++;
    }
    cov_20mi81ad58().s[170]++;
    if (
    /* istanbul ignore next */
    (cov_20mi81ad58().b[53][0]++, conditions.max_amount) &&
    /* istanbul ignore next */
    (cov_20mi81ad58().b[53][1]++, fee.total_amount > conditions.max_amount)) {
      /* istanbul ignore next */
      cov_20mi81ad58().b[52][0]++;
      cov_20mi81ad58().s[171]++;
      return {
        eligible: false,
        reason: "Amount above maximum"
      };
    } else
    /* istanbul ignore next */
    {
      cov_20mi81ad58().b[52][1]++;
    }
    // Check date validity
    const now =
    /* istanbul ignore next */
    (cov_20mi81ad58().s[172]++, new Date());
    /* istanbul ignore next */
    cov_20mi81ad58().s[173]++;
    if (
    /* istanbul ignore next */
    (cov_20mi81ad58().b[55][0]++, conditions.valid_from) &&
    /* istanbul ignore next */
    (cov_20mi81ad58().b[55][1]++, now < conditions.valid_from)) {
      /* istanbul ignore next */
      cov_20mi81ad58().b[54][0]++;
      cov_20mi81ad58().s[174]++;
      return {
        eligible: false,
        reason: "Discount not yet valid"
      };
    } else
    /* istanbul ignore next */
    {
      cov_20mi81ad58().b[54][1]++;
    }
    cov_20mi81ad58().s[175]++;
    if (
    /* istanbul ignore next */
    (cov_20mi81ad58().b[57][0]++, conditions.valid_until) &&
    /* istanbul ignore next */
    (cov_20mi81ad58().b[57][1]++, now > conditions.valid_until)) {
      /* istanbul ignore next */
      cov_20mi81ad58().b[56][0]++;
      cov_20mi81ad58().s[176]++;
      return {
        eligible: false,
        reason: "Discount expired"
      };
    } else
    /* istanbul ignore next */
    {
      cov_20mi81ad58().b[56][1]++;
    }
    // Check early payment (simplified)
    cov_20mi81ad58().s[177]++;
    if (
    /* istanbul ignore next */
    (cov_20mi81ad58().b[59][0]++, conditions.early_payment_days) &&
    /* istanbul ignore next */
    (cov_20mi81ad58().b[59][1]++, fee.items.length > 0)) {
      /* istanbul ignore next */
      cov_20mi81ad58().b[58][0]++;
      const dueDate =
      /* istanbul ignore next */
      (cov_20mi81ad58().s[178]++, new Date(fee.items[0].due_date));
      const earlyPaymentDeadline =
      /* istanbul ignore next */
      (cov_20mi81ad58().s[179]++, new Date(dueDate.getTime() - conditions.early_payment_days * 24 * 60 * 60 * 1000));
      /* istanbul ignore next */
      cov_20mi81ad58().s[180]++;
      if (now > earlyPaymentDeadline) {
        /* istanbul ignore next */
        cov_20mi81ad58().b[60][0]++;
        cov_20mi81ad58().s[181]++;
        return {
          eligible: false,
          reason: "Early payment deadline passed"
        };
      } else
      /* istanbul ignore next */
      {
        cov_20mi81ad58().b[60][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_20mi81ad58().b[58][1]++;
    }
    // Additional condition checks can be added here...
    cov_20mi81ad58().s[182]++;
    return {
      eligible: true,
      reason: "All conditions met"
    };
  }
  static calculateDiscountAmount(rule, originalAmount) {
    /* istanbul ignore next */
    cov_20mi81ad58().f[22]++;
    let discountAmount =
    /* istanbul ignore next */
    (cov_20mi81ad58().s[183]++, 0);
    /* istanbul ignore next */
    cov_20mi81ad58().s[184]++;
    switch (rule.discount_type) {
      case "percentage":
        /* istanbul ignore next */
        cov_20mi81ad58().b[61][0]++;
        {
          /* istanbul ignore next */
          cov_20mi81ad58().s[185]++;
          discountAmount = originalAmount * rule.discount_value / 100;
          /* istanbul ignore next */
          cov_20mi81ad58().s[186]++;
          break;
        }
      case "fixed_amount":
        /* istanbul ignore next */
        cov_20mi81ad58().b[61][1]++;
        {
          /* istanbul ignore next */
          cov_20mi81ad58().s[187]++;
          discountAmount = rule.discount_value;
          /* istanbul ignore next */
          cov_20mi81ad58().s[188]++;
          break;
        }
      default:
        /* istanbul ignore next */
        cov_20mi81ad58().b[61][2]++;
        {
          /* istanbul ignore next */
          cov_20mi81ad58().s[189]++;
          discountAmount = 0;
        }
    }
    // Apply maximum discount limit
    /* istanbul ignore next */
    cov_20mi81ad58().s[190]++;
    if (rule.max_discount_amount) {
      /* istanbul ignore next */
      cov_20mi81ad58().b[62][0]++;
      cov_20mi81ad58().s[191]++;
      discountAmount = Math.min(discountAmount, rule.max_discount_amount);
    } else
    /* istanbul ignore next */
    {
      cov_20mi81ad58().b[62][1]++;
    }
    cov_20mi81ad58().s[192]++;
    return Math.min(discountAmount, originalAmount);
  }
  static async applyDiscountToFee(fee, discountAmount) {
    /* istanbul ignore next */
    cov_20mi81ad58().f[23]++;
    const {
      Fee
    } =
    /* istanbul ignore next */
    (cov_20mi81ad58().s[193]++, await import("@/models/fee.model"));
    const updatedFee =
    /* istanbul ignore next */
    (cov_20mi81ad58().s[194]++, Object.assign(Object.assign({}, fee), {
      discount_amount: (
      /* istanbul ignore next */
      (cov_20mi81ad58().b[63][0]++, fee.discount_amount) ||
      /* istanbul ignore next */
      (cov_20mi81ad58().b[63][1]++, 0)) + discountAmount,
      due_amount: fee.due_amount - discountAmount,
      updated_at: new Date()
    }));
    /* istanbul ignore next */
    cov_20mi81ad58().s[195]++;
    await Fee.updateById(fee.id, updatedFee);
  }
  static async updateRuleUsageCount(rule_id) {
    /* istanbul ignore next */
    cov_20mi81ad58().f[24]++;
    const {
      DiscountRule
    } =
    /* istanbul ignore next */
    (cov_20mi81ad58().s[196]++, await import("@/models/discount_rule.model"));
    const rule =
    /* istanbul ignore next */
    (cov_20mi81ad58().s[197]++, await DiscountRule.findById(rule_id));
    /* istanbul ignore next */
    cov_20mi81ad58().s[198]++;
    if (rule) {
      /* istanbul ignore next */
      cov_20mi81ad58().b[64][0]++;
      cov_20mi81ad58().s[199]++;
      await DiscountRule.updateById(rule_id, {
        used_count: rule.used_count + 1,
        updated_at: new Date()
      });
    } else
    /* istanbul ignore next */
    {
      cov_20mi81ad58().b[64][1]++;
    }
  }
  static async getDiscountRuleById(rule_id) {
    /* istanbul ignore next */
    cov_20mi81ad58().f[25]++;
    cov_20mi81ad58().s[200]++;
    try {
      const {
        DiscountRule
      } =
      /* istanbul ignore next */
      (cov_20mi81ad58().s[201]++, await import("@/models/discount_rule.model"));
      /* istanbul ignore next */
      cov_20mi81ad58().s[202]++;
      return await DiscountRule.findById(rule_id);
    } catch (_a) {
      /* istanbul ignore next */
      cov_20mi81ad58().s[203]++;
      return null;
    }
  }
}