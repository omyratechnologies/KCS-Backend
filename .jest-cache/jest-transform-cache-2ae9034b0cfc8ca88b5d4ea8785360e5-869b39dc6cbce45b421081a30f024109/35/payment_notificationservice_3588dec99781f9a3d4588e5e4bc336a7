  /**
   * Truncate content for SMS (160 characters)
   */
  static truncateForSMS(content) {
    /* istanbul ignore next */
    cov_1q0cl1ya8n().f[7]++;
    // Remove extra whitespace and line breaks
    const cleaned =
    /* istanbul ignore next */
    (cov_1q0cl1ya8n().s[65]++, content.replaceAll(/\s+/g, " ").trim());
    /* istanbul ignore next */
    cov_1q0cl1ya8n().s[66]++;
    if (cleaned.length <= 160) {
      /* istanbul ignore next */
      cov_1q0cl1ya8n().b[32][0]++;
      cov_1q0cl1ya8n().s[67]++;
      return cleaned;
    } else
    /* istanbul ignore next */
    {
      cov_1q0cl1ya8n().b[32][1]++;
    }
    // Truncate and add ellipsis
    cov_1q0cl1ya8n().s[68]++;
    return cleaned.slice(0, 157) + "...";
  }
