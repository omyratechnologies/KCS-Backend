    /* istanbul ignore next */
    cov_1q0cl1ya8n().s[58]++;
    content = content.replaceAll("{{fee_amount}}", data.fee_amount.toString());
    /* istanbul ignore next */
    cov_1q0cl1ya8n().s[59]++;
    content = content.replaceAll("{{due_date}}", data.due_date);
    /* istanbul ignore next */
    cov_1q0cl1ya8n().s[60]++;
    content = content.replaceAll("{{payment_url}}",
    /* istanbul ignore next */
    (cov_1q0cl1ya8n().b[28][0]++, data.payment_url) ||
    /* istanbul ignore next */
    (cov_1q0cl1ya8n().b[28][1]++, ""));
    /* istanbul ignore next */
