    content = content.replaceAll("{{invoice_url}}",
    /* istanbul ignore next */
    (cov_1q0cl1ya8n().b[29][0]++, data.invoice_url) ||
    /* istanbul ignore next */
    (cov_1q0cl1ya8n().b[29][1]++, ""));
