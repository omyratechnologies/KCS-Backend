  /**
   * Schedule automatic payment reminders
   */
  static async schedulePaymentReminders(campus_id) {
    /* istanbul ignore next */
    cov_1q0cl1ya8n().f[12]++;
    cov_1q0cl1ya8n().s[95]++;
    try {
      const {
        Fee
      } =
      /* istanbul ignore next */
      (cov_1q0cl1ya8n().s[96]++, await import("@/models/fee.model"));
      const {
        UserService
      } =
      /* istanbul ignore next */
      (cov_1q0cl1ya8n().s[97]++, await import("@/services/users.service"));
      const today =
      /* istanbul ignore next */
      (cov_1q0cl1ya8n().s[98]++, new Date());
      const threeDaysFromNow =
      /* istanbul ignore next */
      (cov_1q0cl1ya8n().s[99]++, new Date(today.getTime() + 3 * 24 * 60 * 60 * 1000));
      // Get fees due in next 3 days
      const feesDueSoon =
      /* istanbul ignore next */
      (cov_1q0cl1ya8n().s[100]++, await Fee.find({
        campus_id,
        payment_status: "unpaid",
        is_paid: false
      }));
      const schoolData =
      /* istanbul ignore next */
      (cov_1q0cl1ya8n().s[101]++, await this.getSchoolDetails(campus_id));
      /* istanbul ignore next */
      cov_1q0cl1ya8n().s[102]++;
      for (const fee of
      /* istanbul ignore next */
      (cov_1q0cl1ya8n().b[41][0]++, feesDueSoon.rows) ||
      /* istanbul ignore next */
      (cov_1q0cl1ya8n().b[41][1]++, [])) {
        // Check if any item is due in next 3 days
        const hasDueSoon =
        /* istanbul ignore next */
        (cov_1q0cl1ya8n().s[103]++, fee.items.some(item => {
          /* istanbul ignore next */
          cov_1q0cl1ya8n().f[13]++;
          const dueDate =
          /* istanbul ignore next */
          (cov_1q0cl1ya8n().s[104]++, new Date(item.due_date));
          /* istanbul ignore next */
          cov_1q0cl1ya8n().s[105]++;
          return /* istanbul ignore next */(cov_1q0cl1ya8n().b[42][0]++, dueDate >= today) &&
          /* istanbul ignore next */
          (cov_1q0cl1ya8n().b[42][1]++, dueDate <= threeDaysFromNow);
        }));
        /* istanbul ignore next */
        cov_1q0cl1ya8n().s[106]++;
        if (hasDueSoon) {
          /* istanbul ignore next */
          cov_1q0cl1ya8n().b[43][0]++;
          const studentData =
          /* istanbul ignore next */
          (cov_1q0cl1ya8n().s[107]++, await UserService.getUser(fee.user_id));
          /* istanbul ignore next */
          cov_1q0cl1ya8n().s[108]++;
          await this.sendPaymentDueReminder(campus_id, fee, studentData, schoolData);
        } else
        /* istanbul ignore next */
        {
          cov_1q0cl1ya8n().b[43][1]++;
        }
      }
    } catch (error) {
      /* istanbul ignore next */
      cov_1q0cl1ya8n().s[109]++;
      console.error("Failed to schedule payment reminders:", error);
    }
  }
