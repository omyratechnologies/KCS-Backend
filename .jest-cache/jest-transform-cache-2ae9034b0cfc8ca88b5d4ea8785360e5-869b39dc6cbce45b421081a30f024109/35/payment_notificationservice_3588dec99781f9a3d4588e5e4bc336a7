  /**
   * Update reminder count in fee record
   */
  static async updateReminderCount(fee_id, type) {
    /* istanbul ignore next */
    cov_1q0cl1ya8n().f[10]++;
    cov_1q0cl1ya8n().s[79]++;
    try {
      const {
        Fee
      } =
      /* istanbul ignore next */
      (cov_1q0cl1ya8n().s[80]++, await import("@/models/fee.model"));
      const fee =
      /* istanbul ignore next */
      (cov_1q0cl1ya8n().s[81]++, await Fee.findById(fee_id));
      /* istanbul ignore next */
      cov_1q0cl1ya8n().s[82]++;
      if (fee) {
        /* istanbul ignore next */
        cov_1q0cl1ya8n().b[33][0]++;
        const reminderSent =
        /* istanbul ignore next */
        (cov_1q0cl1ya8n().s[83]++,
        /* istanbul ignore next */
        (cov_1q0cl1ya8n().b[34][0]++, fee.reminder_sent) ||
        /* istanbul ignore next */
        (cov_1q0cl1ya8n().b[34][1]++, {
          email_count: 0,
          sms_count: 0
        }));
        /* istanbul ignore next */
        cov_1q0cl1ya8n().s[84]++;
        if (
        /* istanbul ignore next */
        (cov_1q0cl1ya8n().b[36][0]++, type === "email") ||
        /* istanbul ignore next */
        (cov_1q0cl1ya8n().b[36][1]++, type === "both")) {
          /* istanbul ignore next */
          cov_1q0cl1ya8n().b[35][0]++;
          cov_1q0cl1ya8n().s[85]++;
          reminderSent.email_count = (
          /* istanbul ignore next */
          (cov_1q0cl1ya8n().b[37][0]++, reminderSent.email_count) ||
          /* istanbul ignore next */
          (cov_1q0cl1ya8n().b[37][1]++, 0)) + 1;
        } else
        /* istanbul ignore next */
        {
          cov_1q0cl1ya8n().b[35][1]++;
        }
        cov_1q0cl1ya8n().s[86]++;
        if (
        /* istanbul ignore next */
        (cov_1q0cl1ya8n().b[39][0]++, type === "sms") ||
        /* istanbul ignore next */
        (cov_1q0cl1ya8n().b[39][1]++, type === "both")) {
          /* istanbul ignore next */
          cov_1q0cl1ya8n().b[38][0]++;
          cov_1q0cl1ya8n().s[87]++;
          reminderSent.sms_count = (
          /* istanbul ignore next */
          (cov_1q0cl1ya8n().b[40][0]++, reminderSent.sms_count) ||
          /* istanbul ignore next */
          (cov_1q0cl1ya8n().b[40][1]++, 0)) + 1;
